{
  double sqr=0;
  int pts=point.length;
  for (int column=0; column < ncats; column++) {
    double d=point[column];
    if (Double.isNaN(d))     pts--;
 else     if (d != center[column])     sqr+=1.0;
  }
  for (int column=ncats; column < center.length; column++) {
    double d=point[column];
    if (Double.isNaN(d))     pts--;
 else {
      if (mults != null) {
        d-=means[column];
        d*=mults[column];
      }
      double delta=d - center[column];
      sqr+=delta * delta;
    }
  }
  if (0 < pts && pts < point.length)   sqr*=point.length / pts;
  return sqr;
}
