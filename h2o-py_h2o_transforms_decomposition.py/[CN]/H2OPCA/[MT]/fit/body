def fit(self, X, y=None, **params):
    '\n    Fit the PCA.\n\n    :param X: An H2OFrame; may contain NAs and/or categoricals.\n    :param y: (Ignored)\n    :param params: (Ignored)\n    :return: This instance of H2OPCA.\n    '
    self.pca_model = prcomp(x=X, k=self.parms['n_components'], max_iterations=self.parms['max_iterations'], seed=self.parms['seed'], use_all_factor_levels=self.parms['use_all_factor_levels'], pca_method=self.parms['pca_method'])
    return self
