def cluster_is_up(self):
    u'\n        Determine if an H\u2082O cluster is running or not.\n\n        :return: True if the cluster is up; False otherwise\n        '
    try:
        if (self._local_server and (not self._local_server.is_running())):
            return False
        self.request(u'GET /')
        return True
    except (H2OConnectionError, H2OServerError):
        return False
