@staticmethod
def _pwhich(e):
    '\n        POSIX style which\n        '
    ok = os.X_OK
    if e:
        if os.access(e, ok):
            return e
        for path in os.getenv('PATH').split(os.pathsep):
            full_path = os.path.join(path, e)
            if os.access(full_path, ok):
                return full_path
    return None
