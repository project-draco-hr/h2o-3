def __init__(self):
    u'[Internal] Please use H2OConnection.connect() or H2OConnection.start() to create H2OConnection objects.'
    super(H2OConnection, self).__init__()
    globals()[u'__H2OCONN__'] = self
    self._stage = 0
    self._session_id = None
    self._ip = None
    self._port = None
    self._https = None
    self._scheme = None
    self._base_url = None
    self._verify_ssl_cert = None
    self._auth = None
    self._proxies = None
    self._cluster_name = None
    self._cluster_info = None
    self._verbose = None
    self._child = None
    self._requests_counter = 0
    self._timeout = None
    self._is_logging = False
    self._logging_dest = None
