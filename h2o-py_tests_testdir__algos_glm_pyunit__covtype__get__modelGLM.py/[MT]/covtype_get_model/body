def covtype_get_model(ip, port):
    h2o.init(ip, port)
    covtype = h2o.import_frame(path=h2o.locate('smalldata/covtype/covtype.20k.data'))
    Y = 54
    X = [x for x in range(0, 54) if (x not in [20, 28])]
    res_class = random.randint(1, 4)
    covtype[54] = (covtype[54] == res_class)
    covtype_mod1 = h2o.glm(y=covtype[Y], x=covtype[X], family='binomial', alpha=[0], Lambda=[0])
    covtype_mod1.show()
    covtype_mod1 = h2o.get_model(covtype_mod1._key)
    covtype_mod1.show()
    covtype_mod2 = h2o.glm(y=covtype[Y], x=covtype[X], family='binomial', alpha=[0.5], Lambda=[0.0001])
    covtype_mod2.show()
    covtype_mod2 = h2o.get_model(covtype_mod2._key)
    covtype_mod2.show()
    covtype_mod3 = h2o.glm(y=covtype[Y], x=covtype[X], family='binomial', alpha=[1], Lambda=[0.0001])
    covtype_mod3.show()
    covtype_mod3 = h2o.get_model(covtype_mod3._key)
    covtype_mod3.show()
