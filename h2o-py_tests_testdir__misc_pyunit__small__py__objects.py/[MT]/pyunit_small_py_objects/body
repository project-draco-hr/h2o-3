def pyunit_small_py_objects():
    d = h2o.H2OFrame([3.2])
    d.show()
    assert (d.nrow == d.ncol == 1)
    d = h2o.H2OFrame([[3.2], [3.2]])
    assert (d.nrow == 1)
    assert (d.ncol == 2)
    d.show()
    d = h2o.H2OFrame([3.2, 3.2])
    d.show()
    assert (d.nrow == 2)
    assert (d.ncol == 1)
