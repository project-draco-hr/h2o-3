{
  _mb=new MetricBuilderBinomial(domain);
  Chunk actuals=chks[1];
  double[] ds=new double[3];
  for (int i=0; i < chks[0]._len; ++i) {
    ds[2]=chks[0].atd(i);
    ds[1]=1 - ds[2];
    ds[0]=GenModel.getPrediction(ds,null,ds,threshold);
    _mb.perRow(ds,new float[]{actuals.at8(i)},null);
  }
}
