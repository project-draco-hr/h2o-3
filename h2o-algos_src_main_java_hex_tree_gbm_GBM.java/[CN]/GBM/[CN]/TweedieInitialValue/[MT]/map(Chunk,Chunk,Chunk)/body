{
  for (int i=0; i < response._len; ++i) {
    if (response.isNA(i))     continue;
    double w=weight.atd(i);
    if (w == 0)     continue;
    double o=offset.atd(i);
    double y=response.atd(i);
    _num+=w * y * Distributions.exp(o * (1 - _power));
    _denom+=w * Distributions.exp(o * (2 - _power));
  }
}
