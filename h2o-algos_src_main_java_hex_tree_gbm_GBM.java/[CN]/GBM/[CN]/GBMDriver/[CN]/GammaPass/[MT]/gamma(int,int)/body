{
  if (_denom[tree][nid] == 0)   return 0;
  double g=_num[tree][nid] / _denom[tree][nid];
  assert(!Double.isInfinite(g) && !Double.isNaN(g));
  if (_dist.distribution == Distribution.Family.poisson || _dist.distribution == Distribution.Family.gamma || _dist.distribution == Distribution.Family.tweedie) {
    return _dist.link(g);
  }
 else {
    return g;
  }
}
