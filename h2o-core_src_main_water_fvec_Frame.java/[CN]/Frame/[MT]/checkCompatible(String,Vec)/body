{
  if (ArrayUtils.find(_names,name) != -1)   throw new IllegalArgumentException("Duplicate name '" + name + "' in Frame");
  if (vec instanceof AppendableVec)   return;
  Vec v0=anyVec();
  if (v0 == null)   return;
  if (!v0.checkCompatible(vec))   throw new IllegalArgumentException("Vector groups differs - adding vec '" + name + "' into the frame "+ Arrays.toString(_names));
  if (v0.length() != vec.length())   throw new IllegalArgumentException("Vector lengths differ - adding vec '" + name + "' into the frame "+ Arrays.toString(_names));
}
