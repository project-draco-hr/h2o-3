{
  File h2oResultsDir=new File(find_test_file_static("h2o-test-accuracy").getCanonicalFile().toString() + "/results");
  if (!h2oResultsDir.exists()) {
    h2oResultsDir.mkdir();
  }
  File suiteSummary=new File(find_test_file_static("h2o-test-accuracy/results").getCanonicalFile().toString() + "/summary.txt");
  suiteSummary.createNewFile();
  summaryPrintStream=new PrintStream(new FileOutputStream(suiteSummary,false));
  System.out.println("Commenced logging to h2o-test-accuracy/results directory.");
}
