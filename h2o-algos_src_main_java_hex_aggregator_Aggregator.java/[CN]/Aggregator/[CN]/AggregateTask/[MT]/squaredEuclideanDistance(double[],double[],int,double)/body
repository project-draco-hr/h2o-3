{
  double sum=0;
  int n=0;
  boolean missing=false;
  for (int j=0; j < nCols; j++) {
    final double d1=e1[j];
    final double d2=e2[j];
    if (!isMissing(d1) && !isMissing(d2)) {
      final double dist=(d1 - d2);
      sum+=dist * dist;
      n++;
    }
 else {
      missing=true;
    }
    if (!missing && sum > thresh)     break;
  }
  sum*=(double)nCols / n;
  return sum;
}
