def as_date(self, format):
    '\n    Inplace update the column to millis since the epoch.\n    :param format: The date time format string\n    :return: None\n    '
    if (not isinstance(format, str)):
        raise ValueError('format must be a string')
    if (self.key() == ''):
        self._expr.eager()
    expr = ((("(as.Date '" + self.key()) + '\' "{}"'.format(format)) + ')')
    res = h2o.rapids(expr)
    return H2OVec(self._name, Expr(op=res['vec_ids'][0]['name'], length=res['num_rows']))
