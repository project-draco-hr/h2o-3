def _get_vec_from_rapids_string(self, expr, tmp_key):
    h2o.rapids(expr)
    j = h2o.frame(tmp_key)
    fr = j['frames'][0]
    rows = fr['rows']
    veckeys = fr['vec_ids']
    cols = fr['columns']
    colnames = [col['label'] for col in cols]
    vec = H2OVec.new_vecs(zip(colnames, veckeys), rows)[0]
    vec.setName(self._name)
    h2o.removeFrameShallow(tmp_key)
    return vec
