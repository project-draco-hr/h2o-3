def __div__(self, i):
    '\n    :param i: A Vec or a float\n    :return: A new H2OVec.\n    '
    if isinstance(i, H2OVec):
        self._len_check(i)
        return H2OVec(self._name, Expr('/', self, i))
    if isinstance(i, (int, float)):
        return H2OVec(self._name, Expr('/', self, Expr(i)))
    raise NotImplementedError
