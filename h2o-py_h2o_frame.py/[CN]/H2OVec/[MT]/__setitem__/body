def __setitem__(self, b, c):
    '\n        Update-in-place of a Vec.\n        This interface currently only supports whole vector replacement.\n\n        If `c` has length 1, then it\'s assumed that `c` represents a constant vector\n        of its current value.\n\n        :param b: An H2OVec for selecting rows to update in-place.\n        :param c: The "new" values that will write over the values stipulated by `b`.\n        :return: void\n        '
    if (c and (len(c) != 1) and (len(c) != len(self))):
        raise ValueError(((('len(self)=' + len(self)) + ' cannot be broadcast across len(c)=') + len(c)))
    if isinstance(b, H2OVec):
        if (len(b) != len(self)):
            raise ValueError(((('len(self)=' + len(self)) + ' cannot be broadcast across len(b)=') + len(b)))
        self._expr = Expr('=', Expr('[', self._expr, b), c)
    else:
        raise NotImplementedError('Only vector replacement is currently supported.')
