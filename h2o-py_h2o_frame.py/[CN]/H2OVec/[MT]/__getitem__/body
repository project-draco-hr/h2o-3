def __getitem__(self, i):
    '\n    Basic index/sliced lookup\n\n    :param i: An Expr or an H2OVec\n    :return: A new Expr object corresponding to the input query\n    '
    if isinstance(i, H2OVec):
        return self.row_select(i)
    if isinstance(i, int):
        return Expr('[', self, Expr(i), length=1).eager()
    if isinstance(i, slice):
        e = Expr(i)
        return H2OVec(self._name, Expr('[', self, e, length=len(e)))
    raise ValueError('Row selection from a Vec is limited to 1 row, or a boolean Vec')
