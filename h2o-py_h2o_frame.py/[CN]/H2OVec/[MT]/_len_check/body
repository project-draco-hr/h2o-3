def _len_check(self, x):
    if (not x):
        return self
    if isinstance(x, H2OFrame):
        x = x._vecs[0]
    if isinstance(x, Expr):
        raise ValueError('Mixing Vec and Expr')
    if (not isinstance(x, H2OVec)):
        return self
    if (len(self) != len(x)):
        raise ValueError(((('H2OVec length mismatch: ' + str(len(self))) + ' vs ') + str(len(x))))
    return self
