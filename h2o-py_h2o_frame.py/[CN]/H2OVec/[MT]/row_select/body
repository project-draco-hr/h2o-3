def row_select(self, vec):
    '\n    Boolean column select lookup\n\n    :param vec: An H2OVec.\n    :return: A new H2OVec.\n    '
    e = Expr('[', self, vec)
    r = e.eager()
    if isinstance(r, (float, int)):
        e.set_len(1)
    else:
        j = h2o.frame(r)
        e.set_len(j['frames'][0]['rows'])
    return H2OVec(self._name, e)
