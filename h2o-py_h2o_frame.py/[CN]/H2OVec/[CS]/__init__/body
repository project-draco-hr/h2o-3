def __init__(self, name, expr):
    '\n        Create a new instance of an H2OVec object\n        :param name: The name of the column corresponding to this H2OVec.\n        :param expr: The lazy expression representing this H2OVec\n        :return: A new H2OVec\n        '
    assert isinstance(name, str)
    assert isinstance(expr, Expr)
    self._name = name
    self._expr = expr
    expr._name = name
