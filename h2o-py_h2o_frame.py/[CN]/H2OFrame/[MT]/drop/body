def drop(self, i):
    '\n    Column selection via integer, string(name) returns a Vec\n    Column selection via slice returns a subset Frame\n\n    :param i: Column to select\n    :return: Returns an H2OVec or H2OFrame.\n    '
    if ((self._vecs is None) or (self._vecs == [])):
        raise ValueError('Frame Removed')
    if isinstance(i, str):
        for v in self._vecs:
            if (i == v._name):
                return H2OFrame(vecs=[v for v in self._vecs if (i != v._name)])
        raise ValueError((('Name ' + i) + ' not in Frame'))
    elif isinstance(i, int):
        if ((i < 0) or (i >= self.__len__())):
            raise ValueError(((('Index out of range: 0 <= ' + str(i)) + ' < ') + str(self.__len__())))
        return H2OFrame(vecs=[v for v in self._vecs if (v._name != self._vecs[i]._name)])
    raise NotImplementedError
