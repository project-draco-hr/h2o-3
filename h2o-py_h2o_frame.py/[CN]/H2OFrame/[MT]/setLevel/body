def setLevel(self, level):
    '\n    A method to set all column values to one of the levels.\n    :param level: The level at which the column will be set (a string)\n    :return: An H2OFrame with all entries set to the desired level\n    '
    if ((self._vecs is None) or (self._vecs == [])):
        raise ValueError('Frame Removed')
    if (len(self) != 1):
        raise (ValueError, '`setLevel` can only be called on a single H2OVec or an H2OFrame with one column')
    return H2OFrame(vecs=[self[0].setLevel(level=level)])
