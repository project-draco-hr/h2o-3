def prod(self, na_rm=False):
    '\n    Parameters\n    ----------\n      na_rm : bool, default=False\n        True or False to remove NAs from computation.\n\n    Returns\n    -------\n      The product of the column.\n    '
    return H2OFrame._expr(expr=ExprNode(('prod.na' if na_rm else 'prod'), self))._scalar()
