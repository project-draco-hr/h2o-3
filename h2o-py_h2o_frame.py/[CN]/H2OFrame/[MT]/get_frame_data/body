def get_frame_data(self):
    u"Get frame data as str in csv format\n\n        Returns\n        -------\n          A local python string, each line is a row and each element separated by commas,\n          containing this H2OFrame instance's data.\n        "
    url = (h2o.conn().make_url(u'DownloadDataset', 3) + u'?frame_id={}&hex_string=false'.format(self.frame_id))
    return requests.get(url, headers={u'User-Agent': (u'H2O Python client/' + sys.version.replace(u'\n', u'')), }, auth=h2o.conn()._auth, verify=h2o.conn()._verify_ssl_cert, stream=True).text
