def set_name(self, col=None, name=None):
    u'Set the name of the column at the specified index.\n\n        Parameters\n        ----------\n          col : int, str\n            Index of the column whose name is to be set; may be skipped for 1-column frames\n\n          name : str\n            The new name of the column to set\n\n        Returns\n        -------\n          Returns self.\n        '
    if is_str(col):
        col = self.names.index(col)
    if ((not is_int(col)) and (self.ncol > 1)):
        raise ValueError((u'`col` must be an index. Got: ' + str(col)))
    if (self.ncol == 1):
        col = 0
    old_cache = self._ex._cache
    self._ex = ExprNode(u'colnames=', self, col, name)
    self._ex._cache.fill_from(old_cache)
    if (self.names is None):
        self._frame()._ex._cache.fill()
    else:
        self._ex._cache.names = ((self.names[:col] + [name]) + self.names[(col + 1):])
