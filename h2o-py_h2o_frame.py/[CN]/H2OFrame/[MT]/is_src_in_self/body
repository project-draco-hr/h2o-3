def is_src_in_self(self, src):
    if isinstance(src, H2OFrame):
        if (self._ex is src._ex):
            return True
        elif (src._ex._children is not None):
            for ch in src._ex._children:
                if self.is_src_in_self(ch):
                    return True
    elif isinstance(src, ExprNode):
        if (self._ex is src):
            return True
        elif (src._children is not None):
            for ch in src._children:
                if self.is_src_in_self(ch):
                    return True
    return False
