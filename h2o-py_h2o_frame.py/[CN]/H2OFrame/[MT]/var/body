def var(self, y=None, use='everything'):
    '\n    :param use: One of "everything", "complete.obs", or "all.obs".\n    :return: The covariance matrix of the columns in this H2OFrame.\n    '
    fr = H2OFrame._expr(expr=ExprNode('var', self, (self if (y is None) else y), use))
    if (self.ncol == 1):
        return fr._scalar()
    return fr._frame()
