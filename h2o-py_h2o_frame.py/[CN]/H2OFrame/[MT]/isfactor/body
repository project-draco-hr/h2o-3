def isfactor(self):
    u'Test if the selection is a factor column.\n\n    Returns\n    -------\n      True if the column is categorical; otherwise False. For String columns, the result\n      is False.\n    '
    if self._ex._cache.types_valid():
        return [(str(list(viewvalues(self._ex._cache.types))[0]) == u'enum')]
    return [bool(o) for o in ExprNode(u'is.factor', self)._eager_scalar()]
