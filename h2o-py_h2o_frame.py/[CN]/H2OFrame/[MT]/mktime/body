@staticmethod
def mktime(year=1970, month=0, day=0, hour=0, minute=0, second=0, msec=0):
    u'All units are zero-based (including months and days).\n    Missing year is 1970.\n\n    Parameters\n    ----------\n      year : int, H2OFrame\n        the year\n\n      month: int, H2OFrame\n        the month\n\n      day : int, H2OFrame\n        the day\n\n      hour : int, H2OFrame\n        the hour\n\n      minute : int, H2OFrame\n        the minute\n\n      second : int, H2OFrame\n        the second\n\n      msec : int, H2OFrame\n        the milisecond\n\n    Returns\n    -------\n      H2OFrame of one column containing the date in millis since the epoch.\n    '
    return ExprNode(u'mktime', year, month, day, hour, minute, second, msec)._eager_frame()
