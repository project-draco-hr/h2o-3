def describe(self):
    'Generate an in-depth description of this H2OFrame.\n    The description is a tabular print of the type, min, max, sigma, number of zeros,\n    and number of missing elements for each H2OVec in this H2OFrame.\n    '
    self._eager()
    thousands_sep = h2o.H2ODisplay.THOUSANDS
    print 'Rows:', thousands_sep.format(self.nrow), 'Cols:', thousands_sep.format(self.ncol)
    chunk_dist_sum = h2o.frame(self._id)['frames'][0]
    dist_summary = chunk_dist_sum['distribution_summary']
    chunk_summary = chunk_dist_sum['chunk_summary']
    chunk_summary.show()
    dist_summary.show()
    self.summary()
