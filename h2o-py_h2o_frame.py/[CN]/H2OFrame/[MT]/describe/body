def describe(self):
    u'Generate an in-depth description of this H2OFrame. Everything in summary(), plus\n    the data layout.\n    '
    res = h2o.connection().get_json((((u'Frames/' + self.frame_id) + u'?row_count=') + str(10)))[u'frames'][0]
    self._ex._cache._fill_data(res)
    print(u'Rows:{:,}'.format(self.nrow), u'Cols:{:,}'.format(self.ncol))
    res[u'chunk_summary'].show()
    res[u'distribution_summary'].show()
    print(u'\n')
    self.summary()
