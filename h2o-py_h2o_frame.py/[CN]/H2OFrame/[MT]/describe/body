def describe(self):
    u'\n        Generate an in-depth description of this H2OFrame. Everything in summary(), plus the data layout.\n        '
    res = h2o.api((u'GET /3/Frames/%s' % self.frame_id), data={u'row_count': 10, })[u'frames'][0]
    self._ex._cache._fill_data(res)
    print(u'Rows:{:,}'.format(self.nrow), u'Cols:{:,}'.format(self.ncol))
    res[u'chunk_summary'].show()
    res[u'distribution_summary'].show()
    print(u'\n')
    self.summary()
