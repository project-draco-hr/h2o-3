def rbind(self, data):
    '\n    Combine H2O Datasets by Rows.\n    Takes a sequence of H2O data sets and combines them by rows.\n    :param data: an self._newExpr\n    :return: self, with data appended (row-wise)\n    '
    if (not isinstance(data, H2OFrame)):
        raise ValueError('`data` must be an H2OFrame, but got {0}'.format(type(data)))
    return self._newExpr('rbind', self, data)
