def round(self, digits=0):
    '\n    :return: The rounded values in the H2OFrame to the specified number of decimal digits.\n    '
    if ((self._vecs is None) or (self._vecs == [])):
        raise ValueError('Frame Removed')
    key = self.send_frame()
    tmp_key = H2OFrame.py_tmp_key()
    expr = '(= !{} (round %{} #{}))'.format(tmp_key, key, digits)
    h2o.rapids(expr)
    h2o.removeFrameShallow(key)
    j = h2o.frame(tmp_key)
    fr = j['frames'][0]
    rows = fr['rows']
    veckeys = fr['vec_ids']
    cols = fr['columns']
    colnames = [col['label'] for col in cols]
    vecs = H2OVec.new_vecs(zip(colnames, veckeys), rows)
    h2o.removeFrameShallow(tmp_key)
    return H2OFrame(vecs=vecs)
