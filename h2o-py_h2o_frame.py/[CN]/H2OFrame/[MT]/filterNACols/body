def filterNACols(self, frac=0.2):
    '\n    Filter columns with prportion of NAs >= frac.\n    :param frac: Fraction of NAs in the column.\n    :return: A  list of column indices.\n    '
    fr = self.send_frame()
    res = h2o.rapids('(filterNACols %{} #{})'.format(fr, str(frac)))
    l = res['head'][0]
    h2o.removeFrameShallow(fr)
    return [int(float(i)) for i in l]
