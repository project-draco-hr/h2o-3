def concat(self, frames):
    "Append multiple data to this H2OFrame column-wise\n\n    Parameters\n    ----------\n      data : List of H2OFrame's\n        H2OFrame's to be column bound to the right of this H2OFrame.\n\n    Returns\n    -------\n      H2OFrame of the combined datasets.\n    "
    if (not frames):
        assert 'Input list of frames is empty! Nothing to concat.'
    elif (len(frames) == 1):
        assert 'Input list of frames is of length one. Better to use cbind for one frame.'
    df = None
    for i in frames:
        if (df is None):
            df = self
        else:
            df = df.cbind(i)
    return df
