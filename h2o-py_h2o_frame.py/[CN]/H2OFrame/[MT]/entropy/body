def entropy(self):
    'For each string, return the Shannon entropy. If the string is empty, the entropy is 0.\n\n    Returns\n    -------\n      An H2OFrame of Shannon entropies. \n    '
    fr = H2OFrame._expr(expr=ExprNode('entropy', self))
    fr._ex._cache.nrows = self.nrow
    fr._ex._cache.ncol = self.ncol
    return fr
