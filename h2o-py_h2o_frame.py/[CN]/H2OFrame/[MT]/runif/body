def runif(self, seed=None):
    '\n    :param seed: A random seed. If None, then one will be generated.\n    :return: A new H2OVec filled with doubles sampled uniformly from [0,1).\n    '
    import random
    seed = (random.getrandbits(32) if (seed is None) else seed)
    return H2OFrame._expr(expr=ExprNode('h2o.runif', self, seed))
