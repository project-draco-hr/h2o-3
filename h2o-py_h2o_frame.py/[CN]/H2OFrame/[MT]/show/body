def show(self):
    "\n    Used by the H2OFrame.__repr__ method to print or display a snippet of the data frame.\n    If called from IPython, displays an html'ized result\n    Else prints a tabulate'd result\n    :return: None\n    "
    if h2o.H2ODisplay._in_ipy():
        import IPython.display
        IPython.display.display_html(self._tabulate('html', False), raw=True)
    else:
        print self
