def pop(self, i):
    '\n    :param i: The index or name of the column to pop.\n    :return: The column dropped from the frame; the frame is side-effected to lose the column\n    '
    if isinstance(i, basestring):
        i = self.names.index(i)
    col = self._newExpr('cols', self, i)
    self._ex = expr.ExprNode('cols', self, (- (i + 1)))
    return col
