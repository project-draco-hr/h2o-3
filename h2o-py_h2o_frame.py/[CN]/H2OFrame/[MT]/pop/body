def pop(self, i):
    '\n    Pop a colunn out of an self._newExpr.\n\n    :param i: The index or name of the column to pop.\n    :return: The column dropped from the frame.\n    '
    if isinstance(i, basestring):
        i = self._find_idx(i)
    col = self._newExpr('pop', self, i)._frame()
    if self._keep:
        col.keep()
    self._update()
    return col
