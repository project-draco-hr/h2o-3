def pop(self, i):
    '\n    Pop a colunn out of an H2OFrame.\n\n    :param i: The index or name of the column to pop.\n    :return: The column dropped from the frame.\n    '
    if isinstance(i, basestring):
        i = self._find_idx(i)
    col = H2OFrame(expr=ExprNode('pop', self, i))._frame()
    self._update()
    return col
