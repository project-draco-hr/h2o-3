def trim(self):
    'Trim white space on the left and right of strings in a single-column H2OFrame.\n\n    Returns\n    -------\n      H2OFrame with trimmed strings.\n    '
    fr = H2OFrame._expr(expr=ExprNode('trim', self))
    fr._ex._cache.nrows = self.nrow
    fr._ex._cache.ncol = self.ncol
    return fr
