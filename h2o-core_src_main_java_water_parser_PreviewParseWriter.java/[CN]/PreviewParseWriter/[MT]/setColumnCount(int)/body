{
  if (_ncols == 0 && n > 0) {
    _ncols=n;
    _nzeros=new int[n];
    _nstrings=new int[n];
    _nUUID=new int[n];
    _ndates=new int[n];
    _nnums=new int[n];
    _nempty=new int[n];
    _domains=new HashSet[n];
    for (int i=0; i < n; ++i)     _domains[i]=new HashSet<String>();
    for (int i=0; i < MAX_PREVIEW_LINES; i++)     _data[i]=new String[n];
  }
}
