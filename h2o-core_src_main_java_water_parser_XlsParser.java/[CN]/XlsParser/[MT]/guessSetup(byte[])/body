{
  XlsParser p=new XlsParser(new ParseSetup(true,0,null,ParserType.XLS,ParseSetup.AUTO_SEP,-1,false,null,null,0));
  p._buf=bytes;
  p._lim=bytes.length;
  InspectDataOut dout=new InspectDataOut();
  try {
    p.streamParse(new ByteArrayInputStream(bytes),dout);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  return new ParseSetup(dout._ncols > 0 && dout._nlines > 0 && dout._nlines > dout._invalidLines,dout._invalidLines,dout.errors(),ParserType.XLS,ParseSetup.AUTO_SEP,dout._ncols,false,dout.colNames(),dout._data,dout.colNames() == null ? -1 : 1);
}
