{
switch (distribution) {
case AUTO:
case gaussian:
case bernoulli:
case poisson:
    return y - linkInv(f);
case gamma:
  return y * exp(-f) - 1;
case tweedie:
assert(tweediePower > 1 && tweediePower < 2);
return y * exp(f * (1 - tweediePower)) - exp(f * (2 - tweediePower));
default :
throw H2O.unimpl();
}
}
