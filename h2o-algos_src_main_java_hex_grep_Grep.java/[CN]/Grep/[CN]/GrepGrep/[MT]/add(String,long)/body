{
  if (_cnt == 0) {
    _matches=new String[2];
    _linenos=new long[2];
    _offsets=new long[2];
  }
 else   if (_cnt == _matches.length) {
    _matches=Arrays.copyOf(_matches,_cnt << 1);
    _linenos=Arrays.copyOf(_linenos,_cnt << 1);
    _offsets=Arrays.copyOf(_offsets,_cnt << 1);
  }
  _matches[_cnt]=s;
  _offsets[_cnt]=off;
  _cnt++;
}
