{
  doCleanup();
  for (  DataInfo dinfo : _foldInfos)   DKV.remove(dinfo._key);
  if (!_gotException.getAndSet(true)) {
    if (ex instanceof TooManyPredictorsException) {
      _maxLambda=_lambdaId;
      this.tryComplete();
      return false;
    }
    new RemoveCall(null,_dest).invokeTask();
    return true;
  }
  return false;
}
