{
  if (!_gotException.getAndSet(true)) {
    if (ex instanceof JobCancelledException) {
      GLM.this.cancel();
      tryComplete();
      return false;
    }
    if (ex instanceof TooManyPredictorsException) {
      tryComplete();
      return false;
    }
    doCleanup();
    new RemoveCall(null,_dest).invokeTask();
    failed(ex);
    return true;
  }
  return false;
}
