{
  GLMGradientTask gt=_glmp._family == Family.binomial ? new LBFGS_LogisticGradientTask(_dinfo,_glmp,_lambda,beta,1.0 / _nobs).doAll(_dinfo._adaptedFrame) : new GLMGradientTask(_dinfo,_glmp,_lambda,beta,1.0 / _nobs).doAll(_dinfo._adaptedFrame);
  return new GradientInfo(gt._objVal,gt._gradient);
}
