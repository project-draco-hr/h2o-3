{
  BetaConstraint res=new BetaConstraint();
  if (_betaLB != null)   res._betaLB=ArrayUtils.select(_betaLB,activeCols);
  if (_betaUB != null)   res._betaUB=ArrayUtils.select(_betaUB,activeCols);
  if (_betaGiven != null)   res._betaGiven=ArrayUtils.select(_betaGiven,activeCols);
  if (_rho != null)   res._rho=ArrayUtils.select(_rho,activeCols);
  if (_betaStart != null)   res._betaStart=ArrayUtils.select(_betaStart,activeCols);
  return res;
}
