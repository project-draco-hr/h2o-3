{
  int len=cs[0].len();
  Chunk x=cs[0];
  double x_bar=x.vec().isEnum() || x.vec().isUUID() ? Double.NaN : x.vec().mean();
  Chunk y=cs[1];
  double y_bar=y.vec().isEnum() || y.vec().isUUID() ? Double.NaN : y.vec().mean();
  for (int r=0; r < len; ++r) {
    _ss+=(x.at0(r) - x_bar) * (y.at0(r) - y_bar);
  }
}
