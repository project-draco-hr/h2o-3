{
  int i=0;
  int decimalCnt=0;
  if (_len == 0)   return NA;
  if (_buf[_off] == '+' || _buf[_off] == '-')   i++;
  while (i < _len) {
    if (_buf[_off + i] == '.')     decimalCnt++;
    if (_buf[_off + i] < '0' || _buf[_off + i] > '9')     return NA;
    i++;
  }
  if (decimalCnt > 0)   if (decimalCnt == 1)   return REAL;
 else   return NA;
 else   return INT;
}
