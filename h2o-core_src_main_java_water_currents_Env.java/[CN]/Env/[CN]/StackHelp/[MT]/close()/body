{
  Futures fs=null;
  int i, sp=sp();
  while (sp > _sp) {
    Vec vec=_refcnt.remove(--sp);
    if (_globals.contains(vec))     continue;
    for (i=0; i < sp; i++)     if (_refcnt.get(i) == vec)     break;
    if (i == sp) {
      if (fs == null)       fs=new Futures();
      vec.remove(fs);
    }
  }
  if (fs != null)   fs.blockForPending();
}
