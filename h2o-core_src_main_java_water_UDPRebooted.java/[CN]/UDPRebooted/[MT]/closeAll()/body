{
  try {
    NetworkInit._udpSocket.close();
  }
 catch (  IOException ignore) {
  }
  try {
    NetworkInit._apiSocket.close();
  }
 catch (  IOException ignore) {
  }
  try {
    TCPReceiverThread.SOCK.close();
  }
 catch (  IOException ignore) {
  }
  PersistManager PM=H2O.getPM();
  if (PM != null)   PM.getIce().cleanUp();
}
