{
  float sum=score1(chks,fs,row);
  if (isClassifier() && !Float.isInfinite(sum) && sum > 0f)   ArrayUtils.div(fs,sum);
  if (_model._output._priorClassDist != null && _model._output._modelClassDist != null)   ModelUtils.correctProbabilities(fs,_model._output._priorClassDist,_model._output._modelClassDist);
}
