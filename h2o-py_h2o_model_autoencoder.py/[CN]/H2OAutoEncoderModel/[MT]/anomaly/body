def anomaly(self, test_data, per_feature=False):
    '\n    Obtain the reconstruction error for the input test_data.\n\n    :param test_data: The dataset upon which the reconstruction error is computed.\n    :param per_feature: Whether to return the square reconstruction error per feature. Otherwise, return the mean square error.\n    :return: Return the reconstruction error.\n    '
    if (not test_data):
        raise ValueError('Must specify test data')
    j = H2OConnection.post_json(((('Predictions/models/' + self._id) + '/frames/') + test_data.frame_id), reconstruction_error=True, reconstruction_error_per_feature=per_feature)
    return h2o.get_frame(j['model_metrics'][0]['predictions']['frame_id']['name'])
