def _draw(self, txt, final=False):
    u'Print the rendered string to the stdout.'
    if (not self._file_mode):
        sys.stdout.write(u'\r')
    sys.stdout.write(txt)
    if final:
        sys.stdout.write(u'\n')
    else:
        if (not self._file_mode):
            sys.stdout.write(u'\r')
        sys.stdout.flush()
