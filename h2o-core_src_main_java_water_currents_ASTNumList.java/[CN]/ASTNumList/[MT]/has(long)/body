{
  if (min() <= v && v < max()) {
    int[][] res=new int[2][];
    bsearch(v,res);
    if (res[0] != null)     return true;
 else {
      int lb=res[1][0], ub=res[1][1];
      if (_bases[lb] <= v && v < _bases[lb] + _cnts[lb])       return true;
      if (_bases[ub] <= v && v < _bases[ub] + _cnts[ub])       return true;
    }
  }
  return false;
}
