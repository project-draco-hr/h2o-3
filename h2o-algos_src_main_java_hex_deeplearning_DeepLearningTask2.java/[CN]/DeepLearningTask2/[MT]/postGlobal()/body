{
  super.postGlobal();
  _res.model_info().div(_res._chunk_node_count);
  _res.model_info().add_processed_global(_res.model_info().get_processed_local());
  _res.model_info().set_processed_local(0l);
  if (_res.consensusADMM) {
    _consensus_model_info=(DeepLearningModel.DeepLearningModelInfo)_res.model_info().clone();
  }
 else {
    _consensus_model_info=_res.model_info();
  }
}
