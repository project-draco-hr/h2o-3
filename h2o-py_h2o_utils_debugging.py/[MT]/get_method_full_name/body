def get_method_full_name(method):
    u'\n    Return full qualified method name.\n\n    This method will attempt to find the\n\n    :param method: a function object.\n\n    :returns: string of the form "<class name>.<method name>" if the method belongs to a class, or\n        "<module name>.<method name>" otherwise.\n    '
    module = inspect.getmodule(method)
    for cls_name in dir(module):
        cls = getattr(module, cls_name)
        if (not inspect.isclass(cls)):
            continue
        for method_name in dir(cls):
            cls_method = getattr(cls, method_name)
            if (cls_method == method):
                return (u'%s.%s' % (cls_name, method_name))
    if hasattr(method, u'__name__'):
        return (u'%s.%s' % (module.__name__, method.__name__))
    return u'<unknown>'
