@property
def stopping_metric(self):
    u'\n        Enum["auto", "deviance", "logloss", "mse", "auc", "lift_top_group", "r2", "misclassification",\n        "mean_per_class_error"]: Metric to use for early stopping (AUTO: logloss for classification, deviance for\n        regression) (Default: "auto")\n        '
    return self._parms.get(u'stopping_metric')
