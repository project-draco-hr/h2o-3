{
  int rows=chks[0]._len;
  for (  Chunk C : chks) {
    if (!C.vec().isNumeric())     throw new IllegalArgumentException("Numeric columns only");
    double sum=_d;
    for (int r=0; r < rows; r++) {
      double d=C.atd(r);
      if (!Double.isNaN(d))       sum=_bin.op(sum,d);
    }
    _d=sum;
    if (Double.isNaN(sum))     break;
  }
}
