{
switch (type) {
case html:
case java:
case json:
case xml:
{
      Class<Handler> clz=(Class<Handler>)route._handler_class;
      HandlerFactory handlerFactory=route._handler_factory;
      Handler h=handlerFactory.create(clz);
      return h.handle(version,route,parms);
    }
case query:
case help:
default :
  throw H2O.unimpl();
}
}
