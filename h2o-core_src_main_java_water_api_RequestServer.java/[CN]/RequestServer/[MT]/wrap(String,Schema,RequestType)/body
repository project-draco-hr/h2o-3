{
switch (type) {
case json:
    return new Response(http_code,MIME_JSON,new String(S.writeJSON(new AutoBuffer()).buf()));
case xml:
case java:
  throw H2O.unimpl();
case html:
{
  RString html=new RString(_htmlTemplate);
  html.replace("CONTENTS",new String(S.writeHTML(new AutoBuffer()).buf()));
  return new Response(http_code,MIME_HTML,html.toString());
}
default :
throw H2O.fail();
}
}
