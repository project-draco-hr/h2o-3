{
  if (uri.endsWith(".css") || uri.endsWith(".js") || uri.endsWith(".png")|| uri.endsWith(".ico"))   return false;
  if (uri.contains("/Cloud") || (uri.contains("/Jobs") && method.equals("GET")) || uri.contains("/Log")|| uri.contains("/Progress")|| uri.contains("/Typeahead")|| uri.contains("/WaterMeterCpuTicks"))   return false;
  String paddedMethod=String.format("%-6s",method);
  Log.info("Method: " + paddedMethod,", URI: " + uri + ", route: "+ pattern+ ", parms: "+ parms);
  return true;
}
