{
  if (!(v instanceof FileVec))   throw H2O.unimpl("ORC only implemented for HDFS / NFS files");
  try {
    if (((OrcParser.OrcParseSetup)setup).getOrcFileReader() == null)     ((OrcParser.OrcParseSetup)setup).setOrcFileReader(getReader((FileVec)v));
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
