def pybooklet_exec(test_name, h2o_py_dir):
    pyunit = 'import sys\nsys.path.insert(1, "{0}")\nimport h2o\nfrom tests import pybooklet_utils\n'.format(h2o_py_dir)
    with open(test_name, 'r') as t:
        pyunit = (pyunit + t.read())
    pyunit_c = compile(pyunit, '<string>', 'exec')
    p = {}
    exec pyunit_c in p
