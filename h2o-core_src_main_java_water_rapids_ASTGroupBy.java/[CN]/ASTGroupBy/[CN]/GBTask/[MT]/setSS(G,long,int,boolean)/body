{
  double v=Double.longBitsToDouble(vv);
  double o=g._ss[c];
  if (isReduce) {
    while (!G.CAS_ss(g,G.doubleRawIdx(c),Double.doubleToRawLongBits(o),Double.doubleToRawLongBits(o + v)))     o=g._ss[c];
  }
 else {
    while (!G.CAS_ss(g,G.doubleRawIdx(c),Double.doubleToRawLongBits(o),Double.doubleToRawLongBits(o + v * v)))     o=g._ss[c];
  }
}
