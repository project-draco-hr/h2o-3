{
  long start=c[0].start();
  byte[] naMethods=AGG.naMethods(_agg);
  for (int i=0; i < c[0]._len; ++i) {
    G g=new G(i,c,_gbCols,_agg.length,naMethods);
    if (!_g.add(g)) {
      G g2=g;
      g=_g.get(g);
      if (g == null) {
        while (g == null)         g=_g.get(g2);
      }
    }
    long r=g._N;
    while (!G.CAS_N(g,r,r + 1))     r=g._N;
    perRow(_agg,i,start,c,g);
  }
}
