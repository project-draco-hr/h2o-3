{
  long start=c[0].start();
  G g=new G(_gbCols.length,_agg);
  G gOld;
  for (int i=0; i < c[0]._len; ++i) {
    g.fill(i,c,_gbCols);
    String g_old=_g.putIfAbsent(g,"");
    if (g_old == null) {
      gOld=g;
      g=new G(_gbCols.length,_agg);
    }
 else     gOld=_g.getk(g);
    long r=gOld._N;
    while (!G.CAS_N(gOld,r,r + 1))     r=gOld._N;
    perRow(_agg,i,start,c,gOld);
  }
}
