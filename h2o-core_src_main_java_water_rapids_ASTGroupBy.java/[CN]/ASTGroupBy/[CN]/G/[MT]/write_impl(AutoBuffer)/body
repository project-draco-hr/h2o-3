{
  ab.put8(_N);
  ab.putA8(_ND);
  ab.putA8(_f);
  ab.putA8(_l);
  ab.putA8d(_min);
  ab.putA8d(_max);
  ab.putA8d(_sum);
  ab.putA8d(_ss);
  int len=_nd.length;
  ab.put4(len);
  for (  NonBlockingHashSet<Double> a_nd : _nd) {
    int s=a_nd.size();
    ab.put4(s);
    for (    double d : a_nd)     ab.put8d(d);
  }
  return ab;
}
