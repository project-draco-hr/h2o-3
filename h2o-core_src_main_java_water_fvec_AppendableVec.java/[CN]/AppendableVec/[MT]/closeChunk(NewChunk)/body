{
  final int cidx=chk._cidx - _chunkOff;
  while (cidx >= _chunkTypes.length)   _chunkTypes=Arrays.copyOf(_chunkTypes,_chunkTypes.length << 1);
  while (cidx >= _tmp_espc.length)   _tmp_espc=Arrays.copyOf(_tmp_espc,_tmp_espc.length << 1);
  _tmp_espc[cidx]=chk._len;
  _chunkTypes[cidx]=chk.type();
  _naCnt+=chk.naCnt();
  _enumCnt+=chk.enumCnt();
  _strCnt+=chk.strCnt();
  for (int i=0; i < _timCnt.length; i++)   _timCnt[i]+=chk._timCnt[i];
  _totalCnt+=chk._len;
}
