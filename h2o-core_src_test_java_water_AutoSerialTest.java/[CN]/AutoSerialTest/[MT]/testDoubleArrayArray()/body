{
  double[][][] tests={{{0.5,1.5,2.5},{},null,{6.3,-1.3,19.3,-49.4},{Double.MAX_VALUE,Double.MIN_VALUE}},null,{}};
  for (  double[][] exp : tests) {
    _doubless=exp;
    this.write(abw());
    this.read(abr());
    if (exp != null) {
      AssertJUnit.assertEquals(_doubless.length,exp.length);
      for (int i=0; i < exp.length; i++)       AssertJUnit.assertTrue(Arrays.equals(_doubless[i],exp[i]));
    }
 else     AssertJUnit.assertNull(_doubless);
  }
}
