{
  Frame fr=null;
  try {
    String tree="(GB hex [4] [0]  nrow 0 \"rm\"  mean 1 \"rm\"  sum 1 \"rm\"  min 1 \"rm\"  max 1 \"rm\" )";
    fr=chkTree(tree,"smalldata/iris/iris_wheader.csv");
    chkDim(fr,6,3);
    chkFr(fr,0,0,"Iris-setosa");
    chkFr(fr,1,0,50);
    chkFr(fr,2,0,3.418);
    chkFr(fr,3,0,170.9);
    chkFr(fr,4,0,2.3);
    chkFr(fr,5,0,4.4);
    chkFr(fr,0,1,"Iris-versicolor");
    chkFr(fr,1,1,50);
    chkFr(fr,2,1,2.770);
    chkFr(fr,3,1,138.5);
    chkFr(fr,4,1,2.0);
    chkFr(fr,5,1,3.4);
    chkFr(fr,0,2,"Iris-virginica");
    chkFr(fr,1,2,50);
    chkFr(fr,2,2,2.974);
    chkFr(fr,3,2,148.7);
    chkFr(fr,4,2,2.2);
    chkFr(fr,5,2,3.8);
  }
  finally {
    if (fr != null)     fr.delete();
    Keyed.remove(Key.make("hex"));
  }
}
