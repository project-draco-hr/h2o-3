{
  _m=m;
  _n=n;
  _s=new double[m][];
  _y=new double[m][];
  _rho=MemoryManager.malloc8d(m);
  Arrays.fill(_rho,Double.NaN);
  for (int i=0; i < m; ++i) {
    _s[i]=MemoryManager.malloc8d(n);
    Arrays.fill(_s[i],Double.NaN);
    _y[i]=MemoryManager.malloc8d(n);
    Arrays.fill(_y[i],Double.NaN);
  }
}
