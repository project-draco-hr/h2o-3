{
  super("Illegal argument: " + argument + " of function: "+ function+ ": "+ value.toString(),"Illegal argument: " + argument + " of function: "+ function+ ": "+ value.toString()+ " of class: "+ value.getClass());
  this.values=new IcedHashMap<String,Object>();
  this.values.put("function",function);
  this.values.put("argument",argument);
  this.values.put("value",value);
}
