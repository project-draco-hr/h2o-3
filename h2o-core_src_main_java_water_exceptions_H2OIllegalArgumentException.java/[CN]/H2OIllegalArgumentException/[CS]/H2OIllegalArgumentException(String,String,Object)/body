{
  super("Illegal argument: " + argument + " of function: "+ function+ ": "+ (value == null ? "null" : value.toString()),"Illegal argument: " + argument + " of function: "+ function+ ": "+ (value == null ? "null" : value.toString())+ " of class: "+ (value == null ? "null" : value.getClass()));
  this.values=new IcedHashMapGeneric.IcedHashMapStringObject();
  this.values.put("function",function);
  this.values.put("argument",argument);
  if (value != null)   this.values.put("value",value);
}
