def algo_to_classname(algo):
    if (algo == u'deeplearning'):
        return u'H2ODeepLearningEstimator'
    if (algo == u'gbm'):
        return u'H2OGradientBoostingEstimator'
    if (algo == u'glm'):
        return u'H2OGeneralizedLinearEstimator'
    if (algo == u'glrm'):
        return u'H2OGeneralizedLowRankEstimator'
    if (algo == u'kmeans'):
        return u'H2OKMeansEstimator'
    if (algo == u'naivebayes'):
        return u'H2ONaiveBayesEstimator'
    if (algo == u'drf'):
        return u'H2ORandomForestEstimator'
    if (algo == u'svd'):
        return u'H2OSingularValueDecompositionEstimator'
    if (algo == u'pca'):
        return u'H2OPrincipalComponentAnalysisEstimator'
    return ((u'H2O' + algo.capitalize()) + u'Estimator')
