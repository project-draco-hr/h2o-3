def init_extra_for(algo):
    if (algo == u'deeplearning'):
        return u"if isinstance(self, H2OAutoEncoderEstimator): self._parms['autoencoder'] = True"
    if (algo == u'glm'):
        return u'\n            if "Lambda" in kwargs:\n                self._parms["lambda"] = kwargs["Lambda"]'
    if (algo == u'glrm'):
        return u'self._parms["_rest_version"] = 3'
