{
  Chunk c=null;
  for (  Chunk x : chks)   if (x.getClass() == water.fvec.C2Chunk.class) {
    c=x;
    break;
  }
  AssertJUnit.assertNotNull("Expect to find a C2Chunk",c);
  assertTrue(c._vec.writable());
  double d=c._vec.min();
  for (int i=0; i < c.len(); i++) {
    double e=c.at0(i);
    c.set0(i,d);
    d=e;
  }
}
