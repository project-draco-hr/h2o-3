{
  if (testcases == null || testcases.length == 0) {
    return null;
  }
  if (StringUtils.isEmpty(size)) {
    return testcases;
  }
  Object[][] result=null;
  Object[][] temp=null;
  int nrows=0;
  int ncols=0;
  int r=0;
  int i=0;
  Dataset dataset=null;
  ncols=testcases.length;
  nrows=testcases[0].length;
  temp=new Object[ncols][nrows];
  for (i=0; i < ncols; i++) {
    dataset=(Dataset)testcases[i][4];
    if (dataset == null) {
      temp[r++]=testcases[i];
    }
 else     if (size.equals(dataset.dataSetURI)) {
      temp[r++]=testcases[i];
    }
  }
  if (r == 0) {
    System.out.println(String.format("dataset characteristic have no size what is: %s.",size));
  }
 else {
    result=new Object[r][nrows];
    for (i=0; i < r; i++) {
      result[i]=temp[i];
    }
  }
  return result;
}
