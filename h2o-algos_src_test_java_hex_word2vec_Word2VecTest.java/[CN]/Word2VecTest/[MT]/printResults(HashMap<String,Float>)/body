{
  TreeMap<Float,String> reversedMap=new TreeMap<Float,String>();
  for (  Map.Entry entry : hm.entrySet())   reversedMap.put((Float)entry.getValue(),(String)entry.getKey());
  for (  Map.Entry entry : reversedMap.descendingMap().entrySet())   System.out.println(entry.getKey() + ", " + entry.getValue());
}
