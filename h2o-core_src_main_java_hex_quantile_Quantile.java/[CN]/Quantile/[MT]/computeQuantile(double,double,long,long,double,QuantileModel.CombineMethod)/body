{
  if (lo == hi)   return lo;
switch (method) {
case INTERPOLATE:
    return linearInterpolate(lo,hi,row,nrows,prob);
case AVG:
  return 0.5 * (hi + lo);
case LO:
return lo;
case HI:
return hi;
default :
Log.info("Unknown even sample size quantile combination type: " + method + ". Doing linear interpolation.");
return linearInterpolate(lo,hi,row,nrows,prob);
}
}
