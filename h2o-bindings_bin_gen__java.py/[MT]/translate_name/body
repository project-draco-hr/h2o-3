def translate_name(name):
    '\n    Converts names with underscores into camelcase. For example:\n        "num_rows" => "numRows"\n        "very_long_json_name" => "veryLongJsonName"\n        "build_GBM_model" => "buildGbmModel"\n        "KEY" => "key"\n    '
    parts = name.split('_')
    parts[0] = parts[0].lower()
    for i in range(1, len(parts)):
        parts[i] = parts[i].capitalize()
    return ''.join(parts)
