def multi_dim_slice_cols_cmd(self, child):
    c = child._data[1]
    if isinstance(c, int):
        return ('#' + str(c))
    if isinstance(c, slice):
        return (((('(: #' + str(c.start)) + ' #') + str(c.stop)) + ')')
    if (c == 'null'):
        return '"null"'
    raise NotImplementedError
