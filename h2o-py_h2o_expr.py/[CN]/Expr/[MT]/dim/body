def dim(self):
    "\n    Eagerly evaluate the Expr. If it's an H2OFrame, return the number of rows and columns.\n    :return: The number of rows and columns in the H2OFrame as a list [rows, cols].\n    "
    self.eager()
    if isinstance(self._data, unicode):
        frame = h2o.frame(self._data)
        return [frame['frames'][0]['rows'], len(frame['frames'][0]['columns'])]
    raise ValueError('data must be a (unicode) key')
