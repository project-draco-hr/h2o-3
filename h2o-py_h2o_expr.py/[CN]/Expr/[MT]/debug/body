def debug(self):
    '\n    :return: The structure of this object without evaluating.\n    '
    return (((((((((('(' + self._name) + ' <== ') + str((self._left._name if isinstance(self._left, Expr) else self._left))) + ' ') + self._op) + ' ') + str((self._rite._name if isinstance(self._rite, Expr) else self._rite))) + ' ==> ') + str(type(self._data))) + ')')
