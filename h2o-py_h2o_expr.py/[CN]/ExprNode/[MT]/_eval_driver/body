def _eval_driver(self, top):
    exec_str = self._do_it(top)
    res = h2o.rapids(exec_str)
    if ('scalar' in res):
        self._data = res['scalar']
    if ('string' in res):
        self._data = res['string']
    if ('funstr' in res):
        raise NotImplementedError
    if ('key' in res):
        self._set_rows(res['num_rows'])
        self._set_cols(res['num_cols'])
    self._clear_impl()
    gc.collect()
    return self
