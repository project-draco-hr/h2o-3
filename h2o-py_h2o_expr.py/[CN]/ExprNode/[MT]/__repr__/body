def __repr__(self):
    '\n    String representation of all the fields; hopefully possible to\n    directly invert back to a H2OFrame from the string\n    '
    return ('Expr(op=%r,id=%r,ast=%r,nrows=%r,ncols=%r,data=%r)' % (self._op, self._id, self._ast, self._nrows, self._ncols, type(self._data)))
