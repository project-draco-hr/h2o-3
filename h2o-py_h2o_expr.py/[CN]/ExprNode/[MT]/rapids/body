@staticmethod
def rapids(expr):
    u'Execute a Rapids expression.\n\n    Parameters\n    ----------\n    expr : str\n      The rapids expression (ascii string).\n\n    Returns\n    -------\n      The JSON response (as a python dictionary) of the Rapids execution\n    '
    return h2o.connection().post_json(u'Rapids', ast=expr, session_id=h2o.connection().session_id(), _rest_version=99)
