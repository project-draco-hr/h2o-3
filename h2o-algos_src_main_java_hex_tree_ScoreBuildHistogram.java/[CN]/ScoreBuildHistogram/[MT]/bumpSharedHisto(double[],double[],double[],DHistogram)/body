{
  final int len=rh._bins.length;
  for (int b=0; b < len; b++) {
    if (bins[b] != 0) {
      AtomicUtils.DoubleArray.add(rh._bins,b,bins[b]);
      bins[b]=0;
    }
  }
  for (int b=0; b < len; b++) {
    if (sums[b] != 0 || ssqs[b] != 0) {
      rh.incr1(b,sums[b],ssqs[b]);
      sums[b]=ssqs[b]=0;
    }
  }
}
