{
  for (int k=0; k < i; ++k) {
    double w=ws[k];
    if (w == 0)     continue;
    double col_data=cs[k];
    if (col_data < minmax[0])     minmax[0]=col_data;
    if (col_data > minmax[1])     minmax[1]=col_data;
    int b=rh.bin(col_data);
    double resp=ys[k];
    double wy=w * resp;
    bins[b]+=w;
    sums[b]+=wy;
    ssqs[b]+=wy * resp;
  }
}
