{
  if (a instanceof Neurons.DenseVector)   randomlySparsifyActivation((Neurons.DenseVector)a,seed);
 else   if (a instanceof Neurons.SparseVector)   randomlySparsifyActivation((Neurons.SparseVector)a,seed);
 else   throw new UnsupportedOperationException("randomlySparsifyActivation not implemented for this type: " + a.getClass().getSimpleName());
}
