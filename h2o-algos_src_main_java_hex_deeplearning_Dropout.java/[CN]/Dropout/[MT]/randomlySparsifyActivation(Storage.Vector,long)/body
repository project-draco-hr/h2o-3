{
  if (a instanceof Storage.DenseVector)   randomlySparsifyActivation((Storage.DenseVector)a,seed);
 else   if (a instanceof Storage.SparseVector)   randomlySparsifyActivation((Storage.SparseVector)a,seed);
 else   throw new UnsupportedOperationException("randomlySparsifyActivation not implemented for this type: " + a.getClass().getSimpleName());
}
