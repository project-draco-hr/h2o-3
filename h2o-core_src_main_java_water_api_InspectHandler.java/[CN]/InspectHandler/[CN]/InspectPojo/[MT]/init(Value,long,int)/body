{
  _val=val;
  _off=off;
  _len=len;
  if (_val.isFrame()) {
    _schema=new FrameV2((Frame)_val.get(),off,len);
  }
 else   if (_val.isModel()) {
    Model m=_val.get();
    _schema=Schema.schema(Schema.getLatestVersion(),m).fillFromImpl(m);
  }
 else {
    throw H2O.unimpl("Unexpected val class for Inspect: " + _val.get().getClass());
  }
}
