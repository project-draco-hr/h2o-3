def __init__(self, keyvals):
    self._props = {}
    self._connection = None
    for (k, v) in keyvals:
        if ((k == u'__meta') or (k == u'_exclude_fields') or (k == u'__schema')):
            continue
        if (k in _cloud_v3_valid_keys):
            self._props[k] = v
        else:
            raise AttributeError((u'Attribute %s cannot be set on CloudV3 (= %r)' % (k, v)))
