{
  Group g=new Group(_gbCols.length);
  Group gOld;
  int start=(int)c[0].start();
  for (int i=0; i < c[0]._len; ++i) {
    g.fill(i,c,_gbCols);
    String old_g=_grps.putIfAbsent(g,"");
    if (old_g == null) {
      gOld=g;
      g=new Group(_gbCols.length);
    }
 else {
      gOld=_grps.getk(g);
      if (gOld == null)       while (gOld == null)       gOld=_grps.getk(g);
    }
    long cnt=gOld._N;
    while (!Group.CAS_N(gOld,cnt,cnt + 1))     cnt=gOld._N;
    gOld.a.put(start + i,"");
  }
}
