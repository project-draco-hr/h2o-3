{
  Group g=new Group(_gbCols.length);
  Group gOld;
  for (int i=0; i < c[0]._len; ++i) {
    g.fill(i,c,_gbCols);
    if (_grps.add(g)) {
      gOld=g;
      g=new Group(_gbCols.length);
    }
 else {
      gOld=_grps.get(g);
      if (gOld == null)       while (gOld == null)       gOld=_grps.get(g);
    }
    long cnt=gOld._N;
    while (!Group.CAS_N(gOld,cnt,cnt + 1))     cnt=gOld._N;
  }
}
