{
  if (_grps != t._grps) {
    ASTGroupBy.IcedNBHS<Group> l=_grps;
    ASTGroupBy.IcedNBHS<Group> r=t._grps;
    if (l.size() < r.size()) {
      l=r;
      r=_grps;
    }
    for (    Group rg : r) {
      if (!l.add(rg)) {
        Group lg=l.get(rg);
        long L=lg._N;
        while (!Group.CAS_N(lg,L,L + rg._N))         L=lg._N;
      }
    }
    _grps=l;
    t._grps=null;
  }
}
