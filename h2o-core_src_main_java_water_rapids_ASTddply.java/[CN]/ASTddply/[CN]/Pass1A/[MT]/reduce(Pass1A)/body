{
  if (_grps != t._grps) {
    IcedHM<Group,String> l=_grps;
    IcedHM<Group,String> r=t._grps;
    if (l.size() < r.size()) {
      l=r;
      r=_grps;
    }
    for (    Group rg : r.keySet()) {
      if (l.containsKey(rg)) {
        Group lg=l.getk(rg);
        long L=lg._N;
        while (!Group.CAS_N(lg,L,L + rg._N))         L=lg._N;
      }
    }
    _grps=l;
    t._grps=null;
  }
}
