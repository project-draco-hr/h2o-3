def unsupervised(kwargs):
    (x, y) = _supervised_frame_helper(kwargs['x'], None, kwargs['training_frame'])
    vx = None
    if ('validation_x' in kwargs):
        (vx, vy) = _supervised_frame_helper(kwargs['validation_x'], None, kwargs['validation_frame'])
    algo = kwargs['algo']
    parms = {k: v for (k, v) in kwargs.items() if ((k not in ['x', 'validation_x', 'algo']) and (v is not None))}
    return unsupervised_model_build(x, vx, algo, **parms)
