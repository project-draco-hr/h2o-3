def _supervised_frame_helper(x, y, fr):
    if ((not isinstance(x, H2OFrame)) or (not isinstance(y, H2OFrame))):
        if (fr is None):
            raise ValueError('Missing training_frame')
        if (not isinstance(x, H2OFrame)):
            x = fr[x]
        if (not isinstance(y, H2OFrame)):
            y = fr[y]
    return (x, y)
