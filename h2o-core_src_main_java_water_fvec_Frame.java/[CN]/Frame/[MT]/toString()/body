{
  StringBuilder sb=new StringBuilder();
  sb.append("Frame key: " + _key + "\n");
  sb.append("   cols: " + numCols() + "\n");
  sb.append("   rows: " + numRows() + "\n");
  sb.append(" chunks: " + anyVec().nChunks() + "\n");
  sb.append("   size: " + byteSize() + "\n");
  return sb.toString();
}
