{
  return new MRTask(){
    @Override public void map(    Chunk[] cs,    NewChunk[] ncs){
      for (int col=0; col < cs.length; col++) {
        Chunk c=cs[col];
        NewChunk nc=ncs[col];
        for (int row=0; row < c._len; row++)         nc.addNum(c.at0(row));
      }
    }
  }
.doAll(numCols(),this).outputFrame(names(),domains());
}
