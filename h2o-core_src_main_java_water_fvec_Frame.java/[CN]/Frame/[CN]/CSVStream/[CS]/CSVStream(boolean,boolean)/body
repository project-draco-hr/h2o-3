{
  _curChkIdx=0;
  _hex_string=hex_string;
  StringBuilder sb=new StringBuilder();
  Vec vs[]=vecs();
  if (headers) {
    sb.append('"').append(_names[0]).append('"');
    for (int i=1; i < vs.length; i++)     sb.append(',').append('"').append(_names[i]).append('"');
    sb.append('\n');
  }
  _line=sb.toString().getBytes();
}
