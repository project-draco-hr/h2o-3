{
  Chunk pred=chks[chks.length - 1];
  for (int i=0; i < pred.len(); ++i) {
    if (pred.at0(i) != 0) {
      for (int j=0; j < chks.length - 1; j++) {
        Chunk chk=chks[j];
        if (chk._vec.isUUID())         nchks[j].addUUID(chk,i);
 else         nchks[j].addNum(chk.at0(i));
      }
    }
  }
}
