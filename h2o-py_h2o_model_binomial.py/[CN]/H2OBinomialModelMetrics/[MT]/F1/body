def F1(self, thresholds=None):
    '\n    :param thresholds: thresholds parameter must be a list (i.e. [0.01, 0.5, 0.99]). If None, then the thresholds in this set of metrics will be used.\n    :return: The F1 for the given set of thresholds.\n    '
    return self.metric('f1', thresholds=thresholds)
