def accuracy(self, thresholds=None):
    '\n    :param thresholds: thresholds parameter must be a list (i.e. [0.01, 0.5, 0.99]). If None, then the thresholds in this set of metrics will be used.\n    :return: The accuracy for this set of metrics and thresholds\n    '
    return self.metric('accuracy', thresholds=thresholds)
