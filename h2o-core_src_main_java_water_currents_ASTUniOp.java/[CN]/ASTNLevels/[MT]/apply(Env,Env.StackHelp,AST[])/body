{
  int nlevels;
  Frame fr=stk.track(asts[1].exec(env)).getFrame();
  if (fr.numCols() == 1) {
    Vec v=fr.anyVec();
    nlevels=v.isEnum() ? v.domain().length : 0;
    return new ValNum(nlevels);
  }
 else   throw new IllegalArgumentException("nlevels applies to a single column. Got: " + fr.numCols());
}
