{
  Val val=stk.track(asts[1].exec(env));
switch (val.type()) {
case Val.NUM:
    return new ValNum(op(val.getNum()));
case Val.FRM:
  return new ValFrame(new MRTask(){
    @Override public void map(    Chunk chk,    NewChunk cres){
      for (int i=0; i < chk._len; i++)       cres.addNum(op(chk.atd(i)));
    }
  }
.doAll(1,val.getFrame()).outputFrame());
case Val.STR:
throw H2O.unimpl();
default :
throw H2O.fail();
}
}
