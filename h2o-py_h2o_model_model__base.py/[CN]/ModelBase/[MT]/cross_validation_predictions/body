def cross_validation_predictions(self):
    u'\n    Obtain the (out-of-sample) holdout predictions of all cross-validation models on their holdout data.\n    Note that the predictions are expanded to the full number of rows of the training data, with 0 fill-in.\n    :return: list of H2OFrame objects\n    '
    preds = self._model_json[u'output'][u'cross_validation_predictions']
    if (preds is None):
        return None
    m = []
    for p in preds:
        m.append(h2o.get_frame(p[u'name']))
    return m
