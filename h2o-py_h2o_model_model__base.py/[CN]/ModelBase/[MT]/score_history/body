def score_history(self):
    '\n    Retrieve Model Score History\n    :return: the score history (H2OTwoDimTable)\n    '
    model = self._model_json['output']
    if (('scoring_history' in model.keys()) and (model['scoring_history'] != None)):
        s = model['scoring_history']
        if can_use_pandas():
            import pandas
            pandas.options.display.max_rows = 20
            return pandas.DataFrame(s.cell_values, columns=s.col_header)
        return s
    else:
        print 'No score history for this model'
