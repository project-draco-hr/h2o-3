def cross_validation_holdout_predictions(self):
    u'\n    Obtain the (out-of-sample) holdout predictions of all cross-validation models on the training data.\n    This is equivalent to summing up all H2OFrames returned by cross_validation_predictions.\n    :return: H2OFrame\n    '
    preds = self._model_json[u'output'][u'cross_validation_holdout_predictions_frame_id']
    if (preds is None):
        return None
    return h2o.get_frame(preds[u'name'])
