@staticmethod
def _check_targets(y_actual, y_predicted):
    u'Check that y_actual and y_predicted have the same length.\n\n    :param y_actual: An H2OFrame\n    :param y_predicted: An H2OFrame\n    :return: None\n    '
    if (len(y_actual) != len(y_predicted)):
        raise ValueError(u'Row mismatch: [{},{}]'.format(len(y_actual), len(y_predicted)))
