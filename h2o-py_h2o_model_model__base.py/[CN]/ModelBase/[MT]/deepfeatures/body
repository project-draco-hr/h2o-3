def deepfeatures(self, test_data, layer):
    '\n    Return hidden layer details\n\n    :param test_data: Data to create a feature space on\n    :param layer: 0 index hidden layer\n    '
    if (test_data is None):
        raise ValueError('Must specify test data')
    j = H2OJob(h2o.connection().post_json(((('Predictions/models/' + self._id) + '/frames/') + test_data.frame_id), deep_features_hidden_layer=layer, _rest_version=4), 'deepfeatures')
    j.poll()
    return h2o.get_frame(j.dest_key)
