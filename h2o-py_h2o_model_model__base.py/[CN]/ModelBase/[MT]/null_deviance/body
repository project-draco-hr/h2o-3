def null_deviance(self, train=False, valid=False):
    '\n    Retreive the null deviance if this model has the attribute, or None otherwise.\n    :param:  train Get the null deviance for the training set. If both train and valid are False, then train is selected by default.\n    :param:  valid Get the null deviance for the validation set. If both train and valid are True, then train is selected by default.\n    :return: Return the null deviance, or None if it is not present.\n    '
    if ((not train) and (not valid)):
        train = True
    if (train and valid):
        train = True
    if train:
        return self._model_json['output']['training_metrics'].null_deviance()
    else:
        return self._model_json['output']['validation_metrics'].null_deviance()
