@property
def params(self):
    u'\n    Get the parameters and the actual/default values only.\n\n    :return: A dictionary of parameters used to build this model.\n    '
    params = {}
    for p in self.parms:
        params[p] = {u'default': self.parms[p][u'default_value'], u'actual': self.parms[p][u'actual_value'], }
    return params
