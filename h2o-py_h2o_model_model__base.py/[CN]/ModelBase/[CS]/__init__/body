def __init__(self):
    u'Construct a new model instance.'
    super(ModelBase, self).__init__()
    self._id = None
    self._model_json = None
    self._metrics_class = None
    self._is_xvalidated = False
    self._xval_keys = None
    self._parms = {}
    self.parms = {}
    self._estimator_type = u'unsupervised'
    self._future = False
    self._job = None
