{
  sb.append("<span style='display: inline-block;'>");
  sb.append("<table class='table table-striped table-bordered'>\n");
  if (headers != null) {
    sb.append("<tr>");
    for (    String s : headers)     sb.append("<th>").append(s).append("</th>");
    sb.append("</tr>\n");
  }
  return sb;
}
