{
switch (distribution) {
case AUTO:
case gaussian:
case huber:
case laplace:
case quantile:
    return f;
case bernoulli:
case modified_huber:
  return "1/(1+" + expString("-" + f) + ")";
case multinomial:
case poisson:
case gamma:
case tweedie:
return expString(f);
default :
throw H2O.unimpl();
}
}
