{
switch (distribution) {
case gaussian:
case bernoulli:
case multinomial:
case huber:
    return w * z;
case poisson:
  return w * y;
case gamma:
return w * (z + 1);
case tweedie:
return w * y * exp(f * (1 - tweediePower));
case modified_huber:
double yf=(2 * y - 1) * f;
if (yf < -1) return w * 4 * (2 * y - 1);
 else if (yf > 1) return 0;
 else return w * 2 * (2 * y - 1)* (1 - yf);
default :
throw H2O.unimpl();
}
}
