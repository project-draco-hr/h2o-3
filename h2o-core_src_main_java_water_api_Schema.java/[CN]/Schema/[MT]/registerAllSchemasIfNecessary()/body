{
  if (schemas_registered)   return;
  Reflections reflections=null;
  new Schema();
  for (  Class<? extends Schema> schema_class : (new Reflections("")).getSubTypesOf(Schema.class))   if (!Modifier.isAbstract(schema_class.getModifiers()))   Schema.register(schema_class);
  schemas_registered=true;
  Log.info("Registered: " + Schema.schemas().size() + " schemas.");
}
