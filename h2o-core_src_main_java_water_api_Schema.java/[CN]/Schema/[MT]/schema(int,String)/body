{
  Class<? extends Schema> clz=schemaClass(version,type);
  if (null == clz)   clz=schemaClass(Schema.getExperimentalVersion(),type);
  if (null == clz)   throw new H2ONotFoundArgumentException("Failed to find schema for version: " + version + " and type: "+ type,"Failed to find schema for version: " + version + " and type: "+ type);
  return Schema.newInstance(clz);
}
