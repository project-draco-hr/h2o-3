def connect(self, size=1):
    while True:
        cld = requests.get((self.url() + 'Cloud.json')).json()
        if (not cld['cloud_healthy']):
            raise ValueError('Cluster reports unhealthy status', cld)
        if ((cld['cloud_size'] >= size) and cld['consensus']):
            return cld
        time.sleep(0.1)
