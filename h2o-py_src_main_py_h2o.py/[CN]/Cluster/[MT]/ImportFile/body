def ImportFile(self, path):
    j = requests.get((self.url() + 'ImportFiles.json'), params={'path': path, }).json()
    if ('errmsg' in j):
        raise ValueError(j['errmsg'])
    if j['fails']:
        raise ValueError(((('ImportFiles of ' + path) + ' failed on ') + j['fails']))
    return j['keys'][0]
