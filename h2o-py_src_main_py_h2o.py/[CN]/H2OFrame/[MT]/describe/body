def describe(self):
    self._vecs[0].summary()
    print 'Rows:', len(self._vecs[0]), 'Cols:', len(self)
    headers = [vec._name for vec in self._vecs]
    table = [_row('type', self._vecs, None), _row('mins', self._vecs, 0), _row('mean', self._vecs, None), _row('maxs', self._vecs, 0), _row('sigma', self._vecs, None), _row('zeros', self._vecs, None), _row('missing', self._vecs, None)]
    print tabulate(table, headers)
