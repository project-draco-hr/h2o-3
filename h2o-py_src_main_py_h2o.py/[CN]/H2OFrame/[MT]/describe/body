def describe(self):
    print 'Rows:', len(self._vecs[0]), 'Cols:', len(self)
    headers = [vec._name for vec in self._vecs]
    table = [row('missing', self._vecs, None), row('mean', self._vecs, None), row('sigma', self._vecs, None), row('zeros', self._vecs, None), row('mins', self._vecs, 0), row('maxs', self._vecs, 0)]
    print tabulate(table, headers)
