def __setitem__(self, b, c):
    if isinstance(b, str):
        for (i, v) in enumerate(self._vecs):
            if (b == v._name):
                break
        else:
            raise ValueError((('Name ' + i) + ' not in Frame'))
        if (len(c) != len(v)):
            raise ValueError(((('Vec len()=' + len(c)) + ' not compatible with Frame len()=') + len(v)))
        c._name = b
        self._vecs[i] = c
        return self
    raise NotImplementedError
