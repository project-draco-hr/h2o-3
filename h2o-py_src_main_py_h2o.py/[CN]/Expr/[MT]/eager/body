def eager(self):
    if self.isComputed():
        return self._data
    global _CMD
    assert (not _CMD)
    assert (not self._name.startswith('TMP_'))
    _CMD = ''
    self._doit()
    cmd = _CMD
    _CMD = None
    if self.isLocal():
        return self._data
    j = H2OCONN.Rapids(cmd)
    if isinstance(self._data, unicode):
        pass
    else:
        self._data = (j['head'] if j['num_rows'] else j['scalar'])
    return self._data
