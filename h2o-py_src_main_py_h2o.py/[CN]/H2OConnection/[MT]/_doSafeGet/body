def _doSafeGet(self, url):
    r = requests.get(url)
    j = r.json()
    if ('errmsg' in j):
        raise ValueError(j['errmsg'])
    if ('http_status' in j):
        if ((j['http_status'] == 404) or (j['http_status'] == 500)):
            if j['msg']:
                raise ValueError(j['msg'])
            if ('stacktrace' in j):
                raise ValueError(j['stacktrace'])
            raise ValueError(j)
    return j
