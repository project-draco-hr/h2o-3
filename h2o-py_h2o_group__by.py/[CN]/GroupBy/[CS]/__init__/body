def __init__(self, fr, by):
    self._fr = fr
    self._by = by
    self._aggs = {}
    self._res = None
    if test_type(by, str):
        self._by = [self._fr.names.index(by)]
    elif test_type(by, list, tuple):
        self._by = [(self._fr.names.index(b) if test_type(b, str) else b) for b in by]
    else:
        self._by = [self._by]
