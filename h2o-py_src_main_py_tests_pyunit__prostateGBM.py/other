import h2o
h2o.init()
df = h2o.import_frame(path='smalldata/logreg/prostate.csv')
print df.describe()
train = df.drop('ID')
vol = train['VOL']
vol[(vol == 0)] = None
gle = train['GLEASON']
gle[(gle == 0)] = None
train['CAPSULE'] = train['CAPSULE'].asfactor()
print train.describe()
gbm = H2OGBM(dataset=train, x='CAPSULE', ntrees=50, shrinkage=0.1)
print gbm._model
