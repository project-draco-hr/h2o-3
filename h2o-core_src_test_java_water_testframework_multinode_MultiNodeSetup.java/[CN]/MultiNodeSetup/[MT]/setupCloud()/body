{
  System.out.println("MultiNodeSetup creating " + _numNodes + " nodes...");
  H2O.main(new String[]{});
  if (System.getProperty("ai.h2o.skipNodeCreation") == null)   for (int i=0; i < _numNodes - 1; i++)   new NodeContainer(new String[0]).start();
  H2O.waitForCloudSize(_numNodes,10000);
  System.out.println("Created");
}
