{
  double[][] gt=ArrayUtils.gaussianArray(_parms._nv,_ncolExp,seed);
  RandSubInit rtsk=new RandSubInit(self(),dinfo,gt);
  rtsk.doAll(dinfo._adaptedFrame);
  Frame yinit=rtsk.outputFrame(Key.make(),null,null);
