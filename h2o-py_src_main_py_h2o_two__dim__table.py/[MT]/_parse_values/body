def _parse_values(values, types):
    for (k, v) in enumerate(values):
        for (j, val) in enumerate(v):
            if (types[j] == 'integer'):
                values[k][j] = int(float.fromhex(val))
            elif ((types[j] == 'double') or (types[j] == 'float') or (types[j] == 'long')):
                values[k][j] = float.fromhex(val)
            else:
                continue
    return values
