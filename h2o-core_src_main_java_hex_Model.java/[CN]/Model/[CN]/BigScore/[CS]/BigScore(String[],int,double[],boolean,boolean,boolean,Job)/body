{
  _j=j;
  _domain=domain;
  _npredcols=ncols;
  _mean=mean;
  _computeMetrics=computeMetrics;
  _makePreds=makePreds;
  if (_output._hasWeights && _computeMetrics && !testHasWeights)   throw new IllegalArgumentException("Missing weights when computing validation metrics.");
  _hasWeights=testHasWeights;
}
