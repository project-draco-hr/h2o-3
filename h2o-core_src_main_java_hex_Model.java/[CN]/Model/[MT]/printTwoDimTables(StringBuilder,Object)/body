{
  for (  Field f : Weaver.getWovenFields(o.getClass())) {
    Class<?> c=f.getType();
    if (c.isAssignableFrom(TwoDimTable.class)) {
      try {
        TwoDimTable t=(TwoDimTable)f.get(this);
        f.setAccessible(true);
        if (t != null)         sb.append(t.toString());
      }
 catch (      IllegalAccessException e) {
        e.printStackTrace();
      }
    }
  }
}
