{
  long start_time=System.currentTimeMillis();
  Frame adaptFr=new Frame(fr);
  adaptTestForTrain(adaptFr,true);
  Frame output=scoreImpl(adaptFr);
  computeModelMetrics(start_time,fr,output);
  Vec[] vecs=adaptFr.vecs();
  for (int i=0; i < vecs.length; i++)   if (vecs[i] == fr.vecs()[i])   vecs[i]=null;
  adaptFr.delete();
  return output;
}
