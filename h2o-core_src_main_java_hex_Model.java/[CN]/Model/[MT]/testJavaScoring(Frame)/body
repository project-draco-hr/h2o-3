{
  try {
    Class clz=ClassPool.getDefault().toClass(makeCtClass());
    Object modelo=clz.newInstance();
  }
 catch (  CannotCompileException cce) {
    throw new Error(cce);
  }
catch (  InstantiationException cce) {
    throw new Error(cce);
  }
catch (  IllegalAccessException cce) {
    throw new Error(cce);
  }
}
