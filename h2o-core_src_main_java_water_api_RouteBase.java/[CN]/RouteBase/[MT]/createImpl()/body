{
  Route i=null;
  try {
    Class<? extends Handler> clz=(Class<? extends Handler>)Class.forName(this.handler_class);
    i=new Route(this.http_method,Pattern.compile(this.url_pattern),this.summary,clz,null,null,this.path_params);
  }
 catch (  Exception e) {
    Log.warn("Caught exception instantiating a Route: " + e + "; "+ this);
  }
  return i;
}
