def grid_wineGBM(ip, port):
    h2o.init(ip, port)
    wine = h2o.import_frame(path=h2o.locate('smalldata/gbm_test/wine.data'))
    x_cols = (range(2, 14) + [0])
    wine_grid = h2o.gbm(y=wine[1], x=wine[x_cols], loss='AUTO', ntrees=[5, 10, 15], max_depth=[2, 3, 4], learn_rate=[0.1, 0.2])
    wine_grid.show()
