{
  String f;
switch (level) {
case "trace":
    f="-1-trace.log";
  break;
case "debug":
f="-2-debug.log";
break;
case "info":
f="-3-info.log";
break;
case "warn":
f="-4-warn.log";
break;
case "error":
f="-5-error.log";
break;
case "fatal":
f="-6-fatal.log";
break;
default :
throw new Exception("Unknown level");
}
return getLogFileNameStem() + f;
}
