{
  Chunk msec=chk[0];
  NewChunk month=ncs[0];
  NewChunk wkday=ncs[1];
  NewChunk hour=ncs[2];
  MutableDateTime mdt=new MutableDateTime();
  for (int i=0; i < msec._len; i++) {
    mdt.setMillis(msec.at8(i));
    month.addNum(mdt.getMonthOfYear() - 1);
    wkday.addNum(mdt.getDayOfWeek() - 1);
    hour.addNum(mdt.getHourOfDay());
  }
}
