{
  this.key=j.key;
  Job[] js=j.jobs;
  jobs=null;
  if (null != js) {
    jobs=new JobV2[js.length];
    for (int i=0; i < js.length; i++) {
      Job job=js[i];
      jobs[i]=new JobV2(job._key,job._description,job._state.toString(),job.progress(),job.msec(),job.dest(),job._exception);
    }
  }
  return this;
}
