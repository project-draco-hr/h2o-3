def head(self, rows=10, cols=200, **kwargs):
    "\n    Analgous to R's `head` call on a data.frame. Display a digestible chunk of the H2OFrame starting from the beginning.\n\n    :param rows: Number of rows to display.\n    :param cols: Number of columns to display.\n    :param kwargs: Extra arguments passed from other methods.\n    :return: None\n    "
    nrows = min(self.nrow(), rows)
    ncols = min(self.ncol(), cols)
    colnames = self.names()[0:ncols]
    head = self[0:10, :]
    res = head.as_data_frame(False)
    print 'First {} rows and first {} columns: '.format(nrows, ncols)
    h2o.H2ODisplay(res, (['Row ID'] + colnames))
