{
  exec.skipWS();
  exec.xpeek('(');
  Val val=exec.val();
  if (!val.isFun() && !val.isID())   exec.throwErr("Expected a function but found a " + val.getClass());
  ArrayList<Val> vals=new ArrayList<>();
  vals.add(0,val);
  while (exec.skipWS() != ')')   vals.add(exec.val());
  exec.xpeek(')');
  return new ASTExec(vals.toArray(new Val[vals.size()]));
}
