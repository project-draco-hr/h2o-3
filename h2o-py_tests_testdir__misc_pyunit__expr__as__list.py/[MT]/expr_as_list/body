def expr_as_list(ip, port):
    h2o.init(ip, port)
    iris = h2o.import_frame(path=h2o.locate('smalldata/iris/iris_wheader.csv'))
    res = (2 - iris)
    res = h2o.as_list(res)
    res = (2 - iris)
    res = h2o.as_list(res[0])
    expr = h2o.as_list(Expr([1, 2, 3]))
    expr = h2o.as_list(Expr([[1, 2, 3], [4, 5, 6]]))
