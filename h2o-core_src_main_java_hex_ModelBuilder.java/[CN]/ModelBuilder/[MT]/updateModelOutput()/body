{
  new TAtomic<M>(){
    @Override public M atomic(    M old){
      if (old != null) {
        old._output._status=_state;
        old._output._start_time=_start_time;
        old._output._end_time=_end_time;
        old._output._run_time=_end_time - _start_time;
      }
      return old;
    }
  }
.invoke(dest());
}
