{
  if (!_builders.containsValue(clz))   throw H2O.fail("Failed to find ModelBuilder class in registry: " + clz);
  for (  Map.Entry<String,Class<? extends ModelBuilder>> entry : _builders.entrySet())   if (entry.getValue().equals(clz))   return entry.getKey();
  throw H2O.fail("Failed to find ModelBuilder class in registry: " + clz);
}
