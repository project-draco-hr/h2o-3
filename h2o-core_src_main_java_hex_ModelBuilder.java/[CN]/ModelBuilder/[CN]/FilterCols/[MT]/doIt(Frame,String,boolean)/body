{
  boolean any=false;
  for (int i=0; i < f.vecs().length; i++) {
    if (filter(f.vecs()[i]) && f._names[i] != _row_weights_name) {
      if (any)       msg+=", ";
      any=true;
      msg+=f._names[i];
      f.remove(i);
      i--;
    }
  }
  if (any) {
    warn("_train",msg);
    if (expensive)     Log.info(msg);
  }
}
