{
  Vec v=new Vec(vecKey,ESPC.rowLayout(vecKey,new long[]{0,vals.length}),domain);
  NewChunk nc=new NewChunk(v,0);
  Futures fs=new Futures();
  for (  double d : vals) {
    assert(Double.isNaN(d) || (long)d == d);
    nc.addNum(d);
  }
  nc.close(fs);
  DKV.put(v._key,v,fs);
  fs.blockForPending();
  return v;
}
