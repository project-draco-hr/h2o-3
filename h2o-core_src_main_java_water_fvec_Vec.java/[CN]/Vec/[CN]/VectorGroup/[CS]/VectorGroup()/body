{
  byte[] bits=new byte[26];
  bits[0]=Key.GRP;
  bits[1]=-1;
  UnsafeUtils.set4(bits,2,-1);
  UnsafeUtils.set4(bits,6,-1);
  UUID uu=UUID.randomUUID();
  UnsafeUtils.set8(bits,10,uu.getLeastSignificantBits());
  UnsafeUtils.set8(bits,18,uu.getMostSignificantBits());
  _key=Key.make(bits);
  _len=0;
}
