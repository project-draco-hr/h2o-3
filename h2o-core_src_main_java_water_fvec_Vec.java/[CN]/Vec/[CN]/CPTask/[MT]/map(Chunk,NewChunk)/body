{
  for (int i=0; i < c._len; ++i) {
    if (c.isNA(i)) {
      nc.addNA();
      continue;
    }
    if (_domain == null)     nc.addNum(c.at8(i));
 else {
      long num=Arrays.binarySearch(_domain,c.at8(i));
      if (num < 0)       throw new IllegalArgumentException("Could not find the enum value!");
      nc.addNum(num);
    }
  }
}
