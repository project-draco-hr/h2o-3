{
  String newUri="";
  StringTokenizer st=new StringTokenizer(uri,"/ ",true);
  while (st.hasMoreTokens()) {
    String tok=st.nextToken();
switch (tok) {
case "/":
      newUri+="/";
    break;
case " ":
  newUri+="%20";
break;
default :
try {
newUri+=URLEncoder.encode(tok,"UTF-8");
}
 catch (UnsupportedEncodingException e) {
throw Log.throwErr(e);
}
break;
}
}
return newUri;
}
