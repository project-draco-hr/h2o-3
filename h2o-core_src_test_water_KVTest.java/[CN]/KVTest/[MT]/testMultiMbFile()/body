{
  NFSFileVec nfs=null;
  try {
    File file=find_test_file("build/h2o-core.jar");
    if (file == null)     return;
    nfs=NFSFileVec.make(file);
    ByteHisto bh=new ByteHisto().doAll(nfs);
    int sum=water.util.Arrays.sum(bh._x);
    assertEquals(file.length(),sum);
  }
  finally {
    if (nfs != null)     nfs.remove();
  }
}
