def stop(self):
    if (self.job_id is None):
        return
    try:
        try:
            print_barrier()
            cmd = ['hadoop', 'job', '-kill', self.job_id]
            print ('+ CMD: ' + str(cmd))
            subprocess.check_call(cmd)
        except Exception as e:
            print ''
            print ''
            print e
            print ''
            print ''
    finally:
        self.job_id = None
