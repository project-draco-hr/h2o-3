{
  value=value.trim();
  if (StringUtils.isEmpty(value)) {
    if (isRequired) {
      return name + "is empty";
    }
 else {
      return null;
    }
  }
  String result=null;
switch (type) {
case "int":
    if (!NumberUtils.isDigits(value)) {
      result=name + " is not digits";
    }
  break;
case "float":
case "double":
if (!NumberUtils.isNumber(value)) {
  result=name + " is not numberic";
}
break;
case "double[]":
break;
default :
break;
}
return result;
}
