{
  _nobs+=nt._nobs;
  ArrayUtils.add(_rescnt,nt._rescnt);
  if (null != _jntcnt) {
    for (int col=0; col < _jntcnt.length; col++)     ArrayUtils.add(_jntcnt[col],nt._jntcnt[col]);
  }
  if (null != _jntsum) {
    for (int col=0; col < _jntsum.length; col++)     ArrayUtils.add(_jntsum[col],nt._jntsum[col]);
  }
}
