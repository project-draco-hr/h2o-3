def __init__(self, **kwargs):
    super(H2OKMeansEstimator, self).__init__()
    self._parms = {}
    for name in ['model_id', 'training_frame', 'validation_frame', 'nfolds', 'keep_cross_validation_predictions', 'keep_cross_validation_fold_assignment', 'fold_assignment', 'fold_column', 'ignored_columns', 'ignore_const_cols', 'score_each_iteration', 'k', 'user_points', 'max_iterations', 'standardize', 'seed', 'init', 'max_runtime_secs']:
        pname = (name[:(-1)] if (name[(-1)] == '_') else name)
        self._parms[pname] = (kwargs[name] if (name in kwargs) else None)
