def parse_args(args):
    global _H2O_IP_
    global _H2O_PORT_
    global _ON_HADOOP_
    global _HADOOP_NAMENODE_
    global _IPYNB_
    i = 1
    while (i < len(args)):
        s = args[i]
        if ((s == '--usecloud') or (s == '--uc')):
            i = (i + 1)
            if (i > len(args)):
                usage()
            argsplit = args[i].split(':')
            _H2O_IP_ = argsplit[0]
            _H2O_PORT_ = int(argsplit[1])
        elif (s == '--hadoopNamenode'):
            i = (i + 1)
            if (i > len(args)):
                usage()
            _HADOOP_NAMENODE_ = args[i]
        elif (s == '--onHadoop'):
            _ON_HADOOP_ = True
        elif (s == '--ipynb'):
            i = (i + 1)
            if (i > len(args)):
                usage()
            _IPYNB_ = args[i]
        else:
            unknownArg(s)
        i = (i + 1)
