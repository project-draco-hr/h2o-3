def run_test(sys_args, test_to_run):
    parse_args(sys_args)
    h2o.init(ip=get_test_ip(), port=get_test_port(), strict_version_check=False)
    h2o.log_and_echo('------------------------------------------------------------')
    h2o.log_and_echo('')
    h2o.log_and_echo(('STARTING TEST: ' + str(h2o.ou())))
    h2o.log_and_echo('')
    h2o.log_and_echo('------------------------------------------------------------')
    try:
        nb = get_ipynb()
        if nb:
            utils.ipy_notebook_exec(nb, save_and_norun=False)
        else:
            test_to_run()
    finally:
        h2o.remove_all()
