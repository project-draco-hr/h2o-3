{
  for (int i=0; i < _mean.length; ++i) {
    double wsum=_wsum[i] + bs._wsum[i];
    double delta=bs._mean[i] - _mean[i];
    _mean[i]=(_wsum[i] * _mean[i] + bs._wsum[i] * bs._mean[i]) / wsum;
    _m2[i]+=bs._m2[i] + delta * delta * _wsum[i]* bs._wsum[i] / wsum;
    _wsum[i]=wsum;
  }
  _nobs+=bs._nobs;
}
