{
  ConcurrencyUtils.setNumberOfThreads(1);
  for (  Vec v : input.vecs()) {
    if (v.naCnt() > 0)     throw new H2OIllegalArgumentException("FFT can not be computed on rows with missing values");
    if (!v.isNumeric())     throw new H2OIllegalArgumentException("FFT can only be computed on numeric columns");
  }
}
