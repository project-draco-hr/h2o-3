  basicDRFTestOOBE_Classification("./smalldata/gbm_test/czechboard_300x300.csv","czechboard_300x300.hex",new PrepData(){
    @Override int prep(    Frame fr){
      Vec resp=fr.remove("C2");
      fr.add("C2",resp.toCategorical());
      resp.remove();
      return fr.find("C3");
    }
  }
