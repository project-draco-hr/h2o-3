{
  for (; i < bytes.length; i++) {
    char b=(char)bytes[i], c=0;
switch (b) {
case '%':
      c='%';
    break;
case '.':
  c='d';
break;
case '/':
c='s';
break;
case ':':
c='c';
break;
case '"':
c='q';
break;
case '>':
c='g';
break;
case '<':
c='l';
break;
case '\\':
c='b';
break;
case '\0':
c='z';
break;
}
if (c != 0) sb.append('%').append(c);
 else sb.append(b);
}
return sb;
}
