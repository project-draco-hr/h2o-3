{
  int type=peekTypeAt(i);
  Object o=peekAt(i);
switch (type) {
case ARY:
    return ((ASTFrame)o)._fr.numRows() + "x" + ((ASTFrame)o)._fr.numCols();
case NUM:
  return Double.toString(((ASTNum)o)._d);
case STR:
return ((ASTString)o)._s;
case ID:
return ((ASTId)o)._id;
case NULL:
return "null";
default :
throw H2O.fail("Bad value on the stack");
}
}
