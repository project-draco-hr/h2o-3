{
  _local.clear();
  Futures fs=new Futures();
  for (  Vec v : _refcnt.keySet()) {
    if (!_locked.contains(v._key))     fs=removeVec(v,fs);
    extinguishCounts(v);
  }
  if (_local_locked != null) {
    for (    Key k : _local_locked) {
      Keyed.remove(k,fs);
    }
  }
  fs.blockForPending();
  _stack.popAll();
}
