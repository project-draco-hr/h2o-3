{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  if (_bitoff > 0)   sb.append("...").append(_bitoff).append(" 0-bits... ");
  int bytes=bytes(_nbits);
  for (int i=0; i < bytes; i++) {
    if (i > 0)     sb.append(' ');
    sb.append(String.format("%8s",Integer.toBinaryString(0xFF & _val[i])).replace(' ','0'));
  }
  sb.append("}");
  return sb.toString();
}
