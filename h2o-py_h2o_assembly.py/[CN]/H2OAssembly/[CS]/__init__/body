def __init__(self, steps):
    '\n    Build a new H2OAssembly.\n\n    :param steps: A list of steps that sequentially transforms the input data.\n    :return: An H2OFrame.\n    '
    steps.append(('dummy', DummyEstimator()))
    super(H2OAssembly, self).__init__(steps)
    self.fuzed = []
    self.in_colnames = None
    self.out_colnames = None
