def __init__(self, steps):
    u'Build a new H2OAssembly.\n\n        Parameters\n        ----------\n        steps : list\n          A list of steps that sequentially transforms the input data.\n\n        Returns\n        -------\n          H2OFrame\n        '
    self.id = None
    self.steps = steps
    self.fuzed = []
    self.in_colnames = None
    self.out_colnames = None
