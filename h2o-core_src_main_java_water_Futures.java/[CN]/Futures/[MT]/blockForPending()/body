{
  try {
    while (true) {
      Future f;
synchronized (this) {
        if (_pending_cnt == 0)         return;
        f=_pending[--_pending_cnt];
      }
      f.get();
    }
  }
 catch (  ExecutionException e) {
    throw Log.throwErr(new RuntimeException(e.getCause()));
  }
catch (  InterruptedException e) {
    throw Log.throwErr(e);
  }
}
