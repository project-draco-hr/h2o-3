{
  if (colIdx < _ncols) {
    if (ParseTime.isDateTime(str)) {
      ++_ndates[colIdx];
      return;
    }
    if (ParseTime.isUUID(str)) {
      ++_nUUID[colIdx];
      return;
    }
    ++_nstrings[colIdx];
    _domains[colIdx].add(str.toString());
    if (_nlines < MAX_PREVIEW_LINES)     _data[_nlines][colIdx]=str.toString();
  }
}
