{
  super.modifyParmsForCrossValidationMainModel(N);
  if (_parms._overwrite_with_best_model) {
    if (!_parms._quiet_mode)     warn("_overwrite_with_best_model","Disabling overwrite_with_best_model for cross-validation main model: No early stopping.");
    _parms._overwrite_with_best_model=false;
  }
}
