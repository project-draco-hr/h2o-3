{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < domains.length; i++) {
    if (domains[i] == null)     sb.append(String.format(format,cols[i]));
 else {
      for (int j=0; j < domains[i].length; j++)       sb.append(String.format(format,domains[i][j]));
    }
  }
  sb.append("\n");
  return sb.toString();
}
