{
  if (_nums == 0)   return;
  assert(in.length == out.length);
  assert(in.length == fullN());
  for (int k=numStart(); k < fullN(); ++k) {
    double m=_normMul == null ? 1 : _normMul[k - numStart()];
    double s=_normSub == null ? 0 : _normSub[k - numStart()];
    out[k]=(float)(in[k] / m + s);
  }
}
