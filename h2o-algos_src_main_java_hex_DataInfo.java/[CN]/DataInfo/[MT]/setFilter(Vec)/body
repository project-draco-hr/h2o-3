{
  if (_filterVec) {
    int id=_adaptedFrame.numCols() - 1 - (_weightsVec ? 1 : 0);
    _adaptedFrame.replace(id,v);
  }
 else {
    String name="_filter_vec";
    while (_adaptedFrame.vec(name) != null)     name="_" + name;
    _adaptedFrame.add(name,v);
    _filterVec=true;
  }
  return this;
}
