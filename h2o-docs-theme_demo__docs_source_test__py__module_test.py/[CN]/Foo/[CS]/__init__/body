def __init__(self, qux, spam=False):
    'Start the Foo.\n\n        :param qux: The first argument to initialize class.\n        :type qux: string\n        :param spam: Spam me yes or no...\n        :type spam: bool\n\n        '
    self.qux = 3
    self.spam = 4
    'Docstring for instance attribute spam.'
