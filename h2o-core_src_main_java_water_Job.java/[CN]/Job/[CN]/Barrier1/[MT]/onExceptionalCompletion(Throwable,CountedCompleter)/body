{
  if (Job.isCancelledException(ex)) {
    new Barrier1OnCom().apply(Job.this);
    _barrier=null;
  }
 else {
    final DException dex=new DException(ex,caller.getClass());
    try {
      Log.err(ex);
    }
 catch (    Throwable t) {
    }
    new Barrier1OnExCom(dex).apply(Job.this);
  }
  _barrier=null;
  return true;
}
