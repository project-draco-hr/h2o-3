{
  if (newworked > 0 || (msg != null && !msg.equals(_msg))) {
    new JAtomic(this){
      @Override boolean abort(      Job job){
        return newworked == 0 && ((msg == null && _msg == null) || (msg != null && msg.equals(job._msg)));
      }
      @Override void update(      Job old){
        old._worked+=newworked;
        old._msg=msg;
      }
    }
;
  }
}
