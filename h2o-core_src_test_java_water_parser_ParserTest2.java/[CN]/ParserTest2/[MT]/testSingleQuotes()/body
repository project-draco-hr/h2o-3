{
  String[] data=new String[]{"Tomas's,test\n'Tomas''s,test2',test2\nlast,'line''","s, trailing, piece'"};
  String[][] expectFalse=new String[][]{ar("Tomas's","test",null,null),ar("'Tomas''s","test2'","test2",null),ar("last","'line''s","trailing","piece'")};
  Key k=ParserTest.makeByteVec(data);
  ParseSetupHandler gSetupF=ParseSetupHandler.guessSetup(data[0].getBytes(),ParserType.CSV,(byte)',',4,false,-1,null);
  Frame frF=ParseDataset2.parse(Key.make(),new Key[]{k},false,gSetupF);
  testParsed(frF,expectFalse);
  String[][] expectTrue=new String[][]{ar("Tomas's,test",null),ar("Tomasstest2","test2"),ar("last","lines trailing piece")};
  ParseSetupHandler gSetupT=ParseSetupHandler.guessSetup(data[0].getBytes(),ParserType.CSV,(byte)',',2,true,-1,null);
  Frame frT=ParseDataset2.parse(Key.make(),new Key[]{k},true,gSetupT);
  frT.delete();
}
