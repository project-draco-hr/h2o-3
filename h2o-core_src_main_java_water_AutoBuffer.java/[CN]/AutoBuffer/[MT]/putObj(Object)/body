{
  if (obj == null)   return putA1(null);
  try {
    ByteArrayOutputStream bos=new ByteArrayOutputStream();
    ObjectOutputStream oos=new ObjectOutputStream(bos);
    oos.writeObject(obj);
    byte[] ba=bos.toByteArray();
    oos.close();
    bos.close();
    return putA1(ba);
  }
 catch (  IOException e) {
    throw Log.throwErr(e);
  }
}
