{
  if (null == key)   throw new H2OIllegalArgumentException(param_name,"Models.getFromDKV()",key);
  Value v=DKV.get(key);
  if (null == v)   throw new H2OKeyNotFoundArgumentException(param_name,key.toString());
  Iced ice=v.get();
  if (!(ice instanceof Model))   throw new H2OKeyWrongTypeArgumentException(param_name,key.toString(),Model.class,ice.getClass());
  return (Model)ice;
}
