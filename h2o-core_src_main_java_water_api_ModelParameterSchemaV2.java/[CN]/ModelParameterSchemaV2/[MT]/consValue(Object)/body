{
  if (null == o)   return null;
  if (!o.getClass().isArray())   return o.toString();
  StringBuilder sb=new StringBuilder();
  sb.append("[");
  for (int i=0; i < Array.getLength(o); i++) {
    if (i > 0)     sb.append(", ");
    sb.append(consValue(Array.get(o,i)));
  }
  sb.append("]");
  return sb.toString();
}
