{
  RPC<TaskGetKey> rpc;
  while (true) {
    rpc=TGKS.get(key);
    if (rpc != null && rpc._dt._priority >= priority)     return rpc;
    final RPC<TaskGetKey> old=rpc;
    rpc=new RPC(target,new TaskGetKey(key,priority),1.0f);
    if (TGKS.putIfMatchUnlocked(key,rpc,old) == old) {
      rpc.setTaskNum().call();
      return rpc;
    }
  }
}
