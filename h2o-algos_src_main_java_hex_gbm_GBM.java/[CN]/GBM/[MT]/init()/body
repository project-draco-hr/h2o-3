{
  super.init();
  if (!(0. < _parms._learn_rate && _parms._learn_rate <= 1.0))   error("learn_rate","learn_rate must be between 0 and 1");
  if (_parms._loss == GBMModel.GBMParameters.Family.bernoulli && _nclass != 2)   error("loss","Bernoulli requires the response to be a 2-class categorical");
}
