{
  while (!isDone()) {
    try {
      if (_target == H2O.SELF) {
        _dt.get();
        _done=true;
      }
 else {
        wait(1000);
      }
    }
 catch (    InterruptedException ignore) {
    }
catch (    ExecutionException e) {
      _dt.setException(e.getCause());
      _done=true;
      break;
    }
  }
  return true;
}
