{
  if (!START_TIME_MILLIS.compareAndSet(0L,System.currentTimeMillis()))   return;
  parseArguments(args);
  String ice=DEFAULT_ICE_ROOT();
  if (ARGS.ice_root != null)   ice=ARGS.ice_root.replace("\\","/");
  try {
    ICE_ROOT=new URI(ice);
  }
 catch (  URISyntaxException ex) {
    throw new RuntimeException("Invalid ice_root: " + ice + ", "+ ex.getMessage());
  }
  printAndLogVersion();
  if (ARGS.version) {
    exit(0);
  }
  if (ARGS.help) {
    printHelp();
    exit(0);
  }
  NetworkInit.findInetAddressForSelf();
  startLocalNode();
  try {
    String logDir=Log.getLogDir();
    Log.info("Log dir: '" + logDir + "'");
  }
 catch (  Exception e) {
    System.err.println("ERROR: Log.getLogDir() failed, exiting now.");
    e.printStackTrace();
    H2O.exit(1);
  }
  initializePersistence();
  startNetworkServices();
}
