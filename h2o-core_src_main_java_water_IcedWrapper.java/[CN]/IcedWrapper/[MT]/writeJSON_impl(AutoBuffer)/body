{
  ab.putJSONStr("value").put1(':');
  if (Byte.class == t)   return ab.putJSON1((byte)i);
 else   if (Integer.class == t)   return ab.putJSON4(i);
 else   if (Long.class == t)   return ab.putJSON8(l);
 else   if (Float.class == t)   return ab.putJSON4f(f);
 else   if (Double.class == t)   return ab.putJSON8d(d);
 else   if (Boolean.class == t)   return ab.putJSONStrUnquoted(b ? "true" : "false");
 else   if (String.class == t)   return ab.putJSONName(s);
 else   if (Enum.class == t)   return ab.putJSONName(e);
  throw H2O.fail("Unhandled type: " + t);
}
