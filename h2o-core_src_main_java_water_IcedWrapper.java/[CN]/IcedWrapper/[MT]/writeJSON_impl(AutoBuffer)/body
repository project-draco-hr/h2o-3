{
  ab.putJSONStr("value").put1(':');
  if (is_array) {
    if (Byte.TYPE == t)     return ab.putJSONA4(i_ar);
 else     if (Integer.TYPE == t)     return ab.putJSONA4(i_ar);
 else     if (Long.TYPE == t)     return ab.putJSONA8(l_ar);
 else     if (Float.TYPE == t)     return ab.putJSONA4f(f_ar);
 else     if (Double.TYPE == t)     return ab.putJSONA8d(d_ar);
 else     if (Boolean.TYPE == t)     return ab.putJSONAStr(null);
 else     if (String.class == t)     return ab.putJSONAStr(s_ar);
 else     if (Enum.class == t)     return ab.putJSONAStr(e_ar);
  }
 else {
    if (Byte.TYPE == t)     return ab.putJSON1((byte)i);
 else     if (Integer.TYPE == t)     return ab.putJSON4(i);
 else     if (Long.TYPE == t)     return ab.putJSON8(l);
 else     if (Float.TYPE == t)     return ab.putJSON4f(f);
 else     if (Double.TYPE == t)     return ab.putJSON8d(d);
 else     if (Boolean.TYPE == t)     return ab.putJSONStrUnquoted(b ? "true" : "false");
 else     if (String.class == t)     return ab.putJSONName(s);
 else     if (Enum.class == t)     return ab.putJSONName(e);
  }
  throw H2O.fail("Unhandled type: " + t);
}
