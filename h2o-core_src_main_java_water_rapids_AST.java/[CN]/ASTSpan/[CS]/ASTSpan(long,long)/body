{
  _ast_min=new ASTNum(min);
  _ast_max=new ASTNum(max);
  _min=min;
  _max=max;
  if (_min < 0 && _max < 0) {
    if (_max > _min)     throw new IllegalArgumentException("max>min: All negative, incorrect order.");
  }
 else {
    if (_min > _max)     throw new IllegalArgumentException("min > max: min <= max for `:` operator.");
  }
}
