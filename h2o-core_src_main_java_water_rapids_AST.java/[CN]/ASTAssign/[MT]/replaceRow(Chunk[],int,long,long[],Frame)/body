{
  for (int c=0; c < cols.length; ++c) {
    int col=(int)cols[c];
    if (cs[c].vec().isEnum()) {
      if (ary.vecs()[col].isEnum()) {
        ary.vecs()[col].set(row_id,cs[c].atd(row0));
      }
 else {
        ary.vecs()[col].set(row_id,Double.NaN);
      }
    }
    if (cs[c].vec().isNumeric()) {
      if (ary.vecs()[col].isNumeric()) {
        ary.vecs()[col].set(row_id,cs[c].atd(row0));
      }
 else {
        ary.vecs()[col].set(row_id,Double.NaN);
      }
    }
  }
}
