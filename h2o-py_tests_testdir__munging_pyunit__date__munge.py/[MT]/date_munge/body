def date_munge(ip, port):
    crimes_path = h2o.locate('smalldata/chicago/chicagoCrimes10k.csv.zip')
    crimes = h2o.import_frame(path=crimes_path)
    crimes.describe()
    refine_date_col(crimes, 'Date', '%m/%d/%Y %I:%M:%S %p')
    crimes = crimes.drop('Date')
    crimes.describe()
    crimes.show()
