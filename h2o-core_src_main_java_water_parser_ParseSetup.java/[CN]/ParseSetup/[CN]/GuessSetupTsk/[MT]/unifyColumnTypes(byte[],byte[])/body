{
  if (typesA == null)   return typesB;
 else   if (typesB == null)   return typesA;
 else {
    for (int i=0; i < typesA.length; i++) {
      if (i > typesB.length || typesA[i] != typesB[i]) {
        if (typesA[i] == Vec.T_BAD)         typesA[i]=typesB[i];
 else         throw new H2OParseSetupException("Column types do not match between files.");
      }
    }
    return typesA;
  }
}
