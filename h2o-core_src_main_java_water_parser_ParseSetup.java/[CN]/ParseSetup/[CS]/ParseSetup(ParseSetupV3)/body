{
  this(ps.parse_type,ps.separator,ps.single_quotes,ps.check_header,GUESS_COL_CNT,ps.column_names,strToColumnTypes(ps.column_types),null,ps.na_strings,null,new ParseWriter.ParseErr[0],ps.chunk_size);
  if (ps.parse_type == null)   _parse_type=ParserType.GUESS;
  if (ps.separator == 0)   _separator=GUESS_SEP;
}
