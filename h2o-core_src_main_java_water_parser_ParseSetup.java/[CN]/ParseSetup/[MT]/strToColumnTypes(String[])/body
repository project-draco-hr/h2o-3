{
  if (strs == null)   return null;
  byte[] types=new byte[strs.length];
  for (int i=0; i < types.length; i++) {
switch (strs[i].toLowerCase()) {
case "unknown":
      types[i]=Vec.T_BAD;
    break;
case "uuid":
  types[i]=Vec.T_UUID;
break;
case "string":
types[i]=Vec.T_STR;
break;
case "numeric":
types[i]=Vec.T_NUM;
break;
case "enum":
types[i]=Vec.T_ENUM;
break;
case "time":
types[i]=Vec.T_TIME;
break;
default :
types[i]=Vec.T_BAD;
Log.err("Column type " + strs[i] + " is unknown.");
}
}
return types;
}
