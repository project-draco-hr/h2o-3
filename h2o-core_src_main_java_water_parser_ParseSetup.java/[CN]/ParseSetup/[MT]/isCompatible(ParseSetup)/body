{
  if ((_parse_type != other._parse_type) && !(_parse_type == ParserType.ARFF && other._parse_type == ParserType.CSV) && !(_parse_type == ParserType.CSV && other._parse_type == ParserType.ARFF))   return false;
  if (_separator != other._separator && (other._separator != AUTO_SEP || _separator != AUTO_SEP))   return false;
  if (_number_columns != other._number_columns && (other._number_columns > 0 && _number_columns > 0))   return false;
  return true;
}
