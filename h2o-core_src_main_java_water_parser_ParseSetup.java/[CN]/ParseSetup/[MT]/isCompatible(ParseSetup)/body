{
  if (other == null || _pType != other._pType)   return false;
  if (_pType == ParserType.CSV && (_sep != other._sep || _ncols != other._ncols))   return false;
  if (_ctypes == null)   _ctypes=other._ctypes;
 else   if (other._ctypes != null) {
    for (int i=0; i < _ctypes.length; ++i)     _ctypes[i].merge(other._ctypes[i]);
  }
  return true;
}
