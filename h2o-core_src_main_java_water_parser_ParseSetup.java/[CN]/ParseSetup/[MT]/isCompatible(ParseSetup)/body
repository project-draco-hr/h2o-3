{
  if ((_parse_type != other._parse_type) && !(_parse_type == ParserType.ARFF && other._parse_type == ParserType.CSV) && !(_parse_type == ParserType.CSV && other._parse_type == ParserType.ARFF))   throw new H2OParseSetupException("Cannot parse files of type " + _parse_type + " and "+ other._parse_type+ " as one dataset","File type mismatch: " + _parse_type + ", "+ other._parse_type);
  if (other._separator != _separator && other._separator != GUESS_SEP)   return false;
  return _number_columns == other._number_columns || other._number_columns == 0 || _number_columns == 0;
}
