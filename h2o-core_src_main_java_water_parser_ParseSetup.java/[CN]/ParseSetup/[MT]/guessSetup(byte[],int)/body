{
  assert _isValid;
  ParseSetup ps=guessSetup(bits,_singleQuotes,checkHeader);
  if (!ps._isValid)   return ps;
  if (_pType != ps._pType || (_pType == ParserType.CSV && (_sep != ps._sep || _ncols != ps._ncols)))   return new ParseSetup(ps,"Conflicting file layouts, expecting: " + this + " but found "+ ps+ "\n");
  return ps;
}
