def legalFunction(function):
    if (function in xFcnOp1Set):
        return 1
    if (function in xFcnOp2Set):
        return 2
    if (function in xFcnOp3Set):
        return 3
    if (function in xFcnOpBinSet):
        return 2
    if (function in xFcnUser):
        return 3
    else:
        return 0
