def __str__(self):
    if self.assignDisable:
        return ('%s' % self.rhs)
    else:
        lhsAssign = re.sub('^\\$', '', str(self.lhs))
        if (isinstance(self.lhs, (Key, basestring)) and (not isinstance(self.lhs, KeyIndexed))):
            lhsprefix = '!'
        else:
            lhsprefix = ''
        return ('(= %s%s %s)' % (lhsprefix, lhsAssign, self.rhs))
