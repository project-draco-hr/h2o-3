def __init__(self):
    self.refcnt = 0
    self.assignDisable = True
    self.funs = False
    self.execExpr = None
    self.execDone = False
    self.assignDone = False
    self.ilshiftDone = False
    self.execResult = None
    self.result = None
    self.scalar = None
    self.function = None
    self.rhs = None
    self.lhs = None
