{
  for (; i < bytes.length; i++) {
    byte b=bytes[i];
switch (b) {
case '%':
      sb.append("%%");
    break;
case '.':
  sb.append("%d");
break;
case '/':
sb.append("%s");
break;
case ':':
sb.append("%c");
break;
case '\\':
sb.append("%b");
break;
case '"':
sb.append("%q");
break;
case '\0':
sb.append("%z");
break;
default :
sb.append((char)b);
break;
}
}
return sb;
}
