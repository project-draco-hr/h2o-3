{
  while (rs != null && rs.isComputing()) {
    rs._tsk.join();
    oldValue=DKV.get(_rsKey);
    rs=oldValue == null ? null : oldValue.<RollupStats>get();
  }
  if (rs != null && rs.isMutating())   throw new IllegalArgumentException("Can not compute rollup stats while vec is being changed.");
  if (rs == null)   rs=RollupStats.makeComputing(_rsKey);
  _rs=rs;
  computeRollups(_vecKey,_rsKey,oldValue);
}
