{
  for (int rows=0; rows < chks[0]._len; rows++)   for (int cols=0; cols < chks.length >> 1; cols++) {
    double d0=chks[cols].at0(rows);
    double d1=chks[cols + (chks.length >> 1)].at0(rows);
    if (!(Double.isNaN(d0) && Double.isNaN(d1)) && (d0 != d1)) {
      _ok=false;
      return;
    }
  }
}
