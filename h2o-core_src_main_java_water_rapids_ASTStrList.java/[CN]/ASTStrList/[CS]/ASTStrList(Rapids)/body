{
  ArrayList<String> strs=new ArrayList<>();
  while (true) {
    char c=e.skipWS();
    if (c == ']')     break;
    if (e.isQuote(c))     strs.add(e.match(c));
 else     throw new IllegalArgumentException("Expecting the start of a string");
  }
  e.xpeek(']');
  _strs=strs.toArray(new String[strs.size()]);
}
