{
  String pid="-1";
  try {
    pid=getProcessId();
    _pid=pid;
  }
 catch (  Exception xe) {
  }
  File f=new File("/proc/stat");
  if (!f.exists()) {
    return;
  }
  try {
    readSystemProcFile();
    readProcessProcFile(pid);
    readProcessNumOpenFds(pid);
    readProcessStatusFile(pid);
    parseSystemProcFile(_systemData);
    parseProcessProcFile(_processData);
    parseProcessStatusFile(_processStatus);
  }
 catch (  Exception xe) {
  }
}
