def __init__(self, ip='localhost', port=54321):
    '\n        Wipes out the current __H2OCONN__, expects __make__ to be called in conjunction.\n        :return: None\n        '
    global __H2OCONN__
    self._ip = None
    self._port = None
    self._session_id = None
    self._rest_version = __H2O_REST_API_VERSION__
    __H2OCONN__ = self
    H2OConnectionBase.__make__(ip, port)
