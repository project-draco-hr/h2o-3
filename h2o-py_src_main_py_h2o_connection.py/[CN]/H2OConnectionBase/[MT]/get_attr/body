@staticmethod
def get_attr(name):
    '\n        A helper function for static methods\n            rest_version, session_id, port, ip\n        :param name: The attribute to lookup in __H2OCONN__\n        :return: The desired attribute\n        '
    global __H2OCONN__
    H2OConnectionBase.check_conn()
    return getattr(__H2OCONN__, name)
