{
  if (_chks == r._chks && _row == r._row)   return true;
  int len=_chks.length;
  for (int c=0; c < len; ++c) {
    boolean lb=_chks[c].isNA(_row), rb=r._chks[c].isNA(r._row);
    if (lb && rb)     continue;
    if (lb || rb)     return false;
    if (_chks[c].atd(_row) != r._chks[c].atd(r._row))     return false;
  }
  return true;
}
