{
  final NonBlockingHashSet<Row> rows=_rows;
  final int len=chks[0]._len;
  Row row=new Row(_ncols);
  for (int i=0; i < len; i++) {
    boolean added=_rows.add(row.fill(chks,_id_maps,i));
    if (!added) {
      if (_allRite)       row._dup=true;
    }
 else {
      row=new Row(_ncols);
    }
  }
}
