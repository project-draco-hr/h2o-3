def xAssign(lhs='xResult', rhs='xTemp'):
    print ('h2o_exec xAssign: %s' % rhs)
    rhs = xItem(rhs)
    print ('h2o_exec xAssign after xItem: %s' % rhs)
    if re.match('\\$', lhs):
        raise Exception(("h2o_exec xAssign: lhs can't start with '$' %s" % frame))
    return ('(= !%s %s)' % (lhs, rhs))
