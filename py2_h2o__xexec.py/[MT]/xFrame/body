def xFrame(frame='xTemp', row=None, col=None):
    xNumToString(row)
    xNumToString(col)
    if re.match('\\$', frame):
        raise Exception(("xExec xFrame adds '$': frame ref shouldn't start with '$' %s" % frame))
    if (row is None):
        row = '"null"'
    if (col is None):
        col = '"null"'
    return ('([ $%s %s %s)' % (frame, row, col))
