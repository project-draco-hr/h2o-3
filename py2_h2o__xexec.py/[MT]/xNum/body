def xNum(number):
    if isinstance(number, (list, tuple)):
        raise Exception(("xexec xNum doesn't take lists or tuples %s" % number))
    elif isinstance(number, basestring):
        if re.match('#', number):
            return number
    try:
        junk = (1.0 + number)
        return ('#%s' % number)
    except:
        return number
