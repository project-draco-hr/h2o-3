def xNum(item):
    if isinstance(item, (list, tuple)):
        raise Exception(("h2o_exec xNum doesn't take lists or tuples %s" % item))
    elif isinstance(item, basestring):
        if re.match('#', item):
            return item
    try:
        junk = float(item)
        return ('#%s' % item)
    except:
        return item
