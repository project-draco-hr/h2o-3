def xTranslateTextValue(text='F'):
    translate = {'T': '$TRUE', 'F': '$FALSE', 'TRUE': '$TRUE', 'FALSE': '$FALSE', 'True': '$TRUE', 'False': '$FALSE', 'true': '$TRUE', 'false': '$FALSE', 'NULL': '"NULL"', 'null': '"NULL"', }
    if (text is None):
        text = '"NULL"'
    elif (text in translate):
        text = translate[text]
    else:
        pass
    return text
