def __str__(self):
    key = self.key
    if isinstance(key, (list, tuple)):
        raise Exception(("Key doesn't take lists or tuples %s" % key))
    if (not isinstance(key, basestring)):
        raise Exception(('Key wants to be a string %s' % key))
    if re.match('\\$', key):
        return key
    else:
        return ('$%s' % key)
