def __init__(self, frame='xTemp', row=None, col=None):
    super(Frame, self).__init__()
    if re.match('\\$', frame):
        raise Exception(("Frame adds '$': frame ref shouldn't start with '$' %s" % frame))
    self.frame = frame
    if (row is None):
        row = '"null"'
    if (col is None):
        col = '"null"'
    self.row = Item(row)
    self.col = Item(col)
