{
  long nrows=rows.cnt();
  if (nrows == 1) {
    Vec[] vecs=ses.copyOnWrite(dst,cols);
    long drow=(long)rows.expand()[0];
    for (int i=0; i < cols.length; i++)     vecs[cols[i]].set(drow,src);
    return;
  }
  if (dst.numRows() == nrows && rows.isDense()) {
    Vec vsrc=dst.anyVec().makeCon(src);
    for (int i=0; i < cols.length; i++)     dst.replace(cols[i],vsrc);
    if (dst._key != null)     DKV.put(dst);
    return;
  }
  throw H2O.unimpl();
}
