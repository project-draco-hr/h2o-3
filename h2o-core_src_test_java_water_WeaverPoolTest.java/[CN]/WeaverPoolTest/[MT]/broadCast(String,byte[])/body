{
  new MRTask(){
    @Override public void setupLocal(){
      ClassPool cp=Weaver.getPool();
      cp.insertClassPath(new ByteArrayClassPath(name,b));
      try {
        cp.get(name).toClass();
      }
 catch (      CannotCompileException e) {
        e.printStackTrace();
      }
catch (      NotFoundException e) {
        e.printStackTrace();
      }
    }
  }
.doAllNodes();
}
