def __init__(self, **kwargs):
    super(H2ONaiveBayesEstimator, self).__init__()
    self._parms = {}
    for name in ['model_id', 'nfolds', 'seed', 'fold_assignment', 'fold_column', 'keep_cross_validation_predictions', 'keep_cross_validation_fold_assignment', 'training_frame', 'validation_frame', 'response_column', 'ignored_columns', 'ignore_const_cols', 'score_each_iteration', 'balance_classes', 'class_sampling_factors', 'max_after_balance_size', 'max_confusion_matrix_size', 'max_hit_ratio_k', 'laplace', 'min_sdev', 'eps_sdev', 'min_prob', 'eps_prob', 'compute_metrics', 'max_runtime_secs']:
        pname = (name[:(-1)] if (name[(-1)] == '_') else name)
        self._parms[pname] = (kwargs[name] if (name in kwargs) else None)
