{
  ByteArrayOutputStream baos=new ByteArrayOutputStream();
  ZipOutputStream zos=new ZipOutputStream(baos);
  try {
    zipDir(Log.LOG_DIR,baos,zos);
    zos.close();
    baos.close();
    _bytes=baos.toByteArray();
  }
 catch (  Exception e) {
    _bytes=e.toString().getBytes();
  }
 finally {
    tryComplete();
  }
}
