{
  int rows=cs[0]._len;
  int cols=cs.length;
  for (int r=0; r < rows; ++r)   for (int c=0; c < cols; ++c) {
    if (cs[c].vec().isEnum())     ncs[c].addNum(cs[c].at8(r),0);
 else {
      double denom=cs[c].atd(r) / (use_rms ? rms[c] : use_sig ? cs[c].vec().sigma() : scales == null ? 1 : scales[c]);
      ncs[c].addNum(denom);
    }
  }
}
