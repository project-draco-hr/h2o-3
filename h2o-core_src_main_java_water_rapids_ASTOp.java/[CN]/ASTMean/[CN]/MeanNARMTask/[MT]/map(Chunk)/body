{
  if (c.vec().isUUID()) {
    _sum=Double.NaN;
    _rowcnt=0;
    return;
  }
  if (_narm) {
    for (int r=0; r < c._len; r++)     if (!c.isNA(r)) {
      _sum+=c.atd(r);
      _rowcnt++;
    }
  }
 else {
    for (int r=0; r < c._len; r++)     if (c.isNA(r)) {
      _rowcnt=0;
      _sum=Double.NaN;
      return;
    }
 else {
      _sum+=c.atd(r);
      _rowcnt++;
    }
  }
}
