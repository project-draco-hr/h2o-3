{
  int rows=chks[0]._len;
  for (  Chunk C : chks) {
    double prod=_d;
    if (_narm)     for (int r=0; r < rows; r++) {
      double d=C.atd(r);
      if (!Double.isNaN(d))       prod*=d;
    }
 else     for (int r=0; r < rows; r++) {
      double d=C.atd(r);
      prod*=d;
    }
    _d=prod;
    if (Double.isNaN(prod))     break;
  }
}
