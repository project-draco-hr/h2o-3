{
  int rows=chks[0]._len;
  for (  Chunk C : chks) {
    double sum=_d;
    if (_narm)     for (int r=0; r < rows; r++) {
      double d=C.atd(r);
      if (!Double.isNaN(d))       sum+=d;
    }
 else     for (int r=0; r < rows; r++) {
      double d=C.atd(r);
      sum+=d;
    }
    _d=sum;
    if (Double.isNaN(sum))     break;
  }
}
