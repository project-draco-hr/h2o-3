{
  int rows=chks[0]._len;
  for (  Chunk C : chks) {
    if (C.vec().isEnum() || C.vec().isUUID() || C.vec().isString())     continue;
    double sum=0;
    for (int r=0; r < rows; r++) {
      double d=C.atd(r);
      if (!Double.isNaN(d))       sum+=d;
    }
    _d=sum;
    if (Double.isNaN(_d))     break;
  }
}
