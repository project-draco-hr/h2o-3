{
  Frame ary=env.popAry();
  if (ary.numCols() != 1)   throw new IllegalArgumentException("factor requires a single column");
  Vec v0=ary.anyVec();
  if (v0.isEnum()) {
    env.pushAry(ary);
    return;
  }
  Vec v1=v0.toEnum();
  Frame fr=new Frame(ary._names,new Vec[]{v1});
  env.pushAry(fr);
}
