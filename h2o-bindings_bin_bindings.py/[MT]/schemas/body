def schemas(full=False):
    u'\n    Return the list of H\u2082O schemas.\n      :param full: if True, then the complete response to .../schemas is returned (including the metadata)\n    '
    json = _request_or_exit(u'/LATEST/Metadata/schemas')
    if full:
        return json
    else:
        assert (u'schemas' in json), u'Unexpected result from /LATEST/Metadata/schemas call'
        return json[u'schemas']
