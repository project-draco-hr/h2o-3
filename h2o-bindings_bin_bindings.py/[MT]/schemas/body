def schemas(full=False):
    '\n    Return the list of H\xe2\x82\x82O schemas.\n      :param full: if True, then the complete response to .../schemas is returned (including the metadata)\n    '
    json = _request_or_exit('/LATEST/Metadata/schemas')
    if full:
        return json
    else:
        assert ('schemas' in json), 'Unexpected result from /LATEST/Metadata/schemas call'
        return json['schemas']
