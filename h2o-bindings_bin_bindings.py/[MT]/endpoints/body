def endpoints(full=False):
    '\n    Return the list of REST API endpoints.\n      :param full: if True, then the complete response to .../endpoints is returned (including the metadata)\n    '
    json = _request_or_exit('/LATEST/Metadata/endpoints')
    if full:
        return json
    else:
        assert ('routes' in json), 'Unexpected result from /LATEST/Metadata/endpoints call'
        return json['routes']
