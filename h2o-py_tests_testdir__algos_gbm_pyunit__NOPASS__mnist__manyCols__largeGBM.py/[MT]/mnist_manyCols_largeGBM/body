def mnist_manyCols_largeGBM(ip, port):
    h2o.init(ip, port)
    train = h2o.import_frame(path='bigdata/laptop/mnist/train.csv.gz')
    train.tail()
    tail = train.tail()
    gbm_mnist = h2o.gbm(x=train[0:784], y=train[784], ntrees=1, max_depth=1, min_rows=10, learn_rate=0.01)
    gbm_mnist.show()
