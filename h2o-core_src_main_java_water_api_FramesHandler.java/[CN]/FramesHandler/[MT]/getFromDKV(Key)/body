{
  if (null == key)   throw new IllegalArgumentException("Got null key.");
  Value v=DKV.get(key);
  if (null == v)   throw new IllegalArgumentException("Did not find key: " + key.toString());
  Iced ice=v.get();
  if (!(ice instanceof Frame))   throw new IllegalArgumentException("Expected a Frame for key: " + key.toString() + "; got a: "+ ice.getClass());
  return (Frame)ice;
}
