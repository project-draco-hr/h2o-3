{
  PersistManager pm=H2O.getPM();
  OutputStream os=null;
  try {
    os=pm.create(path,force);
    FileUtils.copyStream(csv,os,4 * 1024 * 1024);
  }
  finally {
    if (os != null) {
      try {
        os.close();
        Log.info("Key '" + frameName + "' was written to "+ path+ ".");
      }
 catch (      Exception e) {
        Log.err(e);
      }
    }
  }
}
