{
  int curIdx=0;
  try {
    byte[] bytes=new byte[buffer_size];
    for (; ; ) {
      int count=_csv.read(bytes,0,buffer_size);
      if (count <= 0)       break;
      os.write(bytes,0,count);
      int workDone=((Frame.CSVStream)_csv)._curChkIdx;
      if (curIdx != workDone) {
        _j.update(workDone - curIdx);
        curIdx=workDone;
      }
    }
  }
 catch (  Exception ex) {
    throw new RuntimeException(ex);
  }
}
