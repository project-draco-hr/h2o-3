{
  for (int r=0; r < rowHeaders.length; ++r) {
    for (int c=0; c < colNames.length; ++c) {
      if (this.strings[r] != null && this.strings[r][c] != null && this.doubles[r] != null && this.doubles[r][c] != null)       throw new IllegalArgumentException("Cannot provide both a String and a Double at row idx " + r + " and column idx "+ c+ ".");
    }
  }
}
