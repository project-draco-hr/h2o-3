{
  if (isEmpty(d))   cellValues[row][col]="";
 else {
switch (colTypes[col]) {
case "double":
case "float":
      cellValues[row][col]=Double.toString(d);
    break;
case "long":
case "int":
  cellValues[row][col]=Long.toString((long)d);
break;
default :
cellValues[row][col]=Double.toString(d);
break;
}
}
}
