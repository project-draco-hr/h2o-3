{
  new MRTask(){
    @Override public void setupLocal(){
      try {
        String cmd="/bin/kill -3 " + getProcessId();
        java.lang.Runtime.getRuntime().exec(cmd);
      }
 catch (      Exception xe) {
        xe.printStackTrace();
        throw new H2OIllegalArgumentException("");
      }
    }
  }
.doAllNodes();
  return u;
}
