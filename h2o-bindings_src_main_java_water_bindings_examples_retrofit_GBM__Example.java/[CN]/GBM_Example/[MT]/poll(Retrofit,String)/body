{
  Jobs jobsService=retrofit.create(Jobs.class);
  Response<JobsV3> jobsResponse=null;
  int retries=3;
  JobsV3 jobs=null;
  do {
    try {
      jobsResponse=jobsService.fetch(jobId).execute();
    }
 catch (    IOException e) {
      System.err.println("Caught exception: " + e);
    }
    if (jobsResponse == null || !jobsResponse.isSuccessful())     if (retries-- > 0)     continue;
 else     throw new RuntimeException("/3/Jobs/" + jobId + " failed 3 times.");
    jobs=jobsResponse.body();
    if (null == jobs.jobs || jobs.jobs.length != 1)     throw new RuntimeException("Failed to find Job: " + jobId);
    if (!"RUNNING".equals(jobs.jobs[0].status))     try {
      Thread.sleep(100);
    }
 catch (    InterruptedException e) {
    }
  }
 while ("RUNNING".equals(jobs.jobs[0].status));
  return jobs.jobs[0];
}
