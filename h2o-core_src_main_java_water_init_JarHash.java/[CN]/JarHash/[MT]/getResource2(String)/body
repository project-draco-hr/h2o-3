{
  try {
    if (JARPATH != null)     return ClassLoader.getSystemClassLoader().getResourceAsStream("www" + uri);
    for (    File f : RESOURCE_FILES) {
      File f2=new File(f,uri);
      if (f2.exists())       return new FileInputStream(f2);
    }
  }
 catch (  FileNotFoundException ignore) {
  }
  Log.info("Resource not found: " + uri);
  return null;
}
