{
  try {
    if (JARPATH != null)     return ClassLoader.getSystemClassLoader().getResourceAsStream("www" + uri);
    File f=new File(System.getProperty("user.dir") + "/h2o-client/src/main/resources/www",uri);
    if (f.exists())     return new FileInputStream(f);
    File resources=new File("src/main/resources/www");
    if (resources.exists())     return new FileInputStream(new File(resources,uri));
  }
 catch (  FileNotFoundException ignore) {
  }
  Log.info("Resource not found: " + uri);
  return null;
}
