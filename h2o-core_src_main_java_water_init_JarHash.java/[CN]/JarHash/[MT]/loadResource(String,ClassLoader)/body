{
  Log.info("Trying to load resource " + uri + " via classloader "+ cl);
  InputStream is=null;
  is=cl.getResourceAsStream("resources/www" + uri);
  if (is == null) {
    is=cl.getResourceAsStream("resources/main/www" + uri);
    if (is == null) {
      is=cl.getResourceAsStream("www" + uri);
    }
  }
  return is;
}
