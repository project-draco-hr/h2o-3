{
  StringBuilder sb=new StringBuilder();
  int idx=start + sz;
  try {
    for (int i=5; i > 0; --i)     sb.append("-" + i + ": "+ (0xFF & bb.get(idx - i))+ " ");
    sb.append("0: " + (0xFF & bb.get(idx)));
    for (int i=1; i <= 5; ++i)     sb.append("+" + i + ": "+ (0xFF & bb.get(idx + i))+ " ");
  }
 catch (  Throwable t) {
  }
  return sb.toString();
}
