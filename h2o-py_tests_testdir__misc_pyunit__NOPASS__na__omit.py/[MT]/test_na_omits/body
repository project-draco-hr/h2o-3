def test_na_omits():
    hf = h2o.H2OFrame({'A': [1, 'NA', 2], 'B': [1, 2, 3], 'C': [4, 5, 6], })
    hf.summary()
    hf_col_summary = h2o.H2OConnection.get_json((('Frames/' + urllib.parse.quote(hf.frame_id)) + '/summary'))['frames'][0]['columns']
    hf_col_summary = sum([e['missing_count'] for e in hf_col_summary])
    assert (hf_col_summary == 1)
    hf.na_omit()
    hf.summary()
    hf_col_summary = h2o.H2OConnection.get_json((('Frames/' + urllib.parse.quote(hf.frame_id)) + '/summary'))['frames'][0]['columns']
    hf_col_summary = sum([e['missing_count'] for e in hf_col_summary])
    assert (hf_col_summary == 0)
