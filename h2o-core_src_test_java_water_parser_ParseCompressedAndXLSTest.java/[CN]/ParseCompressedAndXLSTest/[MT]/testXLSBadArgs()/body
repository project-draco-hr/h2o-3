{
  Frame k1=null;
  try {
    File f=find_test_file("smalldata/airlines/AirlinesTest.csv.zip");
    NFSFileVec nfs=NFSFileVec.make(f);
    byte[] ctypes=new byte[12];
    for (int i=0; i < 12; i++)     ctypes[i]=Vec.T_NUM;
    ParseSetup setup=new ParseSetup(ParserType.XLS,(byte)52,true,ParseSetup.NO_HEADER,12,new String[]{"fYear","fMonth","fDayofMonth","fDayOfWeek","DepTime","ArrTime","UniqueCarrier","Origin","Dest","Distance","IsDepDelayed","IsDepDelayed_REC"},ctypes,null,null,null);
    try {
      k1=ParseDataset.parse(Key.make(),new Key[]{nfs._key},true,setup,true)._job.get();
      assertTrue("Should have thrown ParseException since file isn't XLS file",false);
      k1.delete();
    }
 catch (    Throwable t) {
      assertTrue(t.getMessage().contains("H2OParseException"));
    }
  }
  finally {
    if (k1 != null)     k1.delete();
  }
}
