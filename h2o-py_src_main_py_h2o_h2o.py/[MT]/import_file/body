def import_file(path):
    '\n    Import a single file.\n    :param path: A path to a data file (remote or local)\n    :return: Return an H2OFrame.\n    '
    j = h2oConn.do_safe_get_json(url_suffix='ImportFiles', params={'path': path, })
    if j['fails']:
        raise ValueError(((('ImportFiles of ' + path) + ' failed on ') + j['fails']))
    return j['keys'][0]
