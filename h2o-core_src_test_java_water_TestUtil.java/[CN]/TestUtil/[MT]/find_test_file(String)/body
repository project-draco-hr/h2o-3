{
  File file=new File(fname);
  if (!file.exists())   file=new File("target/" + fname);
  if (!file.exists())   file=new File("../" + fname);
  if (!file.exists())   file=new File("../../" + fname);
  if (!file.exists())   file=new File("../target/" + fname);
  if (!file.exists())   file=null;
  return file;
}
