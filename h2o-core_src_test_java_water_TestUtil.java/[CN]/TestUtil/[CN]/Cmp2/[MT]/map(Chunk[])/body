{
  for (int cols=0; cols < chks.length >> 1; cols++) {
    if (_unequal)     return;
    Chunk c0=chks[cols];
    Vec v1=_fr.vecs()[cols];
    for (int rows=0; rows < chks[0]._len; rows++) {
      double d0=c0.atd(rows), d1=v1.at(c0.start() + rows);
      if (!(Double.isNaN(d0) && Double.isNaN(d1)) && (d0 != d1)) {
        _unequal=true;
        return;
      }
    }
  }
}
