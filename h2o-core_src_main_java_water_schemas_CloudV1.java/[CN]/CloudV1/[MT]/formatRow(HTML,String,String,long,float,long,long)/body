{
  ab.p("<tr").p(color).p(">");
  ab.cell(name).cell(PrettyPrint.msecs(ping,true)).cell(String.format("%4.3f",load));
  int data_perc=total_data == 0 ? 100 : (int)(mem_data * 100 / total_data);
  ab.cell(PrettyPrint.bytes(total_data) + (total_data == 0 ? "" : " (" + data_perc + "%)"));
  return ab.p("</tr>");
}
