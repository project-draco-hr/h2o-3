def to_rest(self, step_name):
    ast = self._transform_helper(self._dummy_frame())._ast._debug_print(pprint=False)
    new_col_names = self.new_col_name
    if (new_col_names is None):
        new_col_names = ['|']
    elif (not isinstance(new_col_names, (list, tuple))):
        new_col_names = [new_col_names]
    return super(H2OColOp, self).to_rest([step_name, 'H2OColOp', ast, self.inplace, '|'.join(new_col_names)])
