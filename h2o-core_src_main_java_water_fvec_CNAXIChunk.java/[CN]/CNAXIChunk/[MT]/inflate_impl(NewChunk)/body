{
  nc.setSparseNA();
  nc.set_len(_len);
  nc.set_sparseLen(_sparseLen);
  nc.alloc_mantissa(_sparseLen);
  nc.alloc_exponent(_sparseLen);
  nc.alloc_indices(_sparseLen);
  int off=_OFF;
  for (int i=0; i < _sparseLen; ++i, off+=ridsz() + valsz()) {
    nc.indices()[i]=getId(off);
    long v=getIValue(off);
    nc.mantissa()[i]=v;
  }
  return nc;
}
