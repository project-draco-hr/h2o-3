{
  String s=a.opStr();
  if (s.equals("\"") || s.equals("\'"))   _x--;
  int len=s.length();
  int idx=len - 1;
  while (true) {
    if (_x <= 0) {
      _x=0;
      break;
    }
    if (idx < 0)     break;
    if (ppeek() != s.charAt(idx)) {
      _x--;
      idx=len - 1;
      continue;
    }
    idx--;
    _x--;
  }
  return this;
}
