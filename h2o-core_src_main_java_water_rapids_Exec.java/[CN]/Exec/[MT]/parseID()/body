{
  StringBuilder sb=new StringBuilder();
  if (peek() == '(') {
    _x++;
    skipWS();
    return parseID();
  }
  if (isSpecial(peek())) {
    return sb.append((char)_ast[_x++]).toString();
  }
  while (_x < _ast.length && _ast[_x] != ' ' && _ast[_x] != ')' && _ast[_x] != ';') {
    sb.append((char)_ast[_x++]);
  }
  _x++;
  return sb.toString();
}
