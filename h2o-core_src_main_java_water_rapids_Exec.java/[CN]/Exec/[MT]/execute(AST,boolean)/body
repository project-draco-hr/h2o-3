{
  Env env=new Env();
  Val val=ast.exec(env);
  if (deepCopy && val.isFrame()) {
    Frame fr=val.getFrame();
    if (fr._key != null)     val=new ValFrame(fr=new Frame(null,fr.names(),fr.vecs()));
    Vec vecs[]=fr.vecs();
    for (int i=0; i < vecs.length; i++)     if (env.isPreExistingGlobal(vecs[i]))     fr.replace(i,vecs[i].makeCopy());
  }
  return val;
}
