def _update_progress(self):
    if self._100_percent:
        return
    progress = min(self.progress, 1)
    if (progress == 1):
        self._100_percent = True
    if H2OJob.__PROGRESS_BAR__:
        p = int((self._progress_bar_width * progress))
        sys.stdout.write(((u'\r' + self._job_type) + (u' Progress: [%s%s] %02d%%' % ((u'#' * p), (u' ' * (self._progress_bar_width - p)), (100 * progress)))))
        sys.stdout.flush()
