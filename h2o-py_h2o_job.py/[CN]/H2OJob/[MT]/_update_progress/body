def _update_progress(self):
    if self._100_percent:
        return
    progress = min(self.progress, 1)
    if (progress == 1):
        self._100_percent = True
    if (__PROGRESS_BAR__ or self._100_percent):
        p = int((self._progress_bar_width * progress))
        sys.stdout.write((('\r' + self._job_type) + (' Progress: [%s%s] %02d%%' % (('#' * p), (' ' * (self._progress_bar_width - p)), (100 * progress)))))
        sys.stdout.flush()
