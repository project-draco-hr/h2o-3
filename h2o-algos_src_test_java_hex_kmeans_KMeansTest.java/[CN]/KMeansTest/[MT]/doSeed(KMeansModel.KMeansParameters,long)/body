{
  parms._seed=seed;
  KMeans job=null;
  KMeansModel kmm=null;
  try {
    job=new KMeans(parms);
    job.train();
    kmm=job.get();
  }
  finally {
    if (job != null)     job.remove();
  }
  for (int i=0; i < parms._K; i++)   Assert.assertTrue("Seed: " + seed,kmm._output._rows[i] != 0);
  return kmm;
}
