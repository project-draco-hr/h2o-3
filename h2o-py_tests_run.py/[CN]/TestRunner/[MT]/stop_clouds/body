def stop_clouds(self):
    '\n        Stop all H2O clouds.\n\n        @return: none\n        '
    if self.terminated:
        return
    if (self.use_cloud or self.use_cloud2):
        print ''
        print 'All tests completed...'
        print ''
        return
    print ''
    print 'All tests completed; tearing down clouds...'
    print ''
    for cloud in self.clouds:
        cloud.stop()
