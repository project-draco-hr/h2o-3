{
  key=fr._key;
  _fr=fr;
  off=0;
  rows=fr.numRows();
  len=(int)Math.min(100,rows);
  byteSize=fr.byteSize();
  columns=new Col[fr.numCols()];
  Vec[] vecs=fr.vecs();
  for (int i=0; i < columns.length; i++)   columns[i]=new Col(fr._names[i],vecs[i],off,len);
  isText=fr.numCols() == 1 && vecs[0] instanceof ByteVec;
}
