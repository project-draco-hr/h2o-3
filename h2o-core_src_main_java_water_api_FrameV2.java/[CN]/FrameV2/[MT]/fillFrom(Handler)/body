{
  off=0;
  rows=_fr.numRows();
  len=(int)Math.min(100,rows);
  if (h instanceof InspectHandler) {
    off=((InspectHandler)h)._off;
    len=((InspectHandler)h)._len;
  }
  byteSize=_fr.byteSize();
  columns=new Col[_fr.numCols()];
  Vec[] vecs=_fr.vecs();
  for (int i=0; i < columns.length; i++)   columns[i]=new Col(_fr._names[i],vecs[i],off,len);
  return this;
}
