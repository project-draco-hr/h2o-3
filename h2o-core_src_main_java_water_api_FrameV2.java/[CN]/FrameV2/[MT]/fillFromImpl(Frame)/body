{
  this._fr=f;
  this.key=f._key;
  this.checksum=_fr.checksum();
  off=0;
  rows=_fr.numRows();
  if (off == 0)   off=1;
  if (len == 0)   len=100;
  off=off - 1;
  len=(int)Math.min(len,rows);
  byteSize=_fr.byteSize();
  columns=new Col[_fr.numCols()];
  Vec[] vecs=_fr.vecs();
  for (int i=0; i < columns.length; i++)   columns[i]=new Col(_fr._names[i],vecs[i],off,len);
  return this;
}
