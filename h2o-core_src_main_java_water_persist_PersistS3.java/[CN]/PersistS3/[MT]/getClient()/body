{
  if (_s3 == null) {
synchronized (_lock) {
      if (_s3 == null) {
        try {
          throw H2O.unimpl();
        }
 catch (        Throwable e) {
          StringBuilder msg=new StringBuilder();
          msg.append(e.getMessage() + "\n");
          msg.append("Unable to load S3 credentials.");
          if (H2O.ARGS.aws_credentials == null)           msg.append(HELP);
          Log.throwErr(new RuntimeException(msg.toString()));
        }
      }
    }
  }
  return _s3;
}
