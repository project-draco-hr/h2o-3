{
  try {
    pre();
    nc.addNA();
    for (int k=1; k < K; ++k)     nc.addNum(k % 254);
    assertEquals(K,nc._len);
    post();
    Assert.assertTrue(cc.isNA(0));
    for (int k=1; k < K; ++k)     Assert.assertEquals(k % 254,cc.at80(k));
    Assert.assertTrue(cc instanceof C1Chunk);
  }
  finally {
    remove();
  }
}
