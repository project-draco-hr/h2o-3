{
  try {
    pre();
    for (int k=0; k < K; ++k)     nc.addNA();
    assertEquals(K,nc._len);
    post();
    cc.set(K - 1,342l);
    post_write();
    assertEquals(K,nc._len);
    for (int k=0; k < K - 1; ++k)     Assert.assertTrue(cc.isNA(k));
    Assert.assertEquals(342,cc.at8(K - 1));
    Assert.assertTrue(!(cc instanceof C0LChunk));
  }
  finally {
    remove();
  }
}
