{
  try {
    pre();
    for (int k=0; k < K; ++k)     nc.addNA();
    assertEquals(nc.len(),K);
    post();
    cc.set0(K - 1,342.34);
    post_write();
    assertEquals(nc.len(),K);
    for (int k=0; k < K - 1; ++k)     AssertJUnit.assertTrue(cc.isNA0(k));
    AssertJUnit.assertEquals(342.34,cc.at0(K - 1));
    AssertJUnit.assertTrue(!(cc instanceof C0DChunk));
  }
  finally {
    remove();
  }
}
