{
  try {
    pre();
    for (int k=0; k < K; ++k)     nc.addNum(Double.POSITIVE_INFINITY);
    assertEquals(K,nc.len());
    post();
    for (int k=0; k < K; ++k)     Assert.assertEquals(Double.POSITIVE_INFINITY,cc.at0(k),0.0001);
    Assert.assertTrue(cc instanceof C0DChunk);
  }
  finally {
    remove();
  }
}
