{
  try {
    pre();
    for (int k=0; k < K; ++k)     nc.addNum(4,0);
    post();
    assertEquals(K,nc.len());
    cc.setNA0(K - 1);
    post_write();
    assertEquals(cc.len(),K);
    for (int k=0; k < K - 1; ++k)     Assert.assertEquals(4,cc.at80(k));
    Assert.assertTrue(cc.isNA0(K - 1));
    Assert.assertTrue(!(cc.chk2() instanceof C0LChunk));
  }
  finally {
    remove();
  }
}
