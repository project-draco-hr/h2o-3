{
  try {
    pre();
    for (int k=0; k < K; ++k)     nc.addNum(Double.NEGATIVE_INFINITY);
    assertEquals(K,nc.len());
    post();
    for (int k=0; k < K; ++k)     AssertJUnit.assertEquals(Double.NEGATIVE_INFINITY,cc.at0(k));
    AssertJUnit.assertTrue(cc instanceof C0DChunk);
  }
  finally {
    remove();
  }
}
