{
  Val val=Exec.exec(str);
switch (val.type()) {
case Val.FRM:
    Frame fr=val.getFrame();
  System.out.println(fr);
checkSaneFrame();
fr.delete();
break;
case Val.NUM:
System.out.println("num= " + val.getNum());
checkSaneFrame();
break;
case Val.STR:
System.out.println("str= " + val.getStr());
checkSaneFrame();
break;
default :
throw water.H2O.fail();
}
}
