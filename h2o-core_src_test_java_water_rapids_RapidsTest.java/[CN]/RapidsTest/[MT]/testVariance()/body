{
  String tree="({x . (var x x \"everything\")} (rows a.hex [0]))";
  checkTree(tree);
  tree="({x . (var x x \"everything\")} a.hex)";
  checkTree(tree);
  tree="(table (trunc (cols a.hex 1)) FALSE)";
  checkTree(tree);
  tree="(table (cols a.hex 1) FALSE)";
  checkTree(tree);
  tree="(table (cols a.hex 1) (cols a.hex 2) FALSE)";
  checkTree(tree);
}
