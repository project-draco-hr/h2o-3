{
  _nums=dinfo._nums;
  _numOff=dinfo._cats;
  _responseId=dinfo.responseChunkId(0);
  _weightId=dinfo._weights ? dinfo.weightChunkId() : -1;
  _offsetId=dinfo._offset ? dinfo.offsetChunkId() : -1;
