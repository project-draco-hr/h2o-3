{
  if (_nobs > 0 && ymt._nobs > 0) {
    double a=_nobs / (_nobs + ymt._nobs);
    double b=ymt._nobs / (_nobs + ymt._nobs);
    _ymu=_ymu * a + ymt._ymu * b;
    _nobs+=ymt._nobs;
    if (_yMin > ymt._yMin)     _yMin=ymt._yMin;
    if (_yMax < ymt._yMax)     _yMax=ymt._yMax;
  }
 else   if (_nobs == 0) {
    _ymu=ymt._ymu;
    _nobs=ymt._nobs;
    _yMin=ymt._yMin;
    _yMax=ymt._yMax;
  }
}
