{
  for (int j=0; j < cols; ++j) {
    for (int i=0; i < rows; ++i) {
      raw[j][i]=get(j,i);
    }
  }
  for (int j=0; j < cols; ++j) {
    chunks[j]=new NewChunk(raw[j]).compress();
    Log.info("Column " + j + " compressed into: "+ chunks[j].getClass().toString());
  }
  Log.info("COLS: " + cols);
  Log.info("ROWS: " + rows);
  Log.info("REPS: " + rep);
  raw();
  raw();
  chunks();
  chunks();
  chunksInline();
  chunksInline();
  mrtask(false);
  mrtask(false);
  bulk();
  bulk();
  Log.info("Now doing funny stuff.\n\n");
  mrtask(true);
  mrtask(true);
  chunksInverted();
  chunksInverted();
  rawInverted();
  rawInverted();
}
