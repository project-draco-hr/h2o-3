{
  super(null);
  _input=input;
  _wordModel=input._wordModel;
  _normModel=input._normModel;
  _vocab=input._vocabKey.get();
  _vocabSize=(int)_vocab.numRows();
  _wordVecSize=input._vecSize;
  _windowSize=input._windowSize;
  _syn0=input._syn0;
  _syn1=input._syn1;
  _initLearningRate=input._initLearningRate;
  _sentSampleRate=input._sentSampleRate;
  _epochs=input._epochs;
  assert(_output == null);
  assert(_vocab.numRows() > 0);
  if (input._normModel == NormModel.NegSampling) {
    _negExCnt=input._numNegEx;
    _unigramTable=input._uniTable;
    _HBWTCode=null;
    _HBWTPoint=null;
  }
 else {
    _negExCnt=0;
    _unigramTable=null;
    _HBWTCode=input._HBWTCode;
    _HBWTPoint=input._HBWTPoint;
  }
}
