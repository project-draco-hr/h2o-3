{
  Vec[] vecs=_train.get().vecs();
  boolean has_na=false;
  for (int i=0; i < vecs.length; i++) {
    if (vecs[i].naCnt() > 0) {
      has_na=true;
      break;
    }
  }
  return (!has_na && _loss == GLRMParameters.Loss.L2 && (_gamma_x == 0 || _regularization_x == GLRMParameters.Regularizer.L2) && (_gamma_y == 0 || _regularization_y == GLRMParameters.Regularizer.L2));
}
