{
  if (u == null)   return u;
switch (regularization) {
case L2:
case L1:
    return u;
case NonNegative:
case OneSparse:
case UnitOneSparse:
  return rproxgrad(u,1,1,regularization,rand);
case Simplex:
double reg=regularize(u,regularization);
if (reg == 0) return u;
return rproxgrad(u,1,1,regularization,rand);
default :
throw new RuntimeException("Unknown regularization function " + regularization);
}
}
