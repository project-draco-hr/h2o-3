{
  redirectStandardStreams();
  try {
    String errorMessage=validate(rawInput);
    if (errorMessage == null) {
      _basic(testcase_id,test_description,train_dataset_id,train_dataset_filename,validate_dataset_id,validate_dataset_filename,toDRFParameters(rawInput),rawInput);
    }
 else {
      System.out.println("[INVALID] " + errorMessage);
      Assert.fail(String.format("INVALID INPUT - this test is skipped."));
    }
  }
  finally {
    try {
      Thread.sleep(100);
    }
 catch (    InterruptedException ex) {
      Thread.currentThread().interrupt();
    }
    resetStandardStreams();
  }
}
