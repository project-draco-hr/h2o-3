{
switch (_output.getModelCategory()) {
case Binomial:
    return new ModelMetricsBinomial.MetricBuilderBinomial(domain,ModelUtils.DEFAULT_THRESHOLDS);
case Multinomial:
  return new ModelMetricsMultinomial.MetricBuilderMultinomial(_output.nclasses(),domain);
case Regression:
return new ModelMetricsRegression.MetricBuilderRegression();
case AutoEncoder:
return new ModelMetricsAutoEncoder.MetricBuilderAutoEncoder(_output.nfeatures());
default :
throw H2O.unimpl();
}
}
