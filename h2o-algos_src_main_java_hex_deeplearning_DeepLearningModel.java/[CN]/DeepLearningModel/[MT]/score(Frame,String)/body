{
  if (!_parms._autoencoder)   return super.score(fr,destination_key);
 else {
    Frame adaptFr=new Frame(fr);
    adaptTestForTrain(adaptFr,true,adaptFr.find(_output.responseName()) != -1);
    Frame output=predictScoreImpl(fr,adaptFr,destination_key);
    cleanup_adapt(adaptFr,fr);
    return output;
  }
}
