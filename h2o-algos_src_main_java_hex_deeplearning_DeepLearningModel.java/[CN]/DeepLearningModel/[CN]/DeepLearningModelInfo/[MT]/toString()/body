{
  StringBuilder sb=new StringBuilder();
  if (get_params()._diagnostics && !get_params()._quiet_mode) {
    if (get_params()._sparsity_beta > 0) {
      for (int k=0; k < get_params()._hidden.length; k++)       sb.append("Average activation in hidden layer " + k + " is  "+ mean_a[k]+ " \n");
    }
    createSummaryTable();
    sb.append(summaryTable.toString(1));
  }
  return sb.toString();
}
