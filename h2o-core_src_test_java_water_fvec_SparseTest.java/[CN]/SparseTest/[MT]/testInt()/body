{
  runTest(new double[]{1,2,Double.NaN},4,5,CXIChunk.class,CXIChunk.class,C1Chunk.class);
  runTest(new double[]{1,2000,Double.NaN,3},4,5,CXIChunk.class,CXIChunk.class,C2Chunk.class);
