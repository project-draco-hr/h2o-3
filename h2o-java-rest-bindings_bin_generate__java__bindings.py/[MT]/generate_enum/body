def generate_enum(name, values):
    if args.verbose:
        print ('Generating enum: ', name)
    pojo = []
    pojo.append('package water.bindings.pojos;')
    pojo.append('')
    pojo.append((('public enum ' + name) + ' {'))
    for value in values:
        pojo.append('    {value},'.format(value=value))
    pojo.append('}')
    return pojo
