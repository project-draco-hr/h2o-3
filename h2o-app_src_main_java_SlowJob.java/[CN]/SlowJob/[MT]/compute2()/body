{
  if (_hi - _lo >= 2) {
    final int mid=(_lo + _hi) >>> 1;
    setPendingCount(1);
    new SlowJob(this,mid,_hi).fork();
    new SlowJob(this,_lo,mid).compute2();
  }
 else {
    if (_hi > _lo) {
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException ignore) {
      }
      System.out.println("Work " + _lo + " "+ _hi);
      _job.update(1);
    }
    tryComplete();
  }
}
