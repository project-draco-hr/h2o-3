def is_rdemo(file_name):
    '\n    Return True if file_name matches a regexp for a R demo.  False otherwise.\n    '
    demos = ['h2o.anomaly.R', 'h2o.deeplearning.R', 'h2o.gbm.R', 'h2o.glm.R', 'h2o.glrm.R', 'h2o.kmeans.R', 'h2o.naiveBayes.R', 'h2o.prcomp.R', 'h2o.randomForest.R']
    for demo in demos:
        if (file_name == demo):
            return True
    return False
