def is_python_test_file(file_name):
    '\n    Return True if file_name matches a regexp for a python test.  False otherwise.\n    '
    if (file_name == 'test_config.py'):
        return False
    if re.match('^pyunit.*\\.py$', file_name):
        return True
    if re.match('^test.*\\.py$', file_name):
        return True
    return False
