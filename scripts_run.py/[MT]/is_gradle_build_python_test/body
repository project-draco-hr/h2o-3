def is_gradle_build_python_test(file_name):
    '\n    Return True if file_name matches a regexp for on of the python test run during gradle build.  False otherwise.\n    '
    return (file_name in ['generate_rest_api_docs.py', 'generate_java_bindings.py', 'test_gbm_prostate.py', 'test_rest_api.py'])
