def terminate(self):
    '\n        Terminate a running test.  (Due to a signal.)\n\n        @return: none\n        '
    self.terminated = True
    if (self.pid > 0):
        print 'Killing Test {} with PID {}'.format(os.path.join(self.test_short_dir, self.test_name), self.pid)
        try:
            self.child.terminate()
        except OSError:
            pass
    self.pid = (-1)
