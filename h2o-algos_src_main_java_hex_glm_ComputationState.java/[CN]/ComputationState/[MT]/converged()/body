{
  if (_betaDiff < _parms._beta_epsilon)   convergenceMsg="betaDiff < eps; betaDiff = " + _betaDiff + ", eps = "+ _parms._beta_epsilon;
 else   if (_relImprovement < _parms._objective_epsilon)   convergenceMsg="relImprovement < eps; relImprovement = " + _relImprovement + ", eps = "+ _parms._objective_epsilon;
 else   convergenceMsg="not converged, betaDiff = " + _betaDiff + ", relImprovement = "+ _relImprovement;
  return (_betaDiff < _parms._beta_epsilon || _relImprovement < _parms._objective_epsilon);
}
