def _eager(self, sb=None):
    '\n    The top-level call to evaluate an expression DAG.\n    This call is mutually recursive with ExprNode._do_it and H2OFrame._do_it\n\n    :return: sb\n    '
    if (sb is None):
        sb = []
    sb += [(('(' + self._op) + ' ')]
    for child in self._children:
        ExprNode._do_it(child, sb)
    sb += [') ']
    return sb
