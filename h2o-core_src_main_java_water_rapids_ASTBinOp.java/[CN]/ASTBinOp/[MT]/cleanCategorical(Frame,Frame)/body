{
  final boolean categoricalOK=categoricalOK();
  final Vec oldvecs[]=oldfr.vecs();
  final Vec newvecs[]=newfr.vecs();
  for (int i=0; i < oldvecs.length; i++)   if (!oldvecs[i].isNumeric() && !oldvecs[i].isTime() && !(oldvecs[i].isCategorical() && categoricalOK))   newvecs[i]=newvecs[i].makeCon(Double.NaN);
  return new ValFrame(newfr);
}
