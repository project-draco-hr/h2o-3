{
  if (!isRunning())   return true;
  if (model._output._iters > _parms._max_iters)   return true;
  if (oldCenters == null)   return false;
  double average_change=0;
  for (int clu=0; clu < _parms._k; clu++)   average_change+=distance(oldCenters[clu],newCenters[clu],_ncats);
  average_change/=_parms._k;
  if (average_change < TOLERANCE)   return true;
  return false;
}
