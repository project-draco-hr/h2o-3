{
  _means=means;
  _mults=mults;
  _tss=0;
  _isCats=isCats;
  _card=card;
  if (mults != null) {
    _gc=new double[means.length];
    for (int i=0; i < means.length; i++) {
      if (isCats[i] != null)       _gc[i]=Math.min(Math.round(means[i]),_card[i] - 1);
    }
  }
 else   _gc=Arrays.copyOf(means,means.length);
}
