{
  final double CAT_DIST=2.0;
  int min=-1;
  double minSqr=Double.MAX_VALUE;
  for (int cluster=0; cluster < count; cluster++) {
    double sqr=0;
    int pts=point.length;
    for (int column=0; column < ncats; column++) {
      double d=point[column];
      if (Double.isNaN(d))       pts--;
 else       if (d != clusters[cluster][column])       sqr+=CAT_DIST;
    }
    for (int column=ncats; column < clusters[cluster].length; column++) {
      double d=point[column];
      if (Double.isNaN(d))       pts--;
 else {
        double delta=d - clusters[cluster][column];
        sqr+=delta * delta;
      }
    }
    if (0 < pts && pts < point.length)     sqr*=point.length / pts;
    if (sqr < minSqr) {
      min=cluster;
      minSqr=sqr;
    }
  }
  cd._cluster=min;
  cd._dist=minSqr;
  return cd;
}
