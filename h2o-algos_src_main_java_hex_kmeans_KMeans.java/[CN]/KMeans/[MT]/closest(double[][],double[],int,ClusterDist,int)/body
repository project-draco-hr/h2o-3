{
  int min=-1;
  double minSqr=Double.MAX_VALUE;
  for (int cluster=0; cluster < count; cluster++) {
    double sqr=hex.genmodel.GenModel.KMeans_distance(centers[cluster],point,ncats);
    if (sqr < minSqr) {
      min=cluster;
      minSqr=sqr;
    }
  }
  cd._cluster=min;
  cd._dist=minSqr;
  return cd;
}
