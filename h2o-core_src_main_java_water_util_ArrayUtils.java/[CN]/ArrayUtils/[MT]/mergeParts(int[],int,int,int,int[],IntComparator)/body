{
  for (int i=lo; i <= hiIdx; i++) {
    tmp[i]=data[i];
  }
  int i=lo;
  int j=mi + 1;
  int k=lo;
  while (i <= mi && j <= hiIdx) {
    if (cmp.compare(tmp[j],tmp[i]) > 0) {
      data[k]=tmp[i];
      i++;
    }
 else {
      data[k]=tmp[j];
      j++;
    }
    k++;
  }
  while (i <= mi) {
    data[k]=tmp[i];
    k++;
    i++;
  }
}
