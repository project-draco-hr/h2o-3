{
  if (data.length < cutoff) {
    for (int i=0; i < data.length; i++) {
      for (int j=i; j > 0 && comparator.compare(data[j - 1],data[j]) > 0; j--) {
        int tmp=data[j];
        data[j]=data[j - 1];
        data[j - 1]=tmp;
      }
    }
  }
 else {
    int[] tmp=new int[data.length];
    mergeSort(data,0,data.length - 1,tmp,comparator);
  }
}
