@property
def fold_assignment(self):
    u'\n        Enum["auto", "random", "modulo", "stratified"]: Cross-validation fold assignment scheme, if fold_column is not\n        specified. The \'Stratified\' option will stratify the folds based on the response variable, for classification\n        problems. (Default: "auto")\n        '
    return self._parms.get(u'fold_assignment')
