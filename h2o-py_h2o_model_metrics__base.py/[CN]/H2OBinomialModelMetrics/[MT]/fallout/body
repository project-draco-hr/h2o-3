def fallout(self, thresholds=None):
    u'\n    :param thresholds: thresholds parameter must be a list (i.e. [0.01, 0.5, 0.99]). If None, then the thresholds in this set of metrics will be used.\n    :return: The fallout or False Positive Rate for this set of metrics and thresholds\n    '
    return self.metric(u'fpr', thresholds=thresholds)
