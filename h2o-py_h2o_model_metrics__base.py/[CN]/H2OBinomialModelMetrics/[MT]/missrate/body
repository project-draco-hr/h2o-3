def missrate(self, thresholds=None):
    u'\n    :param thresholds: thresholds parameter must be a list (i.e. [0.01, 0.5, 0.99]). If None, then the thresholds in this set of metrics will be used.\n    :return: THe missrate or False Negative Rate.\n    '
    return self.metric(u'fnr', thresholds=thresholds)
