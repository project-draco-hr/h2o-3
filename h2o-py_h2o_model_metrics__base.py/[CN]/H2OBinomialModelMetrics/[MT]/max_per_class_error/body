def max_per_class_error(self, thresholds=None):
    '\n    :param thresholds: thresholds parameter must be a list (i.e. [0.01, 0.5, 0.99]). If None, then the thresholds in this set of metrics will be used.\n    :return: Return 1 - min_per_class_accuracy\n    '
    return (1 - self.metric('min_per_class_accuracy', thresholds=thresholds))
