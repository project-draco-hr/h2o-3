@property
def fprs(self):
    u'\n        Return all false positive rates for all threshold values.\n\n        :return: a list of false positive rates.\n        '
    fpr_idx = self._metric_json[u'thresholds_and_metric_scores'].col_header.index(u'fpr')
    fprs = [x[fpr_idx] for x in self._metric_json[u'thresholds_and_metric_scores'].cell_values]
    return fprs
