def residual_degrees_of_freedom(self):
    u'\n        :return: the residual dof if the model has residual deviance, or None if no residual dof.\n        '
    if MetricsBase._has(self._metric_json, u'residual_degrees_of_freedom'):
        return self._metric_json[u'residual_degrees_of_freedom']
    return None
