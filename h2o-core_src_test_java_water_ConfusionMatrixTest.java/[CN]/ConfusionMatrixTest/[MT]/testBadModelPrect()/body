{
  simpleCMTest(frame("v1",vec(ar("A","B","C"),ari(0,0,1,1,2))),frame("v1",vec(ar("A","B","C"),ari(1,1,2,2,2))),ar("A","B","C"),ar("A","B","C"),ar("A","B","C"),ar(ar(0L,2L,0L),ar(0L,0L,2L),ar(0L,0L,1L)),debug,false);
  simpleCMTest(frame("v1",vec(ar("B","C"),ari(0,0,1,1))),frame("v1",vec(ar("A","B"),ari(1,1,0,0))),ar("B","C"),ar("A","B"),ar("B","C","A"),ar(ar(2L,0L,0L),ar(0L,0L,2L),ar(0L,0L,0L)),debug,false);
}
