@staticmethod
def _html_row(row, bold=False):
    res = '<tr>{}</tr>'
    entry = ('<td><b>{}</b></td>' if bold else '<td>{}</td>')
    entries = '\n'.join([(entry.format(str(r)) if ((len(str(r)) < 10) or (not _is_number(str(r)))) else entry.format('{0:.7f}'.format(float(str(r))))) for r in row])
    return res.format(entries)
