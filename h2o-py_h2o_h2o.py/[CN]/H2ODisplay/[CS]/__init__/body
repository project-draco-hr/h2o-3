def __init__(self, table=None, header=None, **kwargs):
    self.header = header
    self.table = table
    self.kwargs = kwargs
    self.do_print = True
    if H2ODisplay._in_ipy():
        from IPython.display import display
        display(self)
        self.do_print = False
    else:
        self.pprint()
        self.do_print = False
