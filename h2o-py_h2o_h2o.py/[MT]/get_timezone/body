def get_timezone():
    '\n  Get the Time Zone on the H2O Cloud\n  :return: the time zone (string)\n  '
    tmp_key = H2OFrame.py_tmp_key()
    expr = '(= !{} (getTimeZone ))'.format(tmp_key)
    rapids(expr)
    j = frame(tmp_key)
    fr = j['frames'][0]
    rows = fr['rows']
    veckeys = fr['vec_ids']
    cols = fr['columns']
    colnames = [col['label'] for col in cols]
    vecs = H2OVec.new_vecs(zip(colnames, veckeys), rows)
    removeFrameShallow(tmp_key)
    return H2OFrame(vecs=vecs)[(0, 0)]
