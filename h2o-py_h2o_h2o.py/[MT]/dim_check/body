def dim_check(data1, data2):
    '\n  Check that the dimensions of the data1 and data2 are the same\n  :param data1: an H2OFrame, H2OVec or Expr\n  :param data2: an H2OFrame, H2OVec or Expr\n  :return: None\n  '
    (data1_rows, data1_cols) = data1.dim()
    (data2_rows, data2_cols) = data2.dim()
    assert ((data1_rows == data2_rows) and (data1_cols == data2_cols)), 'failed dim check! data1_rows:{0} data2_rows:{1} data1_cols:{2} data2_cols:{3}'.format(data1_rows, data2_rows, data1_cols, data2_cols)
