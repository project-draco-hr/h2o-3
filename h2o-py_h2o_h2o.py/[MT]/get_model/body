def get_model(model_id):
    '\n  Return the specified model\n\n  Parameters\n  ----------\n\n  model_id : str\n    The model identification in h2o\n\n  :return: H2OModel\n\n  '
    m = H2OEstimator()
    model_json = H2OConnection.get_json(('Models/' + model_id))['models'][0]
    m._resolve_model(model_id, model_json)
    return m
