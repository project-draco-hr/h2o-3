def _simple_un_math_op(op, data):
    '\n  Element-wise math operations on H2OFrame and H2OVec\n\n  :param op: the math operation\n  :param data: the H2OFrame or H2OVec object to operate on.\n  :return: H2OFrame or H2oVec, with lazy operation\n  '
    if isinstance(data, H2OFrame):
        return H2OFrame(vecs=[_simple_un_math_op(op, vec) for vec in data._vecs])
    if isinstance(data, H2OVec):
        return H2OVec(data._name, Expr(op, left=data, length=len(data)))
    raise ValueError, (op + ' only operates on H2OFrame or H2OVec objects')
