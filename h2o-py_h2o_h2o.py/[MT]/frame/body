def frame(frame_id, exclude=u''):
    u'\n    Retrieve metadata for an id that points to a Frame.\n\n    Parameters\n    ----------\n    frame_id : str\n      A pointer to a Frame in H2O.\n\n    :returns: dict containing the frame meta-information.\n    '
    return api((u'GET /3/Frames/%s' % (frame_id + exclude)))
