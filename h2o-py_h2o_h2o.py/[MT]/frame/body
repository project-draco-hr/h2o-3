def frame(frame_id, exclude=u''):
    u'Retrieve metadata for an id that points to a Frame.\n\n    Parameters\n    ----------\n    frame_id : str\n      A pointer to a Frame in H2O.\n\n    Returns\n    -------\n      Python dict containing the frame meta-information\n    '
    return h2oconn.get_json(((u'Frames/' + frame_id) + exclude))
