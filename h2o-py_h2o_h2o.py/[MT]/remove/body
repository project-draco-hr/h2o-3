def remove(x):
    'Remove object from H2O.\n\n  Parameters\n  ----------\n  x : H2OFrame or str\n    The object pointing to the object to be removed.\n  '
    if (x is None):
        raise ValueError('remove with no object is not supported, for your protection')
    if isinstance(x, H2OFrame):
        x = x._ex._cache._id
        if (x is None):
            return
        rapids('(rm {})'.format(x))
    elif isinstance(x, H2OEstimator):
        H2OConnection.delete(('DKV/' + x.model_id))
    elif isinstance(x, str):
        H2OConnection.delete(('DKV/' + x))
