def load_model(path):
    '\n  Load a saved H2O model from disk.\n  :param path: The full path of the H2O Model to be imported.\n  :return: the model\n  '
    if (not isinstance(path, str)):
        raise ValueError('`path` must be a non-empty character string')
    kwargs = dict([('dir', path), ('_rest_version', 99)])
    res = H2OConnection.post('Models.bin/', **kwargs)
    return get_model(res.json()['models'][0]['model_id']['name'])
