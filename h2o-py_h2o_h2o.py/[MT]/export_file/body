def export_file(frame, path, force=False):
    u'Export a given H2OFrame to a path on the machine this python session is currently\n    connected to. To view the current session, call h2o.cluster_info().\n\n    Parameters\n    ----------\n    frame : H2OFrame\n      The Frame to save to disk.\n    path : str\n      The path to the save point on disk.\n    force : bool\n      Overwrite any preexisting file with the same path\n    '
    H2OJob(api((u'GET /3/Frames/%s/export/%s/overwrite/%s' % (frame.frame_id, path, str(force).lower()))), u'Export File').poll()
