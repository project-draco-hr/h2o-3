def run_test(sys_args, test_to_run):
    import pkg_resources
    ver = pkg_resources.get_distribution('h2o').version
    print ('H2O PYTHON PACKAGE VERSION: ' + str(ver))
    (ip, port) = sys_args[2].split(':')
    init(ip, port)
    log_and_echo('------------------------------------------------------------')
    log_and_echo('')
    log_and_echo(('STARTING TEST: ' + str(ou())))
    log_and_echo('')
    log_and_echo('------------------------------------------------------------')
    test_to_run(ip, port)
