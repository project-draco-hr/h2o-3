def __init__(self, **kwargs):
    super(H2OPrincipalComponentAnalysisEstimator, self).__init__()
    self._parms = {}
    for name in ['model_id', 'training_frame', 'validation_frame', 'ignored_columns', 'ignore_const_cols', 'score_each_iteration', 'transform', 'pca_method', 'k', 'max_iterations', 'use_all_factor_levels', 'compute_metrics', 'impute_missing', 'seed', 'max_runtime_secs']:
        pname = (name[:(-1)] if (name[(-1)] == '_') else name)
        self._parms[pname] = (kwargs[name] if (name in kwargs) else None)
