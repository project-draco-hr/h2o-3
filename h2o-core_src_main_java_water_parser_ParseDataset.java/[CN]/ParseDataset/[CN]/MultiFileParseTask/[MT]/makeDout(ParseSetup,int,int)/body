{
  AppendableVec[] avs=new AppendableVec[localSetup._ncols];
  long[] espc=MemoryManager.malloc8(nchunks);
  for (int i=0; i < avs.length; ++i)   avs[i]=new AppendableVec(_vg.vecKey(i + _vecIdStart),_setup._chunkSize,espc,chunkOff);
  return localSetup._pType == ParserType.SVMLight ? new SVMLightFVecDataOut(_vg,_vecIdStart,chunkOff,enums(_eKey,localSetup._ncols),_setup._chunkSize,avs) : new FVecDataOut(_vg,chunkOff,enums(_eKey,localSetup._ncols),localSetup._ctypes,_setup._chunkSize,avs);
}
