{
  AppendableVec[] avs=new AppendableVec[_setup._ncols];
  for (int i=0; i < avs.length; ++i)   avs[i]=new AppendableVec(_vg.vecKey(_vecIdStart + i),_espc,_startChunkIdx);
  Categorical[] enums=enums(_eKey,_setup._ncols);
  FVecDataIn din=new FVecDataIn(in);
  FVecDataOut dout;
  Parser p;
switch (_setup._pType) {
case ARFF:
case CSV:
    p=new CsvParser(_setup);
  dout=new FVecDataOut(_vg,_startChunkIdx + in.cidx(),enums,_setup._ctypes,_setup._chunkSize,avs);
break;
case SVMLight:
p=new SVMLightParser(_setup);
dout=new SVMLightFVecDataOut(_vg,_vecIdStart,in.cidx() + _startChunkIdx,enums,_setup._chunkSize,avs);
break;
default :
throw H2O.unimpl();
}
p.parallelParse(in.cidx(),din,dout);
(_dout=dout).close(_fs);
Job.update(in._len,_job_key);
freeMem(in,0);
freeMem(in,1);
}
