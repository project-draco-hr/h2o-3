{
  String[] res=null;
  int i=0;
  if (colNames != null) {
    if (colNames.length == ncols)     return colNames;
 else {
      i=colNames.length;
      res=Arrays.copyOf(colNames,ncols);
    }
  }
  if (res == null)   res=new String[ncols];
  for (; i < res.length; ++i)   res[i]="C" + String.valueOf(i + 1);
  return res;
}
