def expr_show(ip, port):
    h2o.init(ip, port)
    iris = h2o.import_frame(path=h2o.locate('smalldata/iris/iris_wheader.csv'))
    print 'iris:'
    iris.show()
    res = (2 - iris)
    res2 = res[0]
    print 'res2:'
    res2.show()
    res3 = res[0]
    print 'res3:'
    res3.show()
    expr = Expr([1, 2, 3])
    print 'expr:'
    expr.show()
    res = (2 - iris)
    res4 = res[(5, 2)]
    print 'res4:'
    res4.show()
    res5 = res[(5, 2)]
    print 'res5:'
    res5.show()
    expr = Expr([[1, 2, 3], [4, 5, 6]])
    print 'expr:'
    expr.show()
