{
  TwoDimTable table=new TwoDimTable("All numbers",new String[]{"Num1","Num2","Num3"},new String[]{"%f","%f","%f"},new String[]{"R1","R2","R3","R4"},new String[4][],new double[][]{new double[]{1.123,3.42,3200034.00001},new double[]{123.34,emptyDouble,1.0},new double[]{emptyDouble,emptyDouble,3234.00001},new double[]{3.33420923423423,83.32,3.40234234}});
  String ts=table.toString();
  assertTrue(ts.length() > 0);
  Log.info(ts);
  String json=new String(table.writeJSON(new AutoBuffer()).buf());
  assertTrue(json.equals("{\"tableHeader\":\"All numbers\",\"colHeaders\":[\"Num1\",\"Num2\",\"Num3\"],\"colFormatStrings\":[\"%f\",\"%f\",\"%f\"],\"rowHeaders\":[\"R1\",\"R2\",\"R3\",\"R4\"],\"strCellValues\":[[null,null,null],[null,null,null],[null,null,null],[null,null,null]],\"dblCellValues\":[[1.123,3.42,3200034.00001],[123.34,null,1.0],[null,null,3234.00001],[3.33420923423423,83.32,3.40234234]]}"));
  Log.info(json);
}
