def download_pojo(ip, port):
    h2o.init(ip, port)
    iris = h2o.import_frame(path=h2o.locate('smalldata/iris/iris_wheader.csv'))
    print 'iris:'
    iris.show()
    res = (2 - iris)
    res2 = H2OVec(name='C0', expr=res[0])
    print 'res2:'
    res2.show()
    res3 = H2OVec(name='C1', expr=res[1])
    print 'res3:'
    res3.show()
    iris[2].show()
