{
  Frame f=new MRTask(){
    @Override public void map(    Chunk chk,    NewChunk newChk){
      if (chk instanceof C0DChunk)       for (int i=0; i < chk.len(); i++)       newChk.addNA();
 else       if (((CStrChunk)chk)._isAllASCII) {
        ((CStrChunk)chk).asciiToUpper(newChk);
      }
 else {
        BufferedString tmpStr1=new BufferedString();
        BufferedString tmpStr2=new BufferedString();
        for (int i=0; i < chk._len; i++) {
          if (chk.isNA(i))           newChk.addNA();
 else           newChk.addStr(tmpStr2.setTo(chk.atStr(tmpStr1,i).toString().toUpperCase(Locale.ENGLISH)));
        }
      }
    }
  }
.doAll(1,vec).outputFrame();
  return f;
}
