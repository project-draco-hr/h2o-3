{
  AST ary=E.parse();
  _cidx=(int)E.nextDbl();
  AST a=E.parse();
  if (a instanceof ASTDoubleList)   _probs=((ASTDoubleList)a)._d;
 else   if (a instanceof ASTNum)   _probs=new double[]{((ASTNum)a)._d};
 else   _probs=null;
  E.eatEnd();
  ASTQPFPC res=(ASTQPFPC)clone();
  res._probs=_probs;
  res._asts=new AST[]{ary};
  return res;
}
