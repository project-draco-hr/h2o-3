{
  Frame f=new MRTask(){
    @Override public void map(    Chunk[] c,    NewChunk nc){
      for (int i=0; i < c[0]._len; ++i)       if (c[1].at8(i) == _selectValue)       nc.addNum(c[0].atd(i));
