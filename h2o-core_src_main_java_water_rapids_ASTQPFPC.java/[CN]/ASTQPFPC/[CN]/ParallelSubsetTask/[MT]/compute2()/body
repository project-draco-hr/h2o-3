{
  addToPendingCount(_features.length - 1);
  _ps=new ParallelSubsets[_features.length];
  Key[][] fkeys=new Key[_features.length][_selectValues.length];
  for (int i=0; i < fkeys.length; ++i)   for (int j=0; j < _selectValues.length; j++)   fkeys[i][j]=Key.make();
  _fkeys=ArrayUtils.flat(fkeys);
  for (int i=0; i < _features.length; ++i)   (_ps[i]=new ParallelSubsets(new Callback(i),_features[i],_classes,_selectValues,fkeys[i],_probs,_classLabels,_colLabels[i])).fork();
}
