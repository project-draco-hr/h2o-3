{
  String stepName=name();
  StringBuilder sb=new StringBuilder();
  String s="  class " + stepName + " extends Step<"+ stepName+ "> {\n"+ "    private final String[] _cols = {"+ _cols+ "};\n"+ "    public "+ stepName+ "() { }\n"+ "    @Override public RowData transform(RowData row) {\n"+ "      RowData colSelect = new RowData();\n"+ "      for(String s: _cols) \n"+ "        colSelect.put(s, row.get(s));\n"+ "      return colSelect;\n"+ "    }\n"+ "  }\n";
  sb.append(s);
  return sb;
}
