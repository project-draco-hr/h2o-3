{
  int type=peekTypeAt(i);
  Object o=peekAt(i);
switch (type) {
case ARY:
    return ((ValFrame)o)._fr.numRows() + "x" + ((ValFrame)o)._fr.numCols();
case NUM:
  return Double.toString(((ValNum)o)._d);
case STR:
return ((ValStr)o)._s;
case ID:
return ((ValId)o)._id;
case SERIES:
return o.toString();
case SPAN:
return o.toString();
case NULL:
return "null";
default :
throw H2O.fail("Bad value on the stack");
}
}
