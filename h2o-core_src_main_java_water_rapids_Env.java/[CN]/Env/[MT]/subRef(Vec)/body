{
  if (v == null)   return false;
  boolean delete;
  if (getRef(v) == null)   return false;
  if (hasLock(v._key))   return false;
  int cnt=getRef(v)._val - 1;
  if (cnt > 0) {
    putRef(v,new IcedInt(cnt));
    delete=false;
  }
 else {
    extinguishCounts(v);
    delete=true;
  }
  if (delete)   removeVec(v,null);
  return delete;
}
