{
  _local.clear();
  Futures fs=new Futures();
  for (  String k : _local._local_frames.keySet()) {
    Frame f=_local._local_frames.remove(k);
    for (    Vec v : f.vecs())     removeVec(v,fs);
    f.delete();
  }
  if (_local_locked != null) {
    for (    Key k : _local_locked) {
      Keyed.remove(k,fs);
    }
  }
  fs.blockForPending();
}
