{
  String res="c(";
  if (_spans != null) {
    for (    ASTSpan s : _spans) {
      res+=s.toString();
      res+=",";
    }
    if (_idxs == null)     res=res.substring(0,res.length() - 1);
  }
  if (_idxs != null) {
    if (_idxs.length > 20)     res+="many ";
 else {
      for (      long l : _idxs) {
        res+=l;
        res+=",";
      }
    }
    res=res.substring(0,res.length() - 1);
  }
  res+=")";
  return res;
}
