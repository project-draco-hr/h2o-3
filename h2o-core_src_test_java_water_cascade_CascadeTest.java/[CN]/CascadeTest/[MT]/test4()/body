{
  String tree="(== (KEY a.hex) (# 5))";
  checkTree(tree);
  tree="(<= (KEY a.hex) (# 5))";
  checkTree(tree);
  tree="(>= (KEY a.hex) (# 5))";
  checkTree(tree);
  tree="(> (KEY a.hex) (# 5))";
  checkTree(tree);
  tree="(< (KEY a.hex) (# 5))";
  checkTree(tree);
  tree="(!= (KEY a.hex) (# 5))";
  checkTree(tree);
}
