def __repr__(self):
    if (self.do_print or (not H2ODisplay._in_ipy())):
        if (self.header is None):
            return tabulate.tabulate(self.table, **self.kwargs)
        else:
            return tabulate.tabulate(self.table, headers=self.header, **self.kwargs)
    self.do_print = True
    return ''
