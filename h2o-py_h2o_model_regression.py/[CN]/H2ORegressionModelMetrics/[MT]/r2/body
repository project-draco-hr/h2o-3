def r2(self):
    '\n    Return the R^2 for this regression model.\n\n    The R^2 value is defined to be 1 - mse/var,\n    where var is computed as sigma*sigma.\n    :return: The R^2 for this regression model.\n    '
    mse = self._metric_json['mse']
    sigma = self._metric_json['sigma']
    var = (sigma * sigma)
    return (1 - (mse / var))
