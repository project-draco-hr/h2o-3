{
  ConfusionMatrix cm=new ConfusionMatrix(_cm,_domain);
  float[] hr=new float[_K];
  double mse=0;
  if (_count != 0) {
    if (_hits != null) {
      for (int i=0; i < hr.length; i++) {
        hr[i]=_hits[i] / _count;
      }
    }
    mse=_sumsqe / _count;
  }
  return m._output.addModelMetrics(new ModelMetricsMultinomial(m,f,cm,hr,sigma,mse));
}
