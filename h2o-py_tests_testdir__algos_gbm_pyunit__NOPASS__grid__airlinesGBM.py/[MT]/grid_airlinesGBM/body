def grid_airlinesGBM(ip, port):
    h2o.init(ip, port)
    air = h2o.import_frame(path='smalldata/airlines/allyears2k_headers.zip')
    myX = ['DayofMonth', 'DayOfWeek']
    air_grid = h2o.gbm(y=air['IsDepDelayed'], x=air[myX], loss='AUTO', ntrees=[5, 10, 15], max_depth=[2, 3, 4], learn_rate=[0.1, 0.2])
    air_grid.show()
