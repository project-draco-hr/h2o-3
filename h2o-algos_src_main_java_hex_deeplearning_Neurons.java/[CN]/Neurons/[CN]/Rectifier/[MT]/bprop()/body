{
  float m=momentum();
  float r=_minfo.adaDelta() ? 0 : rate(_minfo.get_processed_total()) * (1f - m);
  final int rows=_a.size();
  if (_w instanceof DenseRowMatrix) {
    for (int row=0; row < rows; row++) {
      if (_minfo.get_params()._autoencoder && _index == _minfo.get_params()._hidden.length)       _e.set(row,autoEncoderError(row));
      float g=_a.get(row) > 0f ? _e.get(row) : 0f;
      bprop(row,g,r,m);
    }
  }
 else {
    bprop_sparse(r,m);
  }
}
