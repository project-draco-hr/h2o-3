{
  Matrix m=null;
  if (dcm != null)   m=dcm;
  if (drm != null)   m=drm;
  if (scm != null)   m=scm;
  if (srm != null)   m=srm;
  assert(m.rows() == cs[0]._len);
  assert(m.cols() == cs.length);
  for (int c=0; c < cs.length; ++c) {
    for (int r=0; r < m.rows(); ++r) {
      cs[c].set(r,m.get((int)cs[0].start() + r,c));
    }
  }
}
