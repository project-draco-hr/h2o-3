{
  if (a instanceof Storage.DenseRowMatrix && x instanceof Storage.DenseVector)   gemv(res,(Storage.DenseRowMatrix)a,(Storage.DenseVector)x,y,row_bits);
 else   if (a instanceof Storage.DenseColMatrix && x instanceof Storage.SparseVector)   gemv(res,(Storage.DenseColMatrix)a,(Storage.SparseVector)x,y,row_bits);
 else   if (a instanceof Storage.DenseRowMatrix && x instanceof Storage.SparseVector)   gemv(res,(Storage.DenseRowMatrix)a,(Storage.SparseVector)x,y,row_bits);
 else   if (a instanceof Storage.DenseColMatrix && x instanceof Storage.DenseVector)   gemv(res,(Storage.DenseColMatrix)a,(Storage.DenseVector)x,y,row_bits);
 else   throw new UnsupportedOperationException("gemv for matrix " + a.getClass().getSimpleName() + " and vector + "+ x.getClass().getSimpleName()+ " not yet implemented.");
}
