{
switch (algo) {
case "drf":
    params=makeDrfModelParameters();
  break;
case "glm":
params=makeGlmModelParameters();
break;
case "dl":
params=makeDlModelParameters();
break;
case "gbm":
params=makeGbmModelParameters();
break;
default :
Log.info("Cannot set model parameters for algo: " + algo);
System.exit(-1);
}
}
