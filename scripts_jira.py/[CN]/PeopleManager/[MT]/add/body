def add(self, issue):
    assignee = issue[u'fields'][u'assignee']
    if (assignee is None):
        print ('ERROR: assignee is none for issue: ' + str(issue))
        sys.exit(1)
    assignee_name = issue[u'fields'][u'assignee'][u'name']
    person = self.find(assignee_name)
    person.add(issue)
