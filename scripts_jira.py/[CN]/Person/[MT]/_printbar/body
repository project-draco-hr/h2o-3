def _printbar(self, label, value, char):
    bars_per_day = 4
    sys.stdout.write((label + ':  '))
    num_bars = int((value * bars_per_day))
    i = 0
    while (num_bars > 0):
        if ((i % bars_per_day) == 0):
            sys.stdout.write(' ')
        sys.stdout.write(char)
        num_bars -= 1
        i += 1
    sys.stdout.write('\n')
