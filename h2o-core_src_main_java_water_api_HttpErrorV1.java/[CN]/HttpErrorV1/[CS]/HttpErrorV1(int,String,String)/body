{
  errmsg=msg;
  errurl=url;
switch (status_code) {
case 400:
    this.status_header="400 Bad Request";
  break;
case 404:
this.status_header="404 Not Found";
break;
case 500:
this.status_header="500 Internal Server Error";
break;
default :
throw H2O.unimpl("Unimplemented http status code: " + status_code);
}
}
