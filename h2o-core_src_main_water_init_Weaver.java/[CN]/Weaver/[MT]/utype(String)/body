{
switch (sig.charAt(0)) {
case 'Z':
    return "Boolean";
case 'B':
  return "Byte";
case 'C':
return "Char";
case 'S':
return "Short";
case 'I':
return "Int";
case 'F':
return "Float";
case 'J':
return "Long";
case 'D':
return "Double";
case 'L':
return "Object";
case '[':
return "Object";
}
throw new RuntimeException("unsafe access to type " + sig);
}
