{
switch (sig.charAt(0)) {
case 'Z':
case 'B':
    return "Byte";
case 'C':
  return "Char";
case 'S':
return "Short";
case 'I':
return "Int";
case 'F':
return "Float";
case 'J':
return "Long";
case 'D':
throw H2O.unimpl();
case 'L':
throw H2O.unimpl();
case '[':
return "Object";
}
throw new RuntimeException("unsafe access to type " + sig);
}
