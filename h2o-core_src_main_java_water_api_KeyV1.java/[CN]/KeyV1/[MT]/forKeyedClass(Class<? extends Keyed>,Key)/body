{
  if (Job.class.isAssignableFrom(keyed_class))   return KeySchema.make(JobKeyV1.class,key);
 else   if (Frame.class.isAssignableFrom(keyed_class))   return KeySchema.make(FrameKeyV1.class,key);
 else   if (Model.class.isAssignableFrom(keyed_class))   return KeySchema.make(ModelKeyV1.class,key);
 else   if (Vec.class.isAssignableFrom(keyed_class))   return KeySchema.make(VecKeyV1.class,key);
 else   return KeySchema.make(KeyV1.class,key);
}
