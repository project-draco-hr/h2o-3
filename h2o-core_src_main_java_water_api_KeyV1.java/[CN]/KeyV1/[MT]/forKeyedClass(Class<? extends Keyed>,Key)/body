{
  if (Job.class.isAssignableFrom(keyed_class))   return KeyV1.make(JobKeyV1.class,key);
 else   if (Frame.class.isAssignableFrom(keyed_class))   return KeyV1.make(FrameKeyV1.class,key);
 else   if (Model.class.isAssignableFrom(keyed_class))   return KeyV1.make(ModelKeyV1.class,key);
 else   if (Vec.class.isAssignableFrom(keyed_class))   return KeyV1.make(VecKeyV1.class,key);
 else   return KeyV1.make(KeyV1.class,key);
}
