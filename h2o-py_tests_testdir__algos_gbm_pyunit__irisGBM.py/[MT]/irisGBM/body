def irisGBM(ip, port):
    h2o.init(ip, port)
    train = h2o.import_frame(path=h2o.locate('smalldata/iris/iris_wheader.csv'))
    train.describe()
    my_gbm = h2o.gbm(y=train['class'], validation_y=train['class'], x=train[1:4], validation_x=train[1:4], ntrees=50, learn_rate=0.1, distribution='multinomial')
    my_gbm.show()
    my_gbm_metrics = my_gbm.model_performance(train)
    my_gbm_metrics.show()
    my_gbm_metrics
