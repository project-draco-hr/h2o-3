{
switch (s.toLowerCase()) {
case "boolean":
case "smallint":
case "tinyint":
case "bigint":
case "int":
case "float":
case "double":
case "decimal":
case "timestamp":
case "date":
    return Vec.T_NUM;
case "string":
case "varchar":
case "binary":
case "char":
  return Vec.T_STR;
default :
throw new IllegalArgumentException("Unsupported Orc schema type: " + s);
}
}
