{
  assert _isValid;
  ParserSetup ps=guessSetup(bits,_sep,_ncols,_singleQuotes,0,_columnNames);
  if (!ps._isValid)   return ps;
  if (_pType != ps._pType || (_pType == ParserType.CSV && (_sep != ps._sep && _ncols != ps._ncols)))   return new ParserSetup(ps,"Conflicting file layouts, expecting: " + this + " but found "+ ps+ "\n");
  return ps;
}
