{
  Job[] jobs=Job.jobsUserVisible();
  s.jobs=new JobV3[jobs.length];
  int i=0;
  for (  Job j : jobs) {
    try {
      s.jobs[i]=(JobV3)Schema.schema(version,j).fillFromImpl(j);
    }
 catch (    H2ONotFoundArgumentException e) {
      s.jobs[i]=new JobV3().fillFromImpl(j);
    }
    i++;
  }
  return s;
}
