import h2o
h2o.connect(ip='localhost', port=54321)
a = h2o.import_frame(path='smalldata/iris/iris_wheader.csv')[0:4]
print a[0].name()
print a[0][2].show()
print a['sepal_len'][2].show()
print (a[0] + 2).show()
print (a[0] + a[1]).show()
print sum(a).show()
print a['sepal_len'].mean().show()
try:
    print a['Sepal_len']
except ValueError as ex:
    pass
b = h2o.import_frame(path='smalldata/iris/iris_wheader.csv')[0:4]
c = (a + b)
d = ((c + c) + sum(a))
e = ((c + a) + 1)
print e.show()
print c.show()
c = None
print (1 + (a[0] + b[1]).mean())
c = h2o.import_frame(vecs=[h2o.H2OVec('C1', h2o.Expr([1, 2, 3])), h2o.H2OVec('C2', h2o.Expr([4, 5, 6]))])
print c.show()
