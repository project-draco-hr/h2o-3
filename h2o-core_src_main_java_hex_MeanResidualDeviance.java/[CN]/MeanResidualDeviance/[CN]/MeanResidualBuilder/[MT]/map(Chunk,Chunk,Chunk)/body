{
  _mean_residual_deviance=0;
  _wcount=0;
  final int len=Math.min(ca._len,cp._len);
  for (int i=0; i < len; i++) {
    if (ca.isNA(i))     continue;
    if (cp.isNA(i))     continue;
    final double a=ca.atd(i);
    final double pr=cp.atd(i);
    final double w=cw.atd(i);
    perRow(pr,a,w);
  }
}
