{
  e.xpeek('(');
  AST ast=e.parse();
  if (!(ast instanceof ASTExec) && !(ast instanceof ASTId) && !(ast instanceof ASTFun))   e.throwErr("Expected a function but found a " + ast.getClass());
  ArrayList<AST> asts=new ArrayList<>();
  asts.add(0,ast);
  while (e.skipWS() != ')')   asts.add(e.parse());
  e.xpeek(')');
  _asts=asts.toArray(new AST[asts.size()]);
}
