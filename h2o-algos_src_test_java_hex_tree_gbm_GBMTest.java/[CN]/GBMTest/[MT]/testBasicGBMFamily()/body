{
  Scope.enter();
  basicGBM("./smalldata/logreg/prostate.csv",new PrepData(){
    int prep(    Frame fr){
      fr.remove("ID").remove();
      int ci=fr.find("RACE");
      Scope.track(fr.replace(ci,VecUtils.toCategoricalVec(fr.vecs()[ci]))._key);
      return fr.find("CAPSULE");
    }
  }
,false,Distribution.Family.bernoulli);
  Scope.exit();
}
