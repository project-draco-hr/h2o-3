{
  final PrepData titanicPrep=new PrepData(){
    @Override int prep(    Frame fr){
      return fr.find("survived");
    }
  }
;
  ScoreKeeper[] mseWithoutVal=basicGBM("./smalldata/junit/titanic_alt.csv",titanicPrep,false,Family.bernoulli)._scored_train;
  ScoreKeeper[] mseWithVal=basicGBM("./smalldata/junit/titanic_alt.csv",titanicPrep,true,Family.bernoulli)._scored_valid;
  Assert.assertArrayEquals("GBM has to report same list of MSEs for run without/with validation dataset (which is equal to training data)",mseWithoutVal,mseWithVal);
}
