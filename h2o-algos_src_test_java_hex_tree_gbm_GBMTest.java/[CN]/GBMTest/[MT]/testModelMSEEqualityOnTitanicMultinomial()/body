{
  final PrepData titanicPrep=new PrepData(){
    @Override int prep(    Frame fr){
      return fr.find("survived");
    }
  }
;
  ScoredClassifierRegressor[] mseWithoutVal=basicGBM("./smalldata/junit/titanic_alt.csv",titanicPrep,false,Family.multinomial)._scored_train;
  ScoredClassifierRegressor[] scoredWithVal=basicGBM("./smalldata/junit/titanic_alt.csv",titanicPrep,true,Family.multinomial)._scored_valid;
  Assert.assertArrayEquals("GBM has to report same list of MSEs for run without/with validation dataset (which is equal to training data)",mseWithoutVal,scoredWithVal);
}
