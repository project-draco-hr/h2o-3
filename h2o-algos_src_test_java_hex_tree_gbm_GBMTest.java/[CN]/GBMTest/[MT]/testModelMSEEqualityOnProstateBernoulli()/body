{
  final PrepData prostatePrep=new PrepData(){
    @Override int prep(    Frame fr){
      fr.remove("ID").remove();
      return fr.find("CAPSULE");
    }
  }
;
  ScoredClassifierRegressor[] scoredWithoutVal=basicGBM("./smalldata/logreg/prostate.csv",prostatePrep,false,Family.bernoulli)._scored_train;
  ScoredClassifierRegressor[] mseWithVal=basicGBM("./smalldata/logreg/prostate.csv",prostatePrep,true,Family.bernoulli)._scored_valid;
  Assert.assertArrayEquals("GBM has to report same list of MSEs for run without/with validation dataset (which is equal to training data)",scoredWithoutVal,mseWithVal);
}
