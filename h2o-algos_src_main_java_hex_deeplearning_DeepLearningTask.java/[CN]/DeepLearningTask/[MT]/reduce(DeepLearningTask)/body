{
  if (other._output.get_processed_local() > 0 && other._output != _output) {
    if (_output.get_processed_local() == 0) {
      _output=other._output;
      _chunk_node_count=other._chunk_node_count;
    }
 else {
      _output.add(other._output);
      _chunk_node_count+=other._chunk_node_count;
    }
  }
}
