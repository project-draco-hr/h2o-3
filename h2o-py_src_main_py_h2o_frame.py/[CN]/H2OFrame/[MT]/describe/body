def describe(self):
    print 'Rows:', len(self._vecs[0]), 'Cols:', len(self)
    headers = [vec.name() for vec in self._vecs]
    table = [self._row('type', None), self._row('mins', 0), self._row('mean', None), self._row('maxs', 0), self._row('sigma', None), self._row('zeros', None), self._row('missing', None)]
    print tabulate.tabulate(table, headers)
