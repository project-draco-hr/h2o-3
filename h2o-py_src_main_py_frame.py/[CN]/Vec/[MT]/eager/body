def eager(self):
    if isinstance(self._data, RX):
        assert self._data._data
        self._data = self._data._data
        return self._data
    if (not isinstance(self._data, Expr)):
        return self._data
    self._check_subtree_no_rx()
    self._rapids()
    self._data = self._data.do_it()
    return self._data
