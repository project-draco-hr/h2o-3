def __getitem__(self, i):
    if isinstance(i, int):
        return self._vecs[i]
    if isinstance(i, str):
        for v in self._vecs:
            if (i == v._name):
                return v
        raise ValueError((('Name ' + i) + ' not in Frame'))
    if isinstance(i, slice):
        return Frame(vecs=self._vecs[i])
    raise NotImplementedError
