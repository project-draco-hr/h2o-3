def __init__(self, file):
    with open(file, 'rb') as csvfile:
        __reader__ = csv.reader(csvfile)
        self.vecs = None
        for row in __reader__:
            if (self.vecs == None):
                self.vecs = []
                for name in row:
                    self.vecs.append(Vec(name, []))
            else:
                for (i, data) in enumerate(row):
                    self.vecs[i].append(data)
