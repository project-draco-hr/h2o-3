def compute(self):
    print 'WORK: ', self
    if (self._op == '+'):
        if isinstance(self._rite, Vec):
            return [(x + y) for (x, y) in zip(self._left.eager(), self._rite.eager())]
        if isinstance(self._rite, int):
            return [(x + self._rite) for x in self._left.eager()]
        raise NotImplementedError
    raise NotImplementedError
