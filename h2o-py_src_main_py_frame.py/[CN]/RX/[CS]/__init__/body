def __init__(self, name, x):
    assert isinstance(name, str)
    self._name = name
    if isinstance(x, Expr):
        self._op = x._op
        self._left = deVec(x._left)
        assert isinstance(self._left, (int, float, list, RX))
        self._rite = deVec(x._rite)
        assert (isinstance(self._rite, (int, float, list, RX)) or (not self._rite))
        self._data = None
    else:
        assert isinstance(x, list)
        self._data = x
