{
  _server=new Server();
  QueuedThreadPool p=new QueuedThreadPool();
  p.setName("jetty-h2o");
  p.setMinThreads(3);
  p.setMaxThreads(50);
  p.setMaxIdleTimeMs(3000);
  _server.setThreadPool(p);
  Connector connector=new SocketConnector();
  if (_ip != null) {
    connector.setHost(_ip);
  }
  connector.setPort(_port);
  createServer(connector);
}
