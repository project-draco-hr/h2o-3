{
  super.score0(data,preds,weight,offset);
  int N=_output._ntrees;
  if (_output.nclasses() == 1) {
    if (N >= 1)     preds[0]/=N;
  }
 else {
    if (_output.nclasses() == 2 && binomialOpt()) {
      if (N >= 1) {
        preds[1]/=N;
      }
      preds[2]=1. - preds[1];
    }
 else {
      double sum=MathUtils.sum(preds);
      if (sum > 0)       MathUtils.div(preds,sum);
    }
  }
  return preds;
}
