{
switch (_family) {
case gaussian:
    return (yr - ym) * (yr - ym);
case binomial:
  return 2 * ((y_log_y(yr,ym)) + y_log_y(1 - yr,1 - ym));
case poisson:
if (yr == 0) return 2 * ym;
return 2 * ((yr * Math.log(yr / ym)) - (yr - ym));
case gamma:
if (yr == 0) return -2;
return -2 * (Math.log(yr / ym) - (yr - ym) / ym);
default :
throw new RuntimeException("unknown family " + _family);
}
}
