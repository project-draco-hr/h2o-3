{
switch (_link) {
case logit:
    double div=(x * (1 - x));
  if (div < 1e-6)   return 1e6;
return 1.0 / div;
case identity:
return 1;
case log:
return 1.0 / x;
case inverse:
return -1.0 / (x * x);
case tweedie:
return _tweedie_link_power == 0 ? Math.max(2e-16,Math.exp(x)) : 1.0 / _tweedie_link_power * Math.pow(x,1.0 / _tweedie_link_power - 1.0);
default :
throw H2O.unimpl();
}
}
