{
  if (coefficients.length != predictors.length + 1)   throw new IllegalArgumentException("coefficients length is expected to be predictros.length + 1, as each coefficient must have name + intercept term with no name.");
  GLMParameters parms=new GLMParameters(Family.binomial);
  parms._alpha=new double[]{0};
  parms._lambda=new double[]{0};
  parms._standardize=false;
  parms._prior=-1;
  parms._train=null;
  GLMModel m=fam == Family.multinomial ? new GLMModel(Key.make(),parms,null,0,Double.NaN,Double.NaN,-1,false,false) : new GLMModel(Key.make(),parms,null,fam == Family.binomial ? .5 : 0,Double.NaN,Double.NaN,-1,false,false);
  predictors=ArrayUtils.append(predictors,new String[]{response});
  m._output._names=predictors;
  m._output._coefficient_names=predictors;
  m._output._dinfo=DataInfo.makeEmpty(coefficients.length - 1);
  m._output._domains=new String[predictors.length][];
  m.setSubmodel(new Submodel(0,coefficients,-1,Double.NaN,Double.NaN));
  m._output.setSubmodelIdx(0);
  return m;
}
