{
  Key ikey=null;
  String scheme=uri.getScheme();
  if ("hdfs".equals(scheme)) {
    ikey=I[Value.HDFS].uriToKey(uri);
  }
 else   if ("s3n".equals(scheme)) {
    ikey=I[Value.HDFS].uriToKey(uri);
  }
 else   if ("files".equals(scheme) || scheme == null) {
    ikey=I[Value.NFS].uriToKey(uri);
  }
  return ikey;
}
