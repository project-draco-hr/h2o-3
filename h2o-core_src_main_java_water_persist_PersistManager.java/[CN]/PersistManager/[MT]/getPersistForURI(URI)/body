{
  String scheme=uri.getScheme();
  if (scheme != null) {
switch (scheme) {
case Schemes.FILE:
      return I[Value.ICE];
case Schemes.HDFS:
case Schemes.S3N:
case Schemes.S3A:
    return I[Value.HDFS];
case Schemes.S3:
  return I[Value.S3];
default :
throw new IllegalArgumentException("Cannot find persist manager for scheme " + scheme);
}
}
 else {
return I[Value.ICE];
}
}
