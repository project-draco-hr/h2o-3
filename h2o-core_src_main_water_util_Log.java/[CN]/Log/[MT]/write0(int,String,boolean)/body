{
  StringBuilder sb=new StringBuilder();
  String hdr=header(lvl);
  write0(sb,hdr,s);
  if (stdout)   System.out.println(sb);
  org.apache.log4j.Logger l4j=_logger != null ? _logger : createLog4j();
switch (lvl) {
case ERRR:
    l4j.error(s);
  break;
case WARN:
l4j.warn(s);
break;
case INFO:
l4j.info(s);
break;
case DEBUG:
l4j.debug(s);
break;
}
}
