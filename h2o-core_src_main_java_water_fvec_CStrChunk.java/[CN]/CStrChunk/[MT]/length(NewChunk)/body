{
  nc._ls=MemoryManager.malloc8(_len);
  nc._xs=MemoryManager.malloc4(_len);
  for (int i=0; i < _len; i++) {
    int off=UnsafeUtils.get4(_mem,(i << 2) + _OFF);
    int len=0;
    if (off != NA) {
      while (_mem[_valstart + off + len] != 0)       len++;
      nc.addNum(len,0);
    }
 else     nc.addNA();
  }
  return nc;
}
