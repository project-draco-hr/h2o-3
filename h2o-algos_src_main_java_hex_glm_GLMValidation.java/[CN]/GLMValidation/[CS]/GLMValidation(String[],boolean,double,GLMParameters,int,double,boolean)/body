{
  super(domain);
  _intercept=intercept;
  _rank=rank;
  _parms=parms;
  _threshold=threshold;
  _computeMetrics=computeMetrics;
  _ymu=parms._intercept ? ymu : parms._family == Family.binomial ? .5 : 0;
  _ymuLink=_parms.link(ymu);
  if (_computeMetrics)   _metricBuilder=_parms._family == Family.binomial ? new MetricBuilderBinomial(domain) : new MetricBuilderRegression();
}
