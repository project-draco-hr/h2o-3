def __init__(self, model, hyper_params, grid_id=None):
    self._id = grid_id
    self.model = (model() if (model.__class__.__name__ == 'type') else model)
    self.hyper_params = hyper_params
    self._grid_json = None
    self.models = None
    self._parms = {}
    self.parms = {}
    self._estimator_type = None
    self._future = False
    self._job = None
