def sorted_metric_table(self):
    '\n    Retrieve Summary Table of an H2O Grid Search\n\n    Returns\n    -------\n      The summary table as an H2OTwoDimTable or a Pandas DataFrame.\n    '
    summary = self._grid_json['summary_table']
    if (summary is not None):
        return summary.as_data_frame()
    print('No sorted metric table for this grid search')
