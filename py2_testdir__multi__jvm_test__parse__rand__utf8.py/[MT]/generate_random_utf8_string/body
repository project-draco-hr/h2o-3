def generate_random_utf8_string(length=1, multi=False, row=0, col=0):
    cList = []
    for i in range(length):
        r = random.choice((ordinalChoicesMulti if multi else ordinalChoices))
        if ((row == 1) and (col == 1) and (i == 0)):
            while ((r == 64) or (r == 35)):
                r = random.choice((ordinalChoicesMulti if multi else ordinalChoices))
        cList.append(unichr(r))
    return ''.join(cList)
